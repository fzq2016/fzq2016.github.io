    /**
    * Delete an item via AJAX and remove from the list on the UI.
    * <p>
    * If the ajax response returns JSON with { "removeRow" : true/false } then you can control if the row is updated or deleted.
    * <p>
    * The default behavior in absence of this JSON response is to delete the row.
    */
    this.performDelete = function(itemID) {
        LCAjaxLogger.log(LCAjaxLogger.LIST, ">>> Delete Item ID: " + itemID);
        // self.showSpinner();
        self.showRowSpinner(itemID);
        
        /* create the request URL ... the URL is dynamically generated by WebObjects, so we get it from a hidden HREF 'ajaxDeleteAchievementURL' */
        var requestURL = getAjaxActionURL(deleteUrl) + "?itemID=" + itemID;
        LCAjaxLogger.log(LCAjaxLogger.LIST, "Request URL: " + requestURL);
        
        /* send it to the server */
        new Ajax.Request(requestURL, {
              onSuccess: function(response) {
                   // if it worked we have nothing to do
                    LCAjaxLogger.log(LCAjaxLogger.LIST, "Successfully deleted item on server.");
                    var removeRow = true;
                    if(response.responseText) {
                        var jsonResponse = response.responseJSON;
                        if(!jsonResponse.removeRow) {
                            removeRow = false;
                        }
                    }
                    
                    if(removeRow) {
                        Effect.Fade(ajaxListId + "Row_" + itemID, {duration: 0.5, afterFinish: function() {
                            // delete the row from the DOM so numbers update correctly
                            $(ajaxListId + "Row_" + itemID).remove();
                            LCAjaxLogger.trace("Removed row from list.");
                            
                            /* trigger change event */
                            self.onAjaxListChange();
                            
                            /* update the index numbers for display */
                            self.updateAjaxListSortOrderColumn();
                            
                            // update the header text
                            // self.refreshHeaderText();
                            
                            /*  fire event to notify this has happened */
                            self.fireEvent("rowDeleted");
                            
                            /* we don't check for ownership here as we are definitely done doing all work at this point */
                            // self.hideSpinner();
                            self.hideRowSpinner(itemID);
                        }});
                    } else {
                        // we'll update instead
                        self.updateRow(itemID, function() {
                            /*  fire event to notify this has happened */
                            self.fireEvent("rowDeleted");
                            
                            /* we don't check for ownership here as we are definitely done doing all work at this point */
                            self.hideSpinner();   
                            self.hideRowSpinner(itemID);
                        });
                    }
              },
              onFailure: function(){
                 self.displayAjaxListErrorMessage("Failed to delete item. Please try again."); 
                 self.hideRowSpinner(itemID);
              },
              onException: function(request, exception) { 
                 LCAjaxLogger.log(LCAjaxLogger.LIST, "Exception while deleting item => " + exception);
                 self.displayAjaxListErrorMessage("Failed to delete item. Please try again.");
                 self.hideRowSpinner(itemID);
              }
        });
        
    }

    this.isSearchOnServer = function() {
