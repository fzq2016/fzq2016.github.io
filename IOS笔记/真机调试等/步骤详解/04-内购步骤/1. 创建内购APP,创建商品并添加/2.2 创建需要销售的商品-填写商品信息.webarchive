	            linkElt.appendChild(document.createTextNode(msg));
	            divLink.appendChild(linkElt);
	        } else if (needLink) {
	            var linkElt = new Element("a");
	            linkElt.setAttribute("href", "javascript:showStatusBarForProductAndCountry(\""+this.productId+"\", \"" + this.sType +"\", \""+ statusType+"\", \""+ country +"\")");
	            linkElt.appendChild(document.createTextNode(msg));
	            divLink.appendChild(linkElt);
	        } else {            
	            divLink.appendChild(document.createTextNode(msg));
	        }
        } else {
            divLink.setAttribute("class", "see-details");
