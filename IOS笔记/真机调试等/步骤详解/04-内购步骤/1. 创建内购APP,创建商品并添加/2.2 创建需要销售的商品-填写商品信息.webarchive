var LCContentStatusAjaxItem = Class.create({
    initialize: function(productId, productName, sType, pType, ajaxUrl) {
        this.productId = productId;
        this.productName = productName;
        this.standardizeSType = sType.toLowerCase().replace(/ /g, "-");
        this.divId = "lc-status-box-" + productId;
        this.ajaxUrl = ajaxUrl;
        this.sType = sType;
        this.pType = pType;
        // Internal information
        this.contentRaw = null;
        this.countriesURL = null;
        this.isFetching = false;
    },
    startFetching: function() {
        this.isFetching = true;
    },
    doneFetching: function() {
        this.isFetching = false;
    },
    registerCountryURL: function(countryCode, url) {
        if (!this.countriesURL) { this.countriesURL = {}; }
        this.countriesURL[countryCode] = url;
    },
    registerReasonForStatusTypeAndCountry: function(statusType, country, countryCode, countryCode3, reasons) {
        // Check if we have already the status
        var statuses = this.statusFor(statusType);
        if (!statuses) {
            statuses = [];
            if (!this.contentRaw) { this.contentRaw = {}; }
            this.contentRaw[statusType] = statuses;
        }
        var entry = { "country": country,  "countryCode": countryCode, "countryCode3": countryCode3, "reasons": reasons };
        // Add an entry for the new country
        statuses[statuses.length] = entry;
    },
    getContentForStatusTypeAndCountryCode: function(statusType, countryCode) {
        var statuses = this.statusFor(statusType);
        if (!statuses) { return null; }
        for(i=0;i<statuses.length;i++) {
            var countryStatus = statuses[i];
            if (countryStatus.countryCode == countryCode) {
                return countryStatus;
            }
        }
        return null;
    },
    getReasonMessageTitle: function(statusType, countryCode, sType) {
        var statusForCountry = this.getContentForStatusTypeAndCountryCode(statusType, countryCode);
        if (!statusForCountry) { return "Error retrieving details."; }
        var prefix = '';
        //For Apple Music/Subscription service
        if(sType == 'Subscription') {
            prefix = statusType == 'notOnStore' ? "Not on Apple Music" : "Ready for Apple Music";
        }
        else {
            prefix = statusType == 'notOnStore' ? "Not on Buy Store" : "Ready for Buy Store";
        }
        var reasons = statusForCountry.reasons;
        var suffix = reasons.length > 1 ? " Reasons" : " Reason";
        var msg = prefix + ": " + statusForCountry.reasons.length + suffix;
        return msg; 
    },
    getFlagCodeForStatusTypeAndCountryCode: function(statusType, countryCode) {
        var statusForCountry = this.getContentForStatusTypeAndCountryCode(statusType, countryCode);
        if (!statusForCountry) { return null; }
        return "/images/flags/30/" + statusForCountry.countryCode3.toLowerCase() + ".png";
    },
    getCountryCodeForStatusTypeAndCountryCode: function(statusType, countryCode) {
        var statusForCountry = this.getContentForStatusTypeAndCountryCode(statusType, countryCode);
        if (!statusForCountry) { return null; }
        return statusForCountry.countryCode;
    },
    getBestMessageFrom: function(messages) {
        if (!messages || messages.length == 0) { return null; }
        if (messages.length == 1) { return messages[0]; }
        // Find the message with the lowest priority
        var bestMessage = null;
        messages.each(function(oneMessage) {
            if (bestMessage == null || oneMessage.priority < bestMessage.priority) {
                bestMessage = oneMessage;
            }
        });
        return bestMessage;
    },
    getBestPMPriorityFromMessages: function(messages) {
        var bestPriority = 1000000;
        // Find the best priority
        messages.each(function(oneMessage) {
            // Lookup PM priority
            if (oneMessage.partnerMessageGroup && oneMessage.partnerMessageGroup.priority) {
                if (oneMessage.partnerMessageGroup.priority < bestPriority) {
                    bestPriority = oneMessage.partnerMessageGroup.priority;
                }
            } else if (oneMessage.priority && oneMessage.priority < bestPriority) {
                bestPriority = oneMessage.priority;
            }
        });
        return bestPriority;
    },
    orderedReasonsByPriority: function(reason1, reason2) {
        try {
            var pmPriority1 = reason1.value && reason1.value.message && reason1.value.message.length > 0 ? this.getBestPMPriorityFromMessages(reason1.value.message) : -1;
            var pmPriority2 = reason2.value && reason2.value.message && reason2.value.message.length > 0 ? this.getBestPMPriorityFromMessages(reason2.value.message) : -1;
            return pmPriority1 - pmPriority2;
        } catch (e) {
            LCAjaxLogger.log(LCAjaxLogger.CONTENT_STATUS, "Exception orderedReasonsByPriority :" + e);
            return 0;
        }
    },
    orderedReasons: function(existingReasons) {
        var self = this;
        return existingReasons.sort(function(a,b) { return self.orderedReasonsByPriority(a,b)} );
    },
    getProblemsForStatusTypeAndCountryCode: function(statusType, countryCode) {
        // This method should return all the LCContentStatusProblem for the corresponding statusType, countryCode
        var statusForCountry = this.getContentForStatusTypeAndCountryCode(statusType, countryCode);
        if (!statusForCountry) { return null; }
        var self = this;
        var reasons = this.orderedReasons(statusForCountry.reasons);
        var problems = [];
        var unknownProblems = [];
        var defaultUnknownDesc = "Unknown Issue. Due to an unknown issue, your content is not on the store.";
        var defaultUnknownAction = " Please <a href=\"/WebObjects/iTunesConnect.woa/wa/jumpTo?page=contactUs\">Contact us</a> for more information.";
        var hasUnknownMessage = false;
        var isUnknownMessage = false;
        reasons.each(function(oneReason) {
            isUnknownMessage = false;
            var reasonDesc, reasonAction, reasonInternalName;
            reasonInternalName = oneReason.name;
            if (oneReason.value && oneReason.value.message) {
                var messages = oneReason.value.message;
                var theBestMessage = self.getBestMessageFrom(messages);
                if (!theBestMessage) {
                    reasonDesc = defaultUnknownDesc;
                    reasonAction = defaultUnknownAction;
                    isUnknownMessage = true;
                } else {
                    reasonDesc = theBestMessage.subject;
                    reasonAction = theBestMessage.message;
                }
            } else {
                reasonDesc = defaultUnknownDesc;
                reasonAction = defaultUnknownAction;
                isUnknownMessage = true;
            }
            var newContentStatusProblem = new LCContentStatusProblem("/itc/images/red-alert-icon.png", reasonDesc, reasonAction, reasonInternalName);
            if (isUnknownMessage) {
                unknownProblems[unknownProblems.length] = newContentStatusProblem;
            } else {
                problems[problems.length] = newContentStatusProblem;
            }
        });
        // We should now move any unknown message at the end
        if (unknownProblems.length > 0) {
            problems = problems.concat(unknownProblems); // concat the known problem with all the unknown
        }
        return problems;
    },
    getCountryNameForStatusTypeAndCountryCode: function(statusType, countryCode) {
        var statusForCountry = this.getContentForStatusTypeAndCountryCode(statusType, countryCode);
        if (!statusForCountry) { return "Error retrieving details."; }
        return statusForCountry.country;
    },
    hasContent: function() {
        return this.contentRaw != null;
    },
    statusFor: function(statusType) {
        if (!this.hasContent() || this.isFetching) { return null; }
        return this.contentRaw[statusType];
    },
    setStatusInformationFor: function(statusType, statusInformation) {
        if (!this.hasContent()) { this.contentRaw = {}; }
        var countries = this.contentRaw[statusType];
        if (!countries) {
            countries = [];
            this.contentRaw[statusType] = countries;
        }
		// Check if we are not updating an existing countries
		var shouldAdd = true;
		countries.each(function(oneCountry) {
			if (oneCountry.countryCode == statusInformation.countryCode) {
				shouldAdd = false;
				oneCountry.reasons = statusInformation.reasons;
			}
		});
		if (shouldAdd) {
        	countries[countries.length] = statusInformation;
		}
    },
    refreshCountryStatus: function(statusType, country, countryCode, operationStatus) {
        var spinnerCountry = $("lc-status-country-spinner-" + this.productId + "-" + this.standardizeSType + "-" + countryCode);
        spinnerCountry.hide();
