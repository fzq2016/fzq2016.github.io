/* 
 * Create and display a new lightbox.
 *
 * Returns the instance of LCAjaxLightbox that was created and being displayed
 *
 * Arguments:
 * - lightboxDivId: The ID of the Lightbox to display (from the LCAjaxLightbox WOD binding)
 * - extraArgumentsForURL: Optional and rarely used, but can be used to pass parameters to the server.
 * - keepSaveButtonEnabled: defaults to false, set to true if you want the "save button disabled until modified" logic disabled
 * - messageToDisplayAfterLoad: A string to display as an error message at the top of the panel once it loads (same as if an error is returned from a save event)
 */
function displayLCAjaxLightbox(lightboxDivId, extraArgumentsForURL, keepSaveButtonEnabled, messageToDisplayAfterLoad, followupFunction) {
    var lb = new LCAjaxLightbox(lightboxDivId, extraArgumentsForURL);

    var my_fn = function(){
      document.fire('LCAjaxLightbox:open');
    };
    if(followupFunction === undefined){
      my_fn();
    }else{
      var backup_ff = followupFunction;
      followupFunction = function(){
        my_fn();
        backup_ff;
      };
    }
    
    if(keepSaveButtonEnabled == true) {
        lb.keepSaveButtonEnabled();
    }
    
    /*  now display */
    if(messageToDisplayAfterLoad) {
        lb.display(function() {
            lb._displayMessage(messageToDisplayAfterLoad);
            if(followupFunction) {
                followupFunction.call();
            }
        });
    } else {
        lb.display(function() {
            if(followupFunction) {
                followupFunction.call();
            }            
        });
    }
    return lb;
}


/*
 * Create and display a new lightbox that loads its contents from the given URL.
 *
 * The lightbox created via this method will NOT be capable of saving anything via the LCAjaxLightboxPanel. 
 *
 * This is intended for read-only display. 
 */
function displayLCAjaxLightboxViaURL(lightboxDivId, urlToDisplay) {
    var lb = new LCAjaxLightbox(lightboxDivId);
    /*  now display */
    lb.displayReadOnlyWithUrl(urlToDisplay);
    return lb;
}

/* 
 * Object representing an LCAjaxLightbox.
 *
 * Each lightbox should have an instance of this object.
 * 
 * Normally this object will not be instantiated directly by a customer page. Instead they would use the 'displayLCAjaxLightbox' helper method.
 *
 * Typical usage will be for a page to invoke 'displayLCAjaxLightbox' like this:
 *
 *    displayLCAjaxLightbox('myLightbox');
 *
 * After that, this object will take care of the rest with it's event handlers so normally no other methods on this object would be invoked
 * by the customer page.
 *
 * Instead, custom events are fired off that can be observed for hooking into the flow and adding custom behavior.
 *
 * These events are:
 *  - LCAjaxLightbox:cancel
 *  - LCAjaxLightbox:save
 *  - LCAjaxLightbox:open
 *
 * An event will return three data values that can be used by the event handler:
 *  - lightboxDivId => The ID of the Div for the lightbox.
 *  - eventToken => This is a custom value that can be set via the 'setEventToken' method. 
 *  - id => The primary key returned from the server for the lightbox panel. See the 'fireEvent' method for more details.
 *      The purpose of this 'id' is so that an event handler knows the primary key of the data and can refer to it, such as an itemID
 *      that needs to be looked up in a list (see LCAjaxList) or an object loaded from the server to display in an LCAjaxContainer.
 *
 * These values can be retrieved via the "event.memo.*" property paths, such as "event.memo.lightBoxDivId".
 *
 * An example listener is:
 *      document.observe("LCAjaxLightbox:save", function(event) {
 *          console.log("LightboxDiv: " + event.memo.lightboxDivId);
 *          console.log("Lightbox Panel Data Primary Key: " + event.memo.id);
 *      }
 *
 *
 * The 'extraArgumentsForURL' argument can be used to pass extra URL arguments that are to be sent to the server.
 *
 *     For example: ?itemId=45
 *
 * That could be used to pass in the ID of an item that is to be loaded from the server and displayed.
 */
function LCAjaxLightbox(lightboxDivId, extraArgumentsForURL){
    /* define an internal variable for 'this' that can't get over-written by other call-paths such as event handlers */
    var self = this;
    
    /* register with window so this instance can be looked up */
    window[lightboxDivId] = self;
    
    /* used to compare original and modified state to see if things have changed */
    self.ignoreStateChanges = false;
    
    /* switch for external javascript to control if 'save' should be enabled/disabled ... controlled via events: LCAjaxLightbox:enableSave & LCAjaxLightbox:disableSave */
    self.externalSaveEnabled = true;
    
    /* the action URL for the lightbox */
    var actionUrl = $(lightboxDivId).getAttribute('action');
    if(actionUrl == null) {
        LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "ERROR: Could Not Find Lightbox Action URL. Will not open.");
    }
    
    /* 
     * This will load the lightbox html panel via an ajax call and display it.
     */
    this.display = function(executeAfterSuccess) {
        // use the action URL for the contents of the panel
        self._loadContentPanel(actionUrl, executeAfterSuccess);
    }
    
    /* 
     * This will load the lightbox html panel via an ajax call to the given URL and display it as a read-only window without the ability to save.
     */
    this.displayReadOnlyWithUrl = function(contentUrl, executeAfterSuccess) {
        self._loadContentPanel(contentUrl, executeAfterSuccess);
    }
    
    /* 
     * This will load the lightbox html panel via an ajax call and display it.
     * 
     * This should ONLY be called internally and not called outside of this object.
     *
     * Use 'display' to open the lightbox externally.
     *
     * 'executeAfterSuccess' can be a function passed in to execute once the load is completed (if it completes successfully).
     */
    this._loadContentPanel = function(urlForContent, executeAfterSuccess) {
        LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "Requesting lightbox panel for ID: " + lightboxDivId + "  with URL: " + urlForContent);
        
        if(urlForContent == null) {
            LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "ERROR: URL for Content is NULL. Will not open.");
            return;
        } 
        
        if(extraArgumentsForURL != null) {
            if(!extraArgumentsForURL.startsWith("?")) {
                urlForContent = urlForContent + '?';
            }
            urlForContent = urlForContent + extraArgumentsForURL + "&open=true";
        } else {
            urlForContent = urlForContent +'?open=true'
        }
        
        /* create the URL */
        var lightboxActionURL = getAjaxActionURL(urlForContent); 
        
        LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "lighbox action url: " + lightboxActionURL);
        
        /* send it to the server */
        new Ajax.Request(lightboxActionURL, {
              onSuccess: function(response) {
            	  //LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "lightbox response is: " + response.responseText);
              
                    $(lightboxDivId).innerHTML=response.responseText;
                    
                    // register all the listeners now that the panel is loaded and DOM exists
                    self.registerListeners();
                    
                    /* process any javascript in the returned HTML */
                    $(lightboxDivId).innerHTML.evalScripts();

                    if(!self.ignoreStateChanges) {
                        // by default disable save until something changes
                        self.disableSaveButton();
                    }
                    
                    // now position and display the UI
                   self.resizeToContents();
                                 
                    // capture the current state of the contents so we know if something has changed
                    self.originalStateOfParameters = serializedFormValuesForContainer(self.getContentsContainer());
                                        
                    /* show the panel if it's not already showing */
                    
                    if(!$(lightboxDivId + '-overlayScroll').visible()) {
                        Effect.Appear($(lightboxDivId + '-overlayScroll'), { duration: 0.3 });
                    }
                    /*if(!$(lightboxDivId + '-white-overlay').visible()) {
                        Effect.Appear($(lightboxDivId + '-white-overlay'), { duration: 0.3, to: 0.3 });
                    }*/
                    if(!$(lightboxDivId).visible()) {
                        Effect.Appear($(lightboxDivId), { duration: 0.3 });
                    }

                    
                    // execute the 'executeAfterSuccess' function if applicable
                    if(executeAfterSuccess) {
                        executeAfterSuccess.call();
                    }

              },
              onFailure: function(){
            	LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "Exception while retrieving panel => " + exception);
                displayLCAjaxErrorMessage(getBestFitLCAjaxErrorElementId($(lightboxDivId)), "Failed to retrieve panel. Please try again.");
                self.cancel(); 
              },
              onException: function(request, exception) {
                LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "Exception while retrieving panel => " + exception);
                displayLCAjaxErrorMessage(getBestFitLCAjaxErrorElementId($(lightboxDivId)), "Failed to retrieve panel. Please try again.");
                self.cancel();
              }
        });
    }
    
    this.makeScrollPane = function() {
        $j('body').addClass('noscroll');
    }

    this.resetScrollPane = function() {
        $j('body').removeClass('noscroll');
    }

    this.resizeToContents = function() {
    	var contentsDiv = $(lightboxDivId).select('div.lcAjaxLightboxContents').first();
        /* set the max height to viewport height minus space for header and footer of lightbox */
        var contentHeight = $(lightboxDivId).getDimensions().height;
        var viewportHeight = getViewportHeight();

        /*  we want to keep space both top and bottom, so the maxHeight must be viewport-250px or smaller */
        var possibleHeight = viewportHeight - 250;
        
        //LCAjaxLogger.trace("contentHeight: " + contentHeight + "   possibleHeight: " + possibleHeight + "  viewport height: " + viewportHeight);
        
        // set the maxHeight to the possibleHeight                        
        //contentsDiv.style.maxHeight=possibleHeight+"px";
        
        var scrollOffset = document.viewport.getScrollOffsets().top;
        
        // we are doing absolute positioning instead of fixed, so we need to calculate where we are on the page to show it
        $(lightboxDivId).style.top = (scrollOffset + 75)+'px';
                            
        // prevent scrolling
       self.preventScrolling(scrollOffset);
       self.makeScrollPane();
    }
    
    // register to the various events we observe
    this.registerListeners = function() {
        /* button click listeners */
        $(lightboxDivId).observe('click', self.eventHandler);
        /* esc key listener */
        document.observe('keydown', self.escKeyHandler);
        
        /* listen for save enable/disable events */
        $(lightboxDivId).observe('LCAjaxLightbox:enableSave', self.externalSaveEnableHandler);
        $(lightboxDivId).observe('LCAjaxLightbox:disableSave', self.externalSaveDisableHandler);
        /* listen for messaging events */
        $(lightboxDivId).observe('LCAjaxLightbox:displayMessage', self.messageDisplayHandler);

        /* input change listeners */    
        $(lightboxDivId).observe('keyup', self.inputElementChangeHandler);
        if(BrowserDetect.browser == "Explorer" && BrowserDetect.version < 9) {
            //IE doesn't trigger change events when listening to the parent, only when listening to each objectf
            $(lightboxDivId).select('input').each(function(inputElement) {
              inputElement.observe('change', self.inputElementChangeHandler);
           });
        } else {
            $(lightboxDivId).observe('change', self.inputElementChangeHandler);
        }
    }

    this.returnLightboxDivId = function() {
        return lightboxDivId;
    }
    
    /* If a lightbox needs to ignore the state change logic for enabling/disabling the save button, this can be called immediately after creation */
    this.keepSaveButtonEnabled = function() {
        self.ignoreStateChanges = true;
        self.enableSaveButton();
    }
    
    /* If a lightbox wants to track the state change logic for enabling/disabling the save button */
    this.useStateChangesToControlSaveButton = function() {
    	self.ignoreStateChanges = false;
        self.disableSaveButton();
    }
    
    /* 
     * Internal event handler that will observe user actions on the save and cancel buttons and respond accordingly.
     */
    this.eventHandler = function(event) {
        /* observe onclick events on the lightbox DIV */
          var element = event.element();
          if(element.hasClassName('lightboxSaveButton')) {
            self.save();
          }
          if(element.hasClassName('lightboxCancelButton')) {
            self.cancel();
          }
    }
    
    /* 
     * Listen for keypresses and respond accordingly
     */
    this.escKeyHandler = function(e) {
    	if (e) {
            if(e.keyCode == Event.KEY_ESC){
                // if the key pressed is the escape key, cancel the lightbox
                self.cancel();
            }
            else if (e.keyCode == Event.KEY_RETURN) {
            	var element = e.element();
            	if (element.tagName && element.tagName.toLowerCase() === 'input') {
            		var saveButton = $j('img.lightboxSaveButtonEnabled');
            		if (saveButton.length > 0 && saveButton.is(":visible")) {
	            		// return key pressed inside an input element, save the lightbox
	            		self.save();
            		}
            	}
            }
    	}
    }

    /* handler for change events to monitor for things such as whether the save button should be enabled/disabled */
    this.inputElementChangeHandler = function(event) {
        /* if the external save switch says we should be disabled, it doesn't matter what we do below, it should be dislabed, so shortcut that here */
        if(self.externalSaveEnabled == false) {
            LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "ChangeHandler => External Save Disabled so will disable save button.");
            self.disableSaveButton();
        }
        else if (self.ignoreStateChanges == true) { // Added as part of <rdar://problem/13818897>
            LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "ChangeHandler => ignoreStateChanges is set to true, not going to do anything for the change handler");
        } 
        else {
            if(self.ignoreStateChanges == false && self.originalStateOfParameters == serializedFormValuesForContainer(self.getContentsContainer())) {
                LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "ChangeHandler => No state change so disabling save button.");
                self.disableSaveButton();
            } else {
                LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "ChangeHandler => State has changed so enabling save button.");
                self.enableSaveButton();
            }
        }
        
    }
    
    /* an LCAjaxLightbox:enableSave event */
    this.externalSaveEnableHandler = function(event) {
        LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "External Save Enable event received.");
        self.externalSaveEnabled = true;
        /* trigger the change handler as if an input value changed so the save button can be recalculated */
        self.inputElementChangeHandler();
    }
    
    /* an LCAjaxLightbox:disableSave event */
    this.externalSaveDisableHandler = function(event) {
        LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "External Save Disable event received.");
        self.externalSaveEnabled = false;
        /* trigger the change handler as if an input value changed so the save button can be recalculated */
        self.inputElementChangeHandler();
    }
    
    /* an LCAjaxLightbox:displayMessage event */
    this.messageDisplayHandler = function(event) {
        LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "External Display Message event received.");

        var messageType = event.memo.type;
        var message = event.memo.message;
        
        self._displayMessage(message, messageType);
    }
    
    /* Can be set by the instantiator of the object and will be sent with custom events. Can be used for tracking and knowing what instance this is. */
    this.setEventToken = function(token) {
        self.eventToken = token;
    }

    this.getContentsContainer = function() {
        var panelContentsDiv = $(lightboxDivId).select("div.lcAjaxLightboxContents").first();
        return $(panelContentsDiv);
    }

    this.preventScrolling = function(offset) {
        if(pageHasScrollBar()) {
            if(BrowserDetect.browser == "Firefox") {
                var offsetToStayAt = document.viewport.getScrollOffsets().top;
                self.scrollListener = function(event) {
                   window.scrollTo(0, offsetToStayAt);
                }
                document.observe('scroll', self.scrollListener);
            } else {
                /*  compensate for hiding the scrollbar so that we don't 'bounce' the page back and forth */
                var scrollerWidth = getScrollerWidth();
                if(scrollerWidth == 0) {
                    scrollerWidth = 17; // default on IE
                }
                // generally this should be 15 on Safari and Firefox
                document.documentElement.style.paddingRight = 0 + "px";
                document.documentElement.style.overflow = "hidden";
           }
        }
    }
    
    /* used by firefox preventScrolling implementation so that we can stopObserving this */
    this.scrollListener = 'placeholder';
    
    this.allowScrolling = function() {
        if(pageHasScrollBar()) {
            if(BrowserDetect.browser == "Firefox") {
                document.stopObserving('scroll', self.scrollListener);
            } else { 
                /*  compensate for hiding the scrollbar so that we don't 'bounce' the page back and forth */
                document.documentElement.style.paddingRight = "";
                document.documentElement.style.overflow = "auto";
            }
        }
    }
    
    /*
     * This will close the lightbox without saving.
     *
     * A 'cancelBeforeWindowClose' event will be fired before the window is closed.
     * A 'cancel' event will be fired after the window is closed.
     */
    this.cancel = function() {
        LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "Cancel lightbox ID: " + lightboxDivId);
        /* stop listening for the esc key */
        document.stopObserving('keydown', self.escKeyHandler);
        /* close the window */
        self.close();
        self.fireEvent("cancel");
    }

    var saveHandlers = {};
    
    /*
     * Registers a function to be called when the save button on this lightbox is clicked.  When the save function
     * is called on this lightbox, save handlers are called in the order in which they are registered.
     * 
     * The save handler can return a response with validation errors in the following format:
     * 
	 * Exceptions Array:       response.validationExceptions
     * Number of Exceptions:   response.validationExceptions.length
     *   Message:              validationExceptions[i].message
     *   FieldId:              validationExceptions[i].fieldId  (this can be null)
     *   FieldValue:           validationExceptions[i].fieldValue  (this can be null)
     */
    this.registerSaveHandler = function(name, handlerFunction) {
    	saveHandlers[name] = handlerFunction;
	}
    
    /*
     * Processes the response from a save handler or from the main ajax response on this lightbox.
     * 
     * The response has to be in the following format:
     * 
	 * Exceptions Array:       response.validationExceptions
     * Number of Exceptions:   response.validationExceptions.length
     *   Message:              validationExceptions[i].message
     *   FieldId:              validationExceptions[i].fieldId  (this can be null)
     *   FieldValue:           validationExceptions[i].fieldValue  (this can be null)
     */
    this.handleSaveResponse = function(response) {
        LCAjaxLogger.trace("JSONResponse: " + response.responseText);
        LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "Received Validation Exceptions. Number: " + response.validationExceptions.length);
        
    	if (response != null && response.validationExceptions != null) {
	        // if we are supposed to refresh, do that now
	        if(response.refreshPanel) {
	            LCAjaxLogger.trace("LCAjaxLightbox => refreshing panel on exception");
	            self._loadContentPanel(actionUrl, function() {
	                self._displayErrorMessages(response);
	            });
	        } else {
	            self._displayErrorMessages(response);
	        }
	        
	        // if we are supposed to change the keepSaveButtonEnabled logic
	        if(response.keepSaveButtonEnabled) {
	            LCAjaxLogger.trace("LCAjaxLightbox => setting keepSaveButtonEnabled to: " + response.keepSaveButtonEnabled);
	            if(response.keepSaveButtonEnabled == 'true') {
	                self.keepSaveButtonEnabled();
	            } else if(response.keepSaveButtonEnabled == 'false') {
	                self.useStateChangesToControlSaveButton();
	            }
	        }
    	}
    }
    
    /* 
     * This will submit the form elements as parameters to the action url defined for the lightbox and if successful close the lightbox.
     *
     * A 'saveBeforeWindowClose' event will be fired after the data is saved and before the window is closesd.
     * A 'save' event will be fired after the data is saved and window closed.
     */
    this.save = function() {
        LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "Saving lightbox for ID: " + lightboxDivId);
    
        self.setSavingUI(true);
        var defaultSaveOverride = false;
    
        // call registered save handlers before doing final save
        $j.each(saveHandlers, function(name, handlerFunction) {
        	var response = handlerFunction.call();

        	if (response != null && response.validationExceptions != null) {
        		// handleResponse(response);
                defaultSaveOverride = true;
                self.setSavingUI(false); 
        	}
        });

        if (defaultSaveOverride) return;

        // continue with default save
        var container = self.getContentsContainer();
        var actionURL = container.getAttribute('action');
        var theParameters = serializedFormValuesForContainer(container);
        
        theParameters = theParameters+"&save=true";
        
        LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "Saving lightbox with URL: " + actionURL + '/' + theParameters);
        
        new Ajax.Request(actionURL, {
            method: 'post',
             parameters: theParameters,
             onSuccess: function(response) {
                    LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "Lightbox save successful for ID: " + lightboxDivId);
                    
                    var jsonResponse;
                    if(response.responseText.isJSON()) {
                        jsonResponse = response.responseText.evalJSON();
                    }
                    if(jsonResponse != null && jsonResponse.validationExceptions != null) {
                    	self.handleSaveResponse(jsonResponse)
                    } else {
                        /* the save was successful without any validation messages */
                        // hide the spinner before we close
                        $$('div.lightboxSpinner').each(function(item) {item.hide();});
                        // close window
                        self.close();
                        self.fireEvent("save", response);
                    }
             },
             onFailure: function(response) {
                displayLCAjaxErrorMessage(getBestFitLCAjaxErrorElementId(container), "Lightbox Save Failure: " + response.responseText);
                self.setSavingUI(false);
             },
             onException: function(request, exception) { 
                LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "Exception while saving panel => " + exception);
                displayLCAjaxErrorMessage(getBestFitLCAjaxErrorElementId(container), "Failed to save panel. Please try again.");
                self.setSavingUI(false);
             }
        });
        
    }
    
    // take the jsonResponse from a 'save' and display error messages
    this._displayErrorMessages = function(jsonResponse) {
        var exceptionMessage = '';
        // now display the error messages
        $A(jsonResponse.validationExceptions).each(function (exception) {
            LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "Exception: " + exception.message);
            exceptionMessage += "<li>" + exception.message;
            if(exception.fieldId != null) {
                LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "   Field: " + exception.fieldId);
            }
            if(exception.fieldValue != null) {
                exceptionMessage += " [" + exception.fieldValue + "]";
                LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "   Value: " + exception.fieldValue);
            }
            exceptionMessage += "</li>";
        });
        
        self._displayMessage(exceptionMessage);
        self.setSavingUI(false);  
    }
    
    // set to true to disable function buttons and show spinner, false to revert
    this.setSavingUI = function(saving) {
        if(saving) {
            LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "disable the save box");
            self.disableSaveButton();
            $$('img.lightboxCancelButtonEnabled').each(function(item) {item.hide();});
            $$('img.lightboxCancelButtonDisabled').each(function(item) {item.show();});
            $$('div.lightboxSpinner').each(function(item) {item.show();});
        } else {
            LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "enable the save box");
            self.enableSaveButton();
            $$('img.lightboxCancelButtonEnabled').each(function(item) {item.show();});
            $$('img.lightboxCancelButtonDisabled').each(function(item) {item.hide();});
            $$('div.lightboxSpinner').each(function(item) {item.hide();});
        }
    }
    
    this._isDisplayingMessage = function() {
    	var lightboxMessagesDiv = $(lightboxDivId).select('div.lcAjaxLightboxMessages').first();
    	if(!lightboxMessagesDiv || lightboxMessagesDiv.innerHTML == '') {
    		return false;
    	}
    	return true;
    }
    
    this._hideDisplayMessage = function(executeAfter) {
        var lightboxMessagesDiv = $(lightboxDivId).select('div.lcAjaxLightboxMessages').first();
        if(lightboxMessagesDiv) {
        	lightboxMessagesDiv.hide();
        	executeAfter.call();
        }
    }
    
    this._displayMessage = function(message, messageType) {
             var lightboxMessagesDiv = $(lightboxDivId).select('div.lcAjaxLightboxMessages').first();
             var contentsDiv = $(lightboxDivId).select('div.lcAjaxLightboxContents').first();
             
             if (!message || message == '') {
                contentsDiv.scrollTop = 0;
                Effect.Fade(lightboxMessagesDiv, { duration: 0.5 });
                lightboxMessagesDiv.innerHTML = '';
             } else {
                 if(messageType == 'warning') {
                    lightboxMessagesDiv.removeClassName('error');
                    lightboxMessagesDiv.addClassName('warning');
                 } else {
                    // error by default
                    lightboxMessagesDiv.removeClassName('warning');
                    lightboxMessagesDiv.addClassName('error');
                 }
             
                lightboxMessagesDiv.innerHTML = message;
                contentsDiv.scrollTop = 0;
                Effect.Appear(lightboxMessagesDiv, { duration: 0.5 });
             }
    }
    
    
    /* Will close the window. This should only be called internally as this doesn't fire events. Use "cancel" if it's a user invoked event.' */
    this.close = function() {
        // stop observing, otherwise if this DIV is used again we'll start multiplying the number of observers we have
        $(lightboxDivId).stopObserving();
        // get rid of the UI
        Effect.Fade($(lightboxDivId), { duration: 0.5 });

        $j('#' + lightboxDivId + '-overlayScroll').fadeOut(500, function(){
            self.allowScrolling();
            self.resetScrollPane();
            $j(this).find('.lcAjaxLightbox').empty(); // Added since the lightbox contents were remaining in the DOM after being closed. This was creating issues for lightbox JS, specifically selection of elements. Also generally good to clean up for performance
        })
    }
    
    /* 
     * Enable the save button
     */
    this.enableSaveButton = function() {
        $$('img.lightboxSaveButtonEnabled').each(function(item) {item.show();});
        $$('img.lightboxSaveButtonDisabled').each(function(item) {item.hide();});
    }
    
    /* 
     * Disable the save button
     */
    this.disableSaveButton = function() {
        $$('img.lightboxSaveButtonDisabled').each(function(item) {item.show();});
        $$('img.lightboxSaveButtonEnabled').each(function(item) {item.hide();});
    }

    /* This is used to trigger custom events. */
    this.fireEvent = function(eventName, httpResponse) {
        if(httpResponse == null) {
            LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "Firing LCAjaxLightbox:" + eventName + " event without X-lightboxId header.");
            $(lightboxDivId).fire("LCAjaxLightbox:"+eventName, { lightboxDivId: lightboxDivId, eventToken: self.eventToken });
        } else {
            /*
            *   The 'X-lightboxId' header is a custom response header injected by LCAjaxLightboxPanel in the appendToResponse method
            *   This is being done so that we know what the primary key is from the server side and can include that in our notification
            *   so that the event handlers know how to reference the data if they need to. 
            */
            var XlightboxId = httpResponse.getHeader('X-lightboxId');
            LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "Firing LCAjaxLightbox:" + eventName + " event with X-lightboxId header => " + XlightboxId);
            $(lightboxDivId).fire("LCAjaxLightbox:"+eventName, { lightboxDivId: lightboxDivId, eventToken: self.eventToken, id: XlightboxId });
        }
    }
};


var LCAjaxLightboxUtil = {

		/**
		 * Get the Javascript Object instance of LCAjaxLightbox for the given DivID (can be retrieved using 'currentlyVisibleLightbox().id')
		 */
		instanceOfLightboxForDivId : function(divId) {
            if (!divId) divId = currentlyVisibleLightbox().id;
			return window[divId];
		}
}

/**
 * This will find the currently visible lightbox on the page.
 */
function currentlyVisibleLightbox() {
    var foundLightbox;
    $$('div.lcAjaxLightbox').each(function(lightbox) {
        if(lightbox.visible()) {
            foundLightbox = lightbox;
        }    
    });

    return foundLightbox;
}


/**
 * If there is only one lightbox on the page this will return it.
 * 
 * If there are multiple on the page then it will find the currently visible lightbox on the page.
 */
function currentLightbox() {
    var lightboxes = $$('div.lcAjaxLightbox');
    if (lightboxes.size() == 1) {
        return lightboxes.last();
    } else {
        var visibleLightbox;
        lightboxes.each(function(lightbox) {
            if (lightbox.visible()) {
                visibleLightbox = lightbox;
            }
        });
        return visibleLightbox;
    }
}

/* 
 * Allow a lightbox panel to enable the save button based on its own logic, such as once all required fields are populated.
 *
 * This is intended for external javascript to call and will act on whatever the currently visible lightbox is.
 *
 * The lightbox may still not enable the button - as there is other logic the lightbox maintains - but this sets the switch so nothing external will block it from being enabled.
 */
function enableLCAjaxLightboxSave() {
    // find currently visible lightbox
    var lightbox = currentLightbox();
    
    LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "Enabling Lightbox save button for currentLightbox: " + lightbox);
    
    // tell the lightbox that externally we want the save button 'enabled'
    if(lightbox != null) {
        // fire an event at that div ... that's how we can go across the barrier from DOM to JS object since we don't have a reference to it here
        lightbox.fire("LCAjaxLightbox:enableSave");
    }
}

/* 
 * Allow a lightbox panel to disable the save button based on its own logic, such as when required fields aren't filled in.
 *
 * This is intended for external javascript to call and will act on whatever the currently visible lightbox is.
 *
 * This sets the 'switch' on the lightbox so that it will not enable it for any reason internally since the external switch is 'turned off' 
 */
function disableLCAjaxLightboxSave() {
    // find currently visible lightbox
    var lightbox = currentLightbox();
    
    LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "Disabling Lightbox save button for currentLightbox: " + lightbox);
    
    // tell the lightbox that externally we want the save button 'disabled'
    if(lightbox != null) {
        // fire an event at that div ... that's how we can go across the barrier from DOM to JS object since we don't have a reference to it here
        lightbox.fire("LCAjaxLightbox:disableSave");
    }
}


/* 
 * Allow a lightbox panel to disable the save button based on its own logic, such as when required fields aren't filled in.
 *
 * This is intended for external javascript to call and will act on whatever the currently visible lightbox is.
 *
 * This sets the 'switch' on the lightbox so that it will not enable it for any reason internally since the external switch is 'turned off' 
 */
function displayWarningMessageInCurrentLightbox(message) {
    // find currently visible lightbox
    var lightbox = currentLightbox();
    
    LCAjaxLogger.log(LCAjaxLogger.LIGHTBOX, "Displaying warning message for currentLightbox: " + lightbox);
    
    // send a message to the lightbox to display
    if(lightbox != null) {
        // fire an event at that div ... that's how we can go across the barrier from DOM to JS object since we don't have a reference to it here
        lightbox.fire("LCAjaxLightbox:displayMessage", {type:"warning", message:message});
    }
}


