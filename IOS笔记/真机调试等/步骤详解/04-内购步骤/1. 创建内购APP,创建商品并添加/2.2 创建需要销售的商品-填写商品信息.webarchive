/*
 * Methods for displaying/hiding tooltips 
 */
var LCToolTip = {
// display a tooltip and position it on a given element.
  show : function(event, message, closeOnEventType) {
    this._show(event, message, closeOnEventType, 'normal');
  },
  showWide : function(event, message, closeOnEventType) {
    this._show(event, message, closeOnEventType, 'wide');
  },
  showWithMessageFromContainer : function(event, container, closeOnEventType) {
    this._show(event, container.innerHTML, closeOnEventType, 'normal');
  },
  showWideWithMessageFromContainer : function(event, container, closeOnEventType) {
    this._show(event, container.innerHTML, closeOnEventType, 'wide');
  },
  // variable for storing the toolTipDiv
  _toolTipDiv : undefined,
  // internal function to create/show the tooltip
  _show : function(event, message, closeOnEventType, displayType) {
    Event.extend(event);
    // stop the event from propagating, it interferes with our listener below
    event.stopPropagation(event);

    var element = event.element(); 

    // hide any tooltips already open
    this.hideAnyOpenToolTips();

    // get the tool_tip if it's already been setup
    if(!this._toolTipDiv) {
      // not setup so do so now
      this._toolTipDiv = this._setup(displayType);
    }
    var tool_tip = this._toolTipDiv;

    // set the message
    tool_tip.select('div.tool-tip-main p').last().innerHTML = message;

    // set the location to show at
    this._setupDisplayLocation(element, tool_tip);

    // show it
    tool_tip.show();

    if(closeOnEventType == undefined || closeOnEventType == null) {
      closeOnEventType = 'click';
    }
    // observe for click to close it
    document.observe(closeOnEventType, this._clickObserver);
  },
  _clickObserver : function(event) {
    document.stopObserving(event.type, this._clickObserver);

    // use static referral since this gets run in the context of the event handler
    LCToolTip.hideAnyOpenToolTips();
  },
  hideAnyOpenToolTips : function() {
    $$('div.lc-tool-tip').each(function(item) {
      item.hide();
    });
  },
  _setupDisplayLocation : function(parentElement, toolTip) {
    var showOnRight = true;
    var toolTipVerticalOffset = 25;
    // figure out if we have space to show on right, if not we show on left
    var elementLeftPositionInViewport = parentElement.viewportOffset().left;
    var viewportWidth = document.viewport.getWidth();
    var elementWidth = parentElement.getWidth();
    var popupWidth = $(toolTip).getWidth(); 
    if((elementLeftPositionInViewport+elementWidth+popupWidth) > viewportWidth) {
      showOnRight=false;
    }

    if(showOnRight) {
      // width of element for placement of popup on right side
      var elementWidth = parentElement.getWidth()+3;
      var elementHeight = parentElement.getHeight();
      var offsetFromElementTop = (elementHeight/2)-35;
        
      var beakDiv = $(toolTip).select('div.tool-tip-beak').first();
      if(beakDiv.hasClassName('tool-tip-beak-right')) {
        beakDiv.removeClassName('tool-tip-beak-right');
      }

      var parentElementPosition = $j(parentElement).offset();
      var parentElementWidth = $j(parentElement).outerWidth();

      $j(toolTip).css({
        position: 'absolute',
        top: (parentElementPosition.top - toolTipVerticalOffset) + 'px',
        left: (parentElementPosition.left + parentElementWidth) + 'px'
      });

    } else {
      // show on left side of element
      var leftOffset = ($(toolTip).getWidth()*-1)+5;
      // change the beak to the right side of popup
      var beakDiv = $(toolTip).select('div.tool-tip-beak').first();
      beakDiv.addClassName('tool-tip-beak-right');
      // determine vertical placement
      var elementHeight = parentElement.getHeight();
      var offsetFromElementTop = (elementHeight/2) - toolTipVerticalOffset;
      // get the position of the Delete button
      //toolTip.clonePosition(parentElement, {offsetTop: offsetFromElementTop, offsetLeft: leftOffset, setWidth: false, setHeight: false});
      var offset = $(parentElement).cumulativeOffset();
      $(toolTip).setStyle({'left':offset.left + leftOffset + "px",'top':offset.top + offsetFromElementTop + "px"});
    }
  },
  _setup : function(displayType) {
    var tool_tip = new Element('div');
    tool_tip.addClassName('lc-tool-tip-type-' + displayType);
    tool_tip.addClassName('lc-tool-tip');

    if(displayType == 'wide') {
      tool_tip.addClassName('lc-tool-tip-wide');
    }
    //tool_tip.setStyle({'display': 'none'});

    var tool_tip_beak = new Element('div');
    tool_tip_beak.addClassName('tool-tip-beak');
    tool_tip_beak.innerHTML = '&nbsp;';
    tool_tip.insert(tool_tip_beak);

    var tool_tip_top = new Element('div');
    tool_tip_top.addClassName('tool-tip-top');
    if(displayType == 'wide') {
      tool_tip_top.addClassName('tool-tip-top-wide');
    }
    tool_tip_top.innerHTML = '&nbsp;';
    tool_tip.insert(tool_tip_top);

    var tool_tip_main = new Element('div');
    tool_tip_main.addClassName('tool-tip-main');
    if(displayType == 'wide') {
      tool_tip_main.addClassName('tool-tip-main-wide');
    }
    var tool_tip_main_p = new Element('p');
    tool_tip_main_p.setStyle({'margin': '0 25px', 'line-height': 'normal'});

    tool_tip_main.insert(tool_tip_main_p);
    tool_tip.insert(tool_tip_main);

    var tool_tip_bottom = new Element('div');
    tool_tip_bottom.addClassName('tool-tip-bottom');
    if(displayType == 'wide') {
      tool_tip_bottom.addClassName('tool-tip-bottom-wide');
    }
    tool_tip_bottom.innerHTML = '&nbsp;';
    tool_tip.insert(tool_tip_bottom);

    // now add this to the document
    document.body.appendChild(tool_tip);

    //var arr = $$(".lcAjaxLightboxContents");
    var arr = $$(".lightboxoverlayScroll");
    for(var i = 0, l = arr.length; i < l; i++){
      arr[i].observe('scroll',function(e){
        LCToolTip.hideAnyOpenToolTips();
      });
    };

    return tool_tip;
  }
}


