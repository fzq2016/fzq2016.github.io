var overlay_tooltip = function(elem){
  // removes all open tooltips
  $$('.temporary-tooltip').each(function(div_element){
    document.body.removeChild(div_element);
  });
  // find x,y position of target help icon
  var offset = Element.cumulativeOffset(elem);
  // there are may be many lightboxes on the page, find the one that's open
  var d=[];
  $$(".lcAjaxLightbox").each(function(elem){
    if(elem.style.display != 'none'){
      d = $(elem).select('.lcAjaxLightboxContents');
    };
  });
  // handle y position for nested scrollable elements
  var dv = (d[0] ? (document.documentElement.scrollTop || window.pageYOffset) - d[0].scrollTop : 0);
  var n = elem.children[0].cloneNode(true);
  n.id = '';
  n.className += ' temporary-tooltip';
  n.style['display'] = 'block';
  n.style['position'] = 'absolute';
  n.style['left'] = (offset[0] + 25) + 'px';
  //n.style['top'] = (offset[1] - 25 + dv) + 'px';
  n.style['top'] = (offset[1] - 25) + 'px';
  // attach the tooltip node to the bottom of the page, outside of containers that could hide/crop them
  var node = document.body.appendChild(n);
};
