/**
 * Read more functionality
 * @param maxLines The number of lines to display before hiding remaining text
 * @param elemenntId The element to check the number of lines
 */

function checkNumLines(maxLines, el) {

  var elementsToCheck = $j(el);

  $j.each(elementsToCheck, function(){

    var lineHeight = $j(this).css('line-height');

    if (isNaN(lineHeight)) {
      lineHeight = $j(this).css('font-size');
    }

    lineHeight = parseInt(lineHeight.replace('px',''));
    
    var divHeight = $j(this).innerHeight();

    var numLines = divHeight/lineHeight;

    if (numLines > maxLines) {


      $j(this).addClass("truncate");
      addMoreLink($j(this));
    }

  });
}

function addMoreLink(el) {
  $j(el).after('<a href="#" class="more-link" onclick="javascript:showAllLines(this);">More&hellip;</a>');
}

