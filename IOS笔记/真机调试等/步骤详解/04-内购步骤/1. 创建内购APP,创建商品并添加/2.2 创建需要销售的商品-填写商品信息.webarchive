#resources-help #help-menu.open, #resources-help #help-menu.disappearing {
  left: auto;
  right: -20px;
  top: 35px;
}
#resources-help #help-menu.open:before, #resources-help #help-menu.disappearing:before {
  left: auto;
  right: 14px;
  top: -14px;
}
#resources-help #help-menu.menuopened, #resources-help #help-menu.disappearing {
  left: auto;
  right: -20px;
  top: 35px;
}
#resources-help #help-menu.menuopened:before, #resources-help #help-menu.disappearing:before {
  left: auto;
  right: 14px;
  top: -14px;
}
#resources-help #help-menu.menuopened {
  opacity: 1;
  transform: translate3d(0, 0, 0);
  transition: opacity 0.2s, transform 0.1s;
}
#resources-help #help-menu li {
  display: block;
}
#resources-help #help-menu li a {
  margin-bottom: 0;
  display: block;
  padding: 10px 20px;
  color: #4c4c4c;
  white-space: nowrap;
}
#resources-help #help-menu li a:hover {
  background-color: #e5f2fe;
}
#resources-help #help-menu li.sectionBreak {
  border-bottom: 1px solid #d9d9d9;
}

.session-menu-username {
  font-size: 15px;
}

.session-menu-providername {
  opacity: 0.7;
  color: black;
  font-size: 13px;
}

#help {
  margin: 10px 0;
  padding: 0 0 0 10px;
  border-left: 1px solid #e5e5e5;
  float: left;
}
#help a {
  background: url('/itc/images/layoutelements/Resources.png') top left no-repeat;
  background-size: 20px 20px;
  width: 20px;
  height: 20px;
  background-position: 0 0;
  text-indent: -9999px;
  display: block;
}
#help a:hover {
  opacity: .65;
}

.footer:not(.not-global-style) {
  border-top: 1px solid #e5e5e5;
  margin: 25px 0;
  padding: 20px 0 0;
  text-align: center;
  font-size: 11px;
  line-height: 14px;
  font-family: 'Helvetica Neue', sans-serif;
  font-size: 1em;
  font-weight: 300;
  -webkit-font-smoothing: antialiased;
  text-rendering: optimizeLegibility;
  -moz-osx-font-smoothing: grayscale;
  color: #333;
}
.footer:not(.not-global-style) p {
  font-family: 'Helvetica Neue', sans-serif;
  font-weight: 300;
  font-size: 11px;
  line-height: 14px;
  margin-bottom: 2px;
  -webkit-font-smoothing: antialiased;
  text-rendering: optimizeLegibility;
  -moz-osx-font-smoothing: grayscale;
  color: #333;
}
.footer:not(.not-global-style) ul {
  margin: 0;
  padding: 0;
}
.footer:not(.not-global-style) ul li {
  display: inline-block;
  padding: 0 10px;
  border-right: 1px solid #e5e5e5;
}
.footer:not(.not-global-style) ul li:last-child {
  border-right: none;
}
.footer:not(.not-global-style) a {
  color: #0088ff;
}
.footer:not(.not-global-style) a:hover {
  text-decoration: none;
  color: #0074d9;
}

/* section highlighting */
.ManageApps #manage-apps span,
.ManageMusic #manage-music span,
.ManageMusicVideos #manage-music-videos span,
.ManageTVShows #manage-tv-shows span,
.ManageMovies #manage-movies span,
.ManageECopy #digital-copy span,
.ManageBooks #manage-books span,
.SalesandTrends sales-trends span,
.FinancialReports #financial-reports span,
.ManageiAd #iAd span,
.ManageUsers #manage-users span,
.ContractsTaxAndBanking #contracts-tax-banking span,
.Tickets #tickets span,
.CatalogReports #Catalog-Reports span,
.statistics #statistics span {
  border: 1px solid #b7dafd;
  background-color: #e5f2fe;
  color: #0088ff;
}

.btn, .modal-buttons li a {
  position: relative;
  display: inline-block;
  vertical-align: middle;
  cursor: pointer;
  text-align: right;
  padding: 4px 10px 5px;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  -ms-border-radius: 3px;
  -o-border-radius: 3px;
  border-radius: 3px;
  border: 1px solid #4cabff;
  outline: none;
  color: #0088ff;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-font-smoothing: antialiased;
  text-rendering: optimizeLegibility;
  -moz-osx-font-smoothing: grayscale;
  font-family: 'Helvetica Neue', sans-serif;
  background: none;
  font-size: 15px;
  line-height: 19px;
}
.btn.primary, .modal-buttons li a.primary {
  font-weight: 500;
}

/* MODALS */
.ng-modal-overlay {
  display: none;
  position: fixed;
  z-index: 9500;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.1);
  overflow-y: auto;
  font-size: 0;
  text-align: center;
}

.ng-modal:not(.has-flex) .ng-modal-overlay:before {
  content: '';
  display: inline-block;
  height: 100%;
  vertical-align: middle;
  margin-right: -0.25em;
}

.ng-modal-wrapper {
  display: inline-block;
  vertical-align: middle;
  padding: 50px 0;
  width: 100%;
}

.modal-dialog:not(.sign-out-modal) {
  display: inline-block;
  vertical-align: middle;
  width: 500px;
  text-align: left;
  background-color: #fff;
  -webkit-box-shadow: 0 0 50px rgba(0, 0, 0, 0.1);
  -moz-box-shadow: 0 0 50px rgba(0, 0, 0, 0.1);
  box-shadow: 0 0 50px rgba(0, 0, 0, 0.1);
  -webkit-border-radius: 10px;
  -moz-border-radius: 10px;
  -ms-border-radius: 10px;
  -o-border-radius: 10px;
  border-radius: 10px;
  -webkit-font-smoothing: antialiased;
  text-rendering: optimizeLegibility;
  -moz-osx-font-smoothing: grayscale;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  color: #4C4C4C;
}
.modal-dialog:not(.sign-out-modal) .errormessages {
  font-size: 13px;
  line-height: 16px;
  border-top: 1px solid #d79fa0;
  border-bottom: 1px solid #d79fa0;
  padding: 20px;
  background-color: #fee5e5;
}
.modal-dialog:not(.sign-out-modal) .pagemessage {
  margin: 20px 30px 30px;
}
.modal-dialog:not(.sign-out-modal) .loading {
  position: absolute;
  top: 0;
  -webkit-border-radius: 10px;
  -moz-border-radius: 10px;
  -ms-border-radius: 10px;
  -o-border-radius: 10px;
  border-radius: 10px;
}
.modal-dialog:not(.sign-out-modal) a, .modal-dialog:not(.sign-out-modal) li a {
  color: #0088ff;
}
.modal-dialog:not(.sign-out-modal) .modal-dialog-header {
  padding: 30px 30px 0;
}
.modal-dialog:not(.sign-out-modal) .modal-dialog-content {
  padding: 30px 30px 15px 30px;
  font-size: 15px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.modal-dialog:not(.sign-out-modal) .modal-dialog-content.multiple {
  padding-bottom: 0;
}
.modal-dialog:not(.sign-out-modal) .modal-dialog-content p {
  font-size: 18px;
  line-height: 22px;
  font-family: 'Helvetica Neue', sans-serif;
  color: #4C4C4C;
  -webkit-font-smoothing: antialiased;
  text-rendering: optimizeLegibility;
  -moz-osx-font-smoothing: grayscale;
  font-weight: 300;
}
.modal-dialog:not(.sign-out-modal) .modal-dialog-content .scroll-content p {
  font-size: 15px;
  line-height: 19px;
}
.modal-dialog:not(.sign-out-modal) .modal-dialog-content h1 {
  font-size: 19px;
  line-height: 22px;
  font-weight: 500;
  margin-bottom: 14px;
  margin-top: 0;
  font-family: 'Helvetica Neue', sans-serif;
  color: #4C4C4C;
}
.modal-dialog:not(.sign-out-modal) .modal-dialog-content ul, .modal-dialog:not(.sign-out-modal) .modal-dialog-content ol {
  margin: 0 0 0 20px;
  padding: 0;
  font-weight: 200;
  list-style: disc;
}
.modal-dialog:not(.sign-out-modal) .modal-dialog-content ul li, .modal-dialog:not(.sign-out-modal) .modal-dialog-content ol li {
  margin-bottom: 10px;
  -webkit-font-smoothing: antialiased;
  text-rendering: optimizeLegibility;
  -moz-osx-font-smoothing: grayscale;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  color: #4C4C4C;
  font-family: 'Helvetica Neue', sans-serif;
}
.modal-dialog:not(.sign-out-modal) .modal-dialog-content .sub-text {
  margin: 10px 0;
  text-align: right;
  font-size: 0.9em;
  color: #d9d9d9;
}
.modal-dialog:not(.sign-out-modal) .modal-dialog-content + .modal-dialog-content {
  padding-top: 0;
}
.modal-dialog:not(.sign-out-modal) .modal-dialog-content .fieldwrapper:last-child {
  margin-bottom: 15px;
}
.modal-dialog:not(.sign-out-modal) .modal-dialog-content .fieldwrapper:last-child.nobottom {
  margin-bottom: 0;
}
.modal-dialog:not(.sign-out-modal) .modal-dialog-content .fieldwrapper:last-child.bottom5 {
  margin-bottom: 5px;
}

.modal-dialog-content table {
  margin-bottom: 15px;
}

.modal-buttons {
  border-top: 1px solid #e5e5e5;
  padding: 15px;
  overflow: hidden;
  width: 100%;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.modal-buttons .right-buttons {
  float: right;
}
.modal-buttons .right-buttons button + button {
  margin-left: 10px;
}
.modal-buttons .left-buttons {
  float: left;
}
.modal-buttons.noborder {
  border-top: none;
}
.modal-buttons a {
  color: #0088ff;
}
.modal-buttons a:hover {
  text-decoration: none;
  color: #0088ff;
}
.modal-buttons a.warningLink {
  color: #ff3b30;
}
.modal-buttons ul.leftbuttons {
  float: left;
}
.modal-buttons ul {
  float: right;
}
.modal-buttons li {
  float: left;
}
.modal-buttons li + li {
  margin-left: 10px;
}
.modal-buttons li.main {
  font-weight: 500;
}

.scroll-content {
  border: 1px solid #d9d9d9;
  padding: 10px;
  overflow-y: auto;
  font-size: 13px;
  line-height: 16px;
  margin-bottom: 10px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.scroll-content.fixedHeight {
  max-height: 200px;
}
.scroll-content p {
  font-size: 13px;
  line-height: 16px;
  margin-top: 0;
  margin-bottom: 15px;
}
.scroll-content ul li, .scroll-content ol li {
  font-size: 13px;
  line-height: 16px;
  margin-bottom: 10px;
  -webkit-font-smoothing: antialiased;
  text-rendering: optimizeLegibility;
  -moz-osx-font-smoothing: grayscale;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  color: #4C4C4C;
  font-family: 'Helvetica Neue', sans-serif;
}
.scroll-content ol.digit {
  list-style: decimal;
  margin: 0 0 0 20px;
}
.scroll-content ol.low-letter {
  list-style: lower-alpha;
  margin: 0 0 0 20px;
}
.scroll-content strong {
  font-weight: 500;
}

.footer.legacy_styling {
  margin-top: 35px;
  background: #fff !important;
  border-left: none;
  border-right: none;
  border-bottom: none;
  padding: 10px 0 20px;
  width: auto;
  min-width: 978px;
}

.modal-dialog p.loading-message {
  background: url("/itc/images/layoutelements/spinner_gray.gif") center center no-repeat;
  background-size: 42px 42px;
  padding: 20px 0 50px;
  background-position: center bottom;
  background-size: 35px 35px;
  font-size: 18px !important;
  text-align: center;
}


