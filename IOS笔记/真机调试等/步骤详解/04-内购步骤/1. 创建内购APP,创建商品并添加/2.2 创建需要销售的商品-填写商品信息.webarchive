        // need to reset the width since the clone resized to the button
        confirmDeleteDiv.setStyle({width: originalBoxWidth, height: originalBoxHeight});
        
        // set the event observer for if the delete confirmation is selected
        deleteConfirmButton.observe('click', function(event) {
            LCAjaxLogger.trace("Going to DELETE Item ID: " + itemID);

            self.performDelete(itemID);
            // cancel all listeners on the confirmation box
            deleteConfirmButton.stopObserving('click');
            deleteCancelButton.stopObserving('click');

            popOverOpen = false;

            // update the UI
            Effect.toggle(confirmDeleteDiv, 'slide', { duration: 0.2 }); return false;

            
        });
        
        // set the event observer for if the cancel is selected
        deleteCancelButton.observe('click', function(event) {
