////////////////////////////////////////////////////////////////////////////////
// ensureMinimumNumberOfRows(element, params) - ensures a table will have a minimum
// ==========================================   number of visible rows.   
// Supported params are:  
//  numberOfRows - gives the minimum number of rows that will appear 
//  rowHeight - height, in pixels for added rows
// *NOTE: Does not support empty tables 
////////////////////////////////////////////////////////////////////////////////
function ensureMinimumNumberOfRows(element, params) { 
  var minimumNumberOfRows  = params.numberOfRows || 10,        // default minimumNumberOfRows is 10
      rowHeight            = params.rowHeight    || 30,	       // default rowHeight (for new rows) is 30px
      oTable = $(element).select('div.resultList table')[0],   // Get the first element as $(element).select returns an array 
      numberOfRowsToInsert = minimumNumberOfRows - oTable.rows.length + 1;
 
  if (numberOfRowsToInsert > 0) {
	var clonedRow   = oTable.rows[ oTable.rows.length - 1 ]
	    clonedCells = clonedRow.getElementsByTagName('td');
	for (var i=0;i<numberOfRowsToInsert;i++) {  
      var oRow = document.createElement("TR");
      for (j=0;j<clonedCells.length;j++) {
	    var oCell = clonedCells[j].cloneNode(false);
	    oCell.style.height = rowHeight + "px";  
	    oCell.appendChild( document.createTextNode("\u00a0") );
	    oRow.appendChild(oCell);
      }
      oTable.appendChild( oRow );		
	}
  }
}
////////////////////////////////////////////////////////////////////////////////
// determineIfLengthyContentButtonIsDisplayed(oMoreLessButton) - Shows/Hides a ...More/Less.. button
// ===========================================================   associated with expandOrCollapseLengthyContent
// This function expects oMoreLessButton to have an id like "btn_myLongContent".  
// See structure described in expandOrCollapseLengthyContent comments.
////////////////////////////////////////////////////////////////////////////////
function determineIfLengthyContentButtonIsDisplayed( oMoreLessButton ) {
  if (typeof(oMoreLessButton)=="string")  { oMoreLessButton = document.getElementById(oMoreLessButton); }	
  var oLongDescriptionContent = document.getElementById( oMoreLessButton.id.split("_")[1] );
  if (oLongDescriptionContent) {
    if (oLongDescriptionContent.scrollHeight <= oLongDescriptionContent.clientHeight) {	
	  showOrHide( oMoreLessButton, false );  // Hide the "More..." button if the content being displayed fits within its area
	} else {
	  showOrHide( oMoreLessButton, true );	
	}
  } else {
    if (console) { console.log('Could not find oLongDescriptionContent associated with MoreLess Trigger element id ("' + oMoreLessButton.id + '"). I was looking for "' + oMoreLessButton.id.split("_")[1] + '"'); }	
  }
}
////////////////////////////////////////////////////////////////////////////////
// expandOrCollapseLengthyContent(e) - Expands/collapses lengthy content by providing a
// =================================   ...More button and/or a Less.. button
// This function expects the following HTML structure:
/*
**  <div style="width:300px;">
**    <div id="myLongContent" style="height:100px;overflow:hidden;">
**      .. Lengthy content goes here ..
**    </div>
**    <div style="position:relative;float:right;">
**      <a id='btn_myLongContent' href="#" onclick="expandOrCollapseLengthyContent(event)">...More</a>
**    </div>
**  </div>
*/
// Note that the id of the <a> "triggerElement" contains the id of the oHTMLElement we are wan ted to expand/collapse
// You can, of course, play with the heights/widths as desired.  It is the ids and the parent container setup that are important.
//
////////////////////////////////////////////////////////////////////////////////
function expandOrCollapseLengthyContent(e) {
  var _event=(window.event)? event : e,    // Normalize the event object into standard Object regardless of Browser
      oTriggerElement = (_event.srcElement) ? _event.srcElement : _event.currentTarget;   // Obtain handle to actual button/href that triggered the event
  if (oTriggerElement && oTriggerElement.id) {
    var oHTMLElement = document.getElementById( oTriggerElement.id.split("_")[1] );  // Obtain a handle the HTMLElement containing the content
    if (oHTMLElement) {
      if (oHTMLElement._isExpanded === undefined) { oHTMLElement._isExpanded = false; }
      if (oHTMLElement._originalHeight === undefined) { oHTMLElement._originalHeight = parseFloat(oHTMLElement.offsetHeight); }
      if (!oHTMLElement._isExpanded) {
        oHTMLElement.style.height="inherit";
        oHTMLElement.style.overflow="visible";
        oTriggerElement.innerHTML = "Less...";
      } else {
        oHTMLElement.style.overflow="hidden";
        oHTMLElement.style.height=oHTMLElement._originalHeight + "px";
        oTriggerElement.innerHTML = "...More";
      }
      oHTMLElement._isExpanded = !oHTMLElement._isExpanded;
    } else {
      if (console) { console.log('Could not find oHTMLElement associated with Trigger element id ("' + oTriggerElement.id + '"). I was looking for "' + oTriggerElement.id.split("_")[1] + '"'); }
    }
  } else {
    if (console) { console.log('Trigger element is not set up right. (example: id="btn_myLongContent")');  }
  }
}
////////////////////////////////////////////////////////////////////////////////
//
// showOrHide([object|string] oHTMLElement, boolean bShowOrHide)
//           Shows or Hides an HTMLElement.
//           You can pass in the id to an object or the actual object
//
////////////////////////////////////////////////////////////////////////////////
function showOrHide(oHTMLElement, bShowOrHide) {
  try   {
    if (typeof(oHTMLElement)=="string")  {
      oHTMLElement = document.getElementById(oHTMLElement);
    }
    if (oHTMLElement && oHTMLElement.style) {
      if (bShowOrHide == 'inherit') {
        oHTMLElement.style.visibility = 'inherit';
      } else {
        if (bShowOrHide) {
          if (oHTMLElement.nodeName == 'TR') {
            oHTMLElement.style.visibility = 'inline-table';
          } else {
            oHTMLElement.style.visibility = 'visible';
          }
        } else {
          oHTMLElement.style.visibility = 'hidden';
        }
        try {
          if (bShowOrHide) {
            oHTMLElement.style.display = 'block';
          } else {
            oHTMLElement.style.display = 'none';
          }
        }
        catch (ex) {
        }
      }
    }
  }
  catch (ex) {
  }
}


