    /**
     * Filter the list of items
     */
     this.search = function(query) {
    	if(self.previousQuery == query) {
    		// we already searched on this so don't do it again
    		return;
    	}
    	
    	if(self.previousQuery == undefined && query == '') {
    		// this is the first search and it's an empty query, so ignore it
    		return;
    	}
    	
    	// remember this query (do this before we execute so any others coming in will be ignored if they are the same even while we're processing the previous request)
    	self.previousQuery = query;
    	
        if (self.isSearchOnServer()) {
            self.searchOnServer(query);
        } else {
            // no pagination so sort locally
            self.searchLocally(query);
        }
    }
    
    
    /**
     * Filter the search list via javascript on the client side.
     */
    this.searchLocally = function(query) {
        // lowercase it before searching
        query = query.toLowerCase();
        LCAjaxLogger.log(LCAjaxLogger.LIST, "Search Query is: " + query);
        
        // the effect queue we'll use
        var queue = Effect.Queues.get('ajaxListFilterQueue');
        LCAjaxLogger.trace("Queue: " + queue + " Size: " + queue.size());
        // cancel all queues outstanding since we're starting to change things again and the user can often type faster than the UI refreshes (and it gets all out of sync)
        queue.each(function(effect) { effect.cancel(); });
        
        // the list of searchable rows
        var searchableRows = listDiv.select('LI.ajaxListLI');
        LCAjaxLogger.trace("list: " + searchableRows);
        
        if(query == '') {
            // show everything again
            searchableRows.each(function(aRow) {
                //$(aRow).show();
                new Effect.Appear(aRow, { duration: 0.5, queue: { scope: 'ajaxListFilterQueue' }});
            });
        
        } else {
            var i=1; // for debugging
            // iterate each row in searchableRows
            searchableRows.each(function(aRow) {
                // for each row we'll find the DIVs that are marked as "searchable"
                var rowElements = $(aRow).select('div.ajaxListSearchable');
                LCAjaxLogger.trace("row " + i++ + " elements: " + rowElements);
                
                // find if any of the DIVS match what we're looking for
                var matchedDivs = rowElements.findAll(function(searchableDiv) {
                    var valueToSearch = searchableDiv.innerHTML.strip().toLowerCase();
                    var searchResult = valueToSearch.indexOf(query) > -1;
                    LCAjaxLogger.trace("    Searching in Value: " + valueToSearch + "  => " + searchResult);
                    return searchResult;
                });
                
                LCAjaxLogger.trace("    Row Matched: " + (matchedDivs.size() > 0));
                
                // if we did not match, we reject this row (hide it)
                if(matchedDivs.size() == 0) {
                    new Effect.Fade(aRow, { duration: 0.5, queue: { scope: 'ajaxListFilterQueue' }});
                    //$(aRow).hide();
                } else {
                    new Effect.Appear(aRow, { duration: 0.5, queue: { scope: 'ajaxListFilterQueue' }});
                    //$(aRow).show();
                }
            });
        }
    }
    
    /**
     * Filter the search list on the server and refresh the list.
     */
    this.searchOnServer = function(query) {
        LCAjaxLogger.log(LCAjaxLogger.LIST, "Searching list on server: " + query);
        var ownsSpinner = self.showSpinner();
        
        var requestURL = getAjaxActionURL(searchActionUrl) + "?query=" + query;
        LCAjaxLogger.log(LCAjaxLogger.LIST, "Request URL: " + requestURL);

        /* send it to the server */
        new Ajax.Request(requestURL, {
            onSuccess : function(response) {
                // if it worked we have nothing to do
                LCAjaxLogger.log(LCAjaxLogger.LIST, "Successfully performed search on server.");
                var pageNumbers = response.responseJSON;
                self.displayPageNumbers(pageNumbers);
                self.refreshData(function() {
                    /* we don't check for ownership here as we are definitely done doing all work at this point */
                    self.hideSpinner();                    
                    self.fireEvent("searched");
                });
            },
            onFailure : function(response) {
                self.displayAjaxListErrorMessage("Failed to perform search on server. Please try again.");
            },
            onException : function(request, exception) {
                LCAjaxLogger.log(LCAjaxLogger.LIST, "Exception while performing search on server => " + exception);
                self.displayAjaxListErrorMessage("Failed to perform search on server. Please try again.");
            }
        });
    }
    
    
