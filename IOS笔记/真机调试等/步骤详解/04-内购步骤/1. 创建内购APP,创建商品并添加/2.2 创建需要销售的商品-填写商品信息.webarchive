    return $(ajaxListId+'Container');
}

/* 
 * Allow an external actor to trigger a list to refresh its data.
 *
 * Pass in the AjaxList Container Div as the argument.
 *
 * Example: refreshLCAjaxList('lbListContainer');
 */
function refreshLCAjaxList(lcAjaxListDiv) {
    LCAjaxLogger.log(LCAjaxLogger.LIST, "Firing event for list reload...");
    $(lcAjaxListDiv).fire("LCAjaxList:reload", { div: lcAjaxListDiv });
}

function usingImageAddButton(ajaxListId) {
	    return $j('span.ajaxListAddButton[listId=' + ajaxListId +']').size() == 0;
}

/* 
 * Enable the Add Button for the given Ajax List ID.
 */
function enableAddButtonForLCAjaxList(ajaxListId) {
    LCAjaxLogger.log(LCAjaxLogger.LIST, "Enable Add Button for List: " + ajaxListId);
    if (usingImageAddButton(ajaxListId)) {
        	// enable for image button
    	   $$('img.ajaxListAddButtonEnabled[listId=' + ajaxListId +']').each(function(item) {item.show()});
       	$$('img.ajaxListAddButtonDisabled[listId=' + ajaxListId +']').each(function(item) {item.hide()});
    }
    else {
    	   // enable for span button
    	   $j('span.ajaxListAddButton[listId=' + ajaxListId +']')
    		      .removeClass('disabled')
    		      .removeClass('ajaxListAddButtonDisabled')
    		      .addClass('ajaxListAddButtonEnabled');
    }
}

/* 
 * Disable the Add Button for the given Ajax List ID.
 */
function disableAddButtonForLCAjaxList(ajaxListId) {
    LCAjaxLogger.log(LCAjaxLogger.LIST, "Disable Add Button for List: " + ajaxListId);
    if (usingImageAddButton(ajaxListId)) {
    	   // disable for span button
	       $$('img.ajaxListAddButtonEnabled[listId=' + ajaxListId +']').each(function(item) {item.hide()});
	       $$('img.ajaxListAddButtonDisabled[listId=' + ajaxListId +']').each(function(item) {item.show()});
    }
    else {
    	   // disable for span button
    	   $j('span.ajaxListAddButton[listId=' + ajaxListId +']')
    		     .removeClass('ajaxListAddButtonEnabled')
    		     .addClass('disabled')
			         .addClass('ajaxListAddButtonDisabled');
    }
}

