    doneUpload : function(responseText, httpStatus) {
        enableLCPageSaveButton();
        var success = false;
        var message;
        
        if ((!responseText || responseText == '') && (httpStatus == 0 || httpStatus >= 400)) {
           success = false;
           message = 'There was a problem uploading your video. Open it with QuickTime and make sure it is saved in a supported format.';
        } else {
           var response = responseText.evalJSON();
           if(response.localizedMessage) {
              if (response.failValidation) {
                 message = 'Video files must be less than 2GB.';
              } else {            
                  message=response.localizedMessage;
              }
           } else {
              if(response.statusCode == 403) {
                 message = "User Not Authorized";
              } else {
                 message = "Upload Error: " + response.statusCode;
              }
           }
           if(!response.statusCode) {
              success = true;
           }
        }
        
        if(success) {
            // we are overriding 'updateStatus' so need to handle the rest and stopSpinner
            this.reset();
            this.stopSpinner();
            
            // display video if the response has any
            this.displayVideo();
            /* Report the upload status to the back end server */
            this.submitITCVideoToken(response, this.itcStatusURL, function(result) {
                                                    this.displayStatus("Your video has been uploaded successfully and will begin processing after you click Save. You will be sent an email once the video processing is complete.");
                                                    /*  fire event -- after monitoring the upload and adding the image */
                                                    this.fireEvent("videoAdded");
                                                }, 
                                                function(errorMsg, status) {
                                                    this.videoWell.removeClassName("completed");
                                                    this.videoWell.addClassName("failed");
                                                    this.displayErrorMessage(errorMsg);
                                                    /*  fire event -- failed */
                                                    this.fireEvent("videoFailed");
                                                });
        } else {
            this.videoWell.removeClassName("completed");
            this.videoWell.addClassName("failed");
            if(message) {
                this.displayErrorMessage(message);
            } else {
                this.displayErrorMessage("Unknown Error Occurred");
            }
            /* Report the error to the back end */
            this.submitITCVideoToken({ responses: [] }, this.itcStatusURL, function() {}, 
                                                function(errorMsg, status) {
                                                    LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Error submitting invalid token: " + errorMsg);
                                                });
            this.enableUploadButton();
            this.displayDeleteOption();
            /*  fire event -- failed */
            this.fireEvent("videoFailed");
        }
    },
    /**
     * Logic specific to this implementation for UI work after the video is successfully uploaded.
     */
    displayVideo : function() {
        this.videoWell.removeClassName("failed");
        this.videoWell.addClassName("completed");
        this.displayDeleteOption();
    },
    displayDeleteOption : function() {
        // insert the delete div if we have a delete url
        if(this.itcDeleteURL != null) {
            // if it's not already created, create it now
            if(!this.deleteOverlay) {
                this.deleteOverlay = new Element('div');
                this.deleteOverlay.addClassName('lcUploaderImageDelete');
                this.deleteOverlay.innerHTML = "Delete";
                this.deleteOverlay.observe('click', function(event) {
                    this.deleteVideo();
                }.bind(this));
                this.videoWell.insert(this.deleteOverlay);
            } else if (!this.deleteOverlay.visible()) {
               this.deleteOverlay.show();
            }
        }       
    },
    hideDeleteOverlay: function() {
        if (this.deleteOverlay) {
            this.deleteOverlay.hide();
        }    
    },
    /**
     * Called when the X icon is clicked.
     */
    deleteVideo : function() {
        var requestURL = getAjaxActionURL(this.itcDeleteURL);
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Delete Video with Request URL: " + requestURL);
        
        /* send it to the server */
        new Ajax.Request(requestURL, {
              onSuccess: function(response) {
                  
                   // if it worked we have nothing to do
                    LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Successfully deleted item on server.");

                    // hide the UI
                    this.removeThumbnail();
                    this.videoWell.removeClassName("completed");
                    this.videoWell.removeClassName("failed");
                    this.videoWell.hide();
                    this.hideDeleteOverlay();
                    // Clean any messaging that was hanging
                    this.displayErrorMessage('');
                    this.displayStatus('');
                    /*  fire event */
                    this.fireEvent("videoDeleted");
                    
                    /* change the form to a value so it recognizes it changed and the lightbox 'change' listener sees that the form has been modified */
                    if (this.filenameHidden) { this.filenameHidden.setValue("deleted"); }
                    if(this.uploadForm) {
                        /* trigger that the form changed */
                        triggerEvent(this.uploadForm, 'change');
                    }
                    
              }.bind(this),
              onFailure: function(){
                  this.displayAjaxErrorMessage("Failed to delete image. Please try again."); 
              }.bind(this),
              onException: function(request, exception) { 
                 LCAjaxLogger.trace("Exception while deleting image => " + exception);
                 this.displayAjaxErrorMessage("Failed to delete image. Please try again.");
              }.bind(this)
        });
    },
    /**
     * OVERRIDE: This adds custom logic for the UI with the "Uploading" part.
     */
    displayPercentage : function(percentage) {
        if(percentage) {
            this.percentageDiv.innerHTML = "Uploading<br>" + percentage + "%";
        } else {
            this.percentageDiv.innerHTML = "";
        }
    },
    /**
     * DirectUploader
     */
    submitITCVideoToken: function(data, destination, cbSuccess, cbError) {
        var self = this;
        this.callbackSuccess = cbSuccess || its.emptyFunction;
        this.callbackError = cbError || its.emptyFunction;
        // Simple way to submit the token 
        var xhr = new XMLHttpRequest();
        xhr.open('POST', destination, true);
        xhr.onreadystatechange = function() {
           if (this.readyState != 4) return;
    
           var httpStatus = 0;
           try {
               httpStatus = this.status || 0;  // FYI: status is zero for http 500s
           } catch (e) {}
    
           var rawData = this.responseText;
           var resData = null;
           try {
               resData = its.evalJson(rawData); // TODO: try/catch logic similar to jkl
           } catch (e) {
               LCAjaxLogger.log(LCAjaxLogger.UPLOADER, 'invalid ticket token response, json eval failed: "' + rawData + '", status: ' + httpStatus);
           }
    
           var success = httpStatus >= 200 && httpStatus <= 299;
           if (!success) {
               var errorMsg = resData && resData.localizedMessage ? resData.localizedMessage : resData.nonLocalizedMessage;
               if (!errorMsg && resData && resData.nonLocalizedMessage) { errorMsg = resData.nonLocalizedMessage; }
               LCAjaxLogger.log(LCAjaxLogger.UPLOADER, 'invalid ticket token response, http status code: "' + httpStatus + '", msg: ' + errorMsg);
               self.callbackError(errorMsg, httpStatus);
               return;
           }
           self.callbackSuccess(resData);
        }
        // Post the data
        var jsonPayload = Object.toJSON(data);
        xhr.send(jsonPayload);
}
    
});

/* 
 * Version of LCUploader specific to handling MZArbitraryFile objects
 * 
 * Arguments:
 *      uploadContainer = (Required) the ID or Element of the DIV wrapping the UI (see LCUploadFileComponent.html).
 *      statusURL = (Required) AJAX URL for retrieving status reports on upload progress and completion (JSON).
 *      sortURL = (Optional) AJAX URL for submitting a sort order change of multiple items.
 *      deleteURL = (Optional) AJAX URL for deleting an image.
 *      maxNumberImages = (Optional) The maximum number of files to support and display. [Default: 1]
 *
 * Custom events are fired off that can be observed for hooking into the flow and adding custom behavior.
 *
 * These events are:
 *  - LCUploader:uploadBegin
 *  - LCUploader:fileAdded
 *  - LCUploader:fileDeleted
 *  - LCUploader:uploadFailed
 * 
 * An event will return three data values that can be used by the event handler:
 *  - uploadContainerId => The ID of the Div for the lightbox.
 *
 * These values can be retrieved via the "event.memo.*" property paths, such as "event.memo.uploadContainerId".
 *
 * An example listener is:
 *      document.observe("LCUploader:fileAdded", function(event) {
 *          console.log("UploadContainerId: " + event.memo.uploadContainerId);
 *      });
 */
var LCUploaderArbitraryFiles = Class.create(LCUploader, new function(){
    
    this.initialize = function($super, args) {
        /* initialize super class */
        $super(args);
        
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Initializing LCUploaderArbitraryFiles. [" + this.uploadContainer + "]");
        
        /* image specific arguments */
        this.maxNumberFiles = args.maxNumberFiles || 3;
        this.deleteURL = args.deleteURL           || "";
        this.fileIconURL = args.fileIconURL       || "";
        
        this.fileContainer = this.uploadContainerElement.select('div.lcUploaderFileContainer').first();
        this.jsonFileProperty = 'arbitraryFile'+'_';
    }
    
    /**
     * OVERRIDE of parent. This is invoked once the file is successfully uploaded. 
     */
    this.handleUploadSuccess = function(statusJson) {
        // display images if the response has any
        this.displayFiles(statusJson);
        /*  fire event -- after monitoring the upload and adding the file */
        this.fireEvent("fileAdded",statusJson);
    }
    
    /* If the uploader has a file or files to display, do it here */
    this.displayFiles = function(statusJson) {
        var fileCount = 1;
        
        // if there is a file defined in the JSON but no file on screen, let's add it
        var filePropertyName = this.jsonFileProperty+fileCount;
        while(statusJson[filePropertyName] != null) {
            // add the image if it isn't already there
            if(this.fileContainer.select('[fileId='+statusJson[filePropertyName].fileId+']').length == 0) {
                LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Display Existing File: " + filePropertyName + " => " + statusJson[filePropertyName] + "  URL: " + statusJson[filePropertyName].url + "  Filename: " + statusJson[filePropertyName].filename + "  Timestamp: " + statusJson[filePropertyName].timestamp);
                this.insertFile(statusJson[filePropertyName]);
            } else {
                LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Update Existing File: " + filePropertyName + " => " + statusJson[filePropertyName] + "  URL: " + statusJson[filePropertyName].url + "  Filename: " + statusJson[filePropertyName].filename + "  Timestamp: " + statusJson[filePropertyName].timestamp);
                this.updateFile(statusJson[filePropertyName]);
            }
            // go to next image
            fileCount++;
            filePropertyName = this.jsonFileProperty+fileCount;
        }
    } 
    
    /* 
     * Used for internal calls that have the JSON object.
     */
    this._addFile = function(jsonFileData) {
        this.addFile(jsonFileData.url, jsonFileData.fileId, jsonFileData.filename, jsonFileData.timestamp);
    }
    
    /* 
     * Used for external calls from page using this object.
     */
    this.addFile = function(fileURL, fileId, filename, timestamp) {
    	LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Add File: FileId => " + fileId + "  URL: " + fileURL + "  Filename: " + filename + "  Timestamp: " + timestamp);
        
        var fileWrapper = new Element('div');
        fileWrapper.id = this.fileContainer.identify()+'_'+fileId;
        fileWrapper.addClassName('lcUploaderFile');
        fileWrapper.setAttribute('fileId', fileId);

        // put another div inside to contain the background of the image
        var iconHolder = new Element('div');
        iconHolder.addClassName('lcUploaderIconHolder');
        fileWrapper.insert(iconHolder);
    
        // file icon
        var fileIcon = new Element('img');
        // setup the rest of the properties
        fileIcon.addClassName('lcUploaderFileIcon');
        fileIcon.src = this.fileIconURL;
        iconHolder.insert(fileIcon);
        
        // filename
        var filenameElement = new Element('div');
        filenameElement.innerHTML = filename;
        fileWrapper.insert(filenameElement);
        
        // file create time
        var fileTimestampElement = new Element('div');
        fileTimestampElement.innerHTML = timestamp;
        fileWrapper.insert(fileTimestampElement);
        
        // insert the delete div if we have a delete url
        var divOverlay = new Element('div');
        divOverlay.addClassName('lcUploaderFileDelete');
        divOverlay.innerHTML = "Delete";
        divOverlay.setAttribute('fileId', fileId);
        divOverlay.observe('click', function(event) {
            this.deleteFile(event.element().getAttribute('fileId'));
        }.bind(this));
        fileWrapper.insert(divOverlay);
        
        this._showFileWell();
        
        /*  add the image now */
        this.fileContainer.insert(fileWrapper);
        
    	this.manageButtonVisibility();
    }
    
    // insert a new file
    this.insertFile = function(jsonFileData) {
        this._addFile(jsonFileData);
    }

    // update a file that is already being displayed
    this.updateFile = function(jsonFileData) {
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, ">>> Update File ID: " + jsonFileData.fileId);
        
        // delete the file div for the file we're updating
        var fileDiv = this.fileContainer.select('div[fileId='+jsonFileData.fileId+']').first();
        if(fileDiv == null) {
            LCAjaxLogger.trace("ERROR: Could not find file to update. FileID="+jsonFileData.fileId);
        } else {
            fileDiv.remove();
        }

        // create the new file
        this._addFile(jsonFileData);
    }

    /**
    * Delete an item via AJAX and remove from the list on the UI.
    */
    this.deleteFile = function(fileId) {
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, ">>> Delete File ID: " + fileId);
        
        var requestURL = getAjaxActionURL(this.deleteURL) + "?fileId=" + fileId;
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Delete Image Request URL: " + requestURL);
        
        /* send it to the server */
        new Ajax.Request(requestURL, {
              onSuccess: function(response) {
                  
                   // if it worked we have nothing to do
                    LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Successfully deleted item on server.");
                    
                    var elements = this.fileContainer.select('div[fileId='+fileId+']');
                    elements.each(function(element) {
                        element.remove();
                    });
                    
                    /* if no files are left, show the file well */
                    if(this.numberOfFiles() > 0) {
                        this._showFileWell();
                    }
                    else {
                    	this._hideFileWell();
                    }
                    
                    this.manageButtonVisibility();
                    
                    /*  fire event */
                    this.fireEvent("fileDeleted");
                    
                    /* change the form to a value so it recognizes it changed and the lightbox 'change' listener sees that the form has been modified */
                    this.filenameHidden.setValue("deleted");
                    
                    /* trigger that the form changed */
                    triggerEvent(this.uploadForm, 'change');
                    
              }.bind(this),
              onFailure: function(){
                  this.displayAjaxErrorMessage("Failed to delete file. Please try again."); 
              }.bind(this),
              onException: function(request, exception) { 
                 LCAjaxLogger.trace("Exception while deleting file => " + exception);
                 this.displayAjaxErrorMessage("Failed to delete file. Please try again.");
              }.bind(this)
        });
        
    }
    
    /*  if the well is showing, hide it */    
    this._hideFileWell = function() {
    	this.fileContainer.parentNode.hide();
    }
    
    /*  if the well is hiding, show it */    
    this._showFileWell = function() {
        this.fileContainer.parentNode.show();
    }
    
    // determine if the choose files button should show or not
    this.manageButtonVisibility = function() {
    	if(this.maxNumberFiles > 0) {
            var fileCount = this.numberOfFiles();
            if (fileCount >= this.maxNumberFiles) {
                this.disableUploadButton();
                // hide the add button
                //this.buttonContainer.hide();
            } else {
                // make sure the add button is showing
                // this.buttonContainer.show();
                this.enableUploadButton();
            }
        }
    }
    
    /* how many images are displaying */
    this.numberOfFiles = function() {
        return this.uploadContainerElement.select('div.lcUploaderFile').length;
    }

});

