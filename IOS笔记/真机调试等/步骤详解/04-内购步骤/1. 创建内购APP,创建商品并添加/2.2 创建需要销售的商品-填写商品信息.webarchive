/* 
 * When the delete button is clicked display a confirmation box and if confirmed execute the action.
 * 
 * If the LCDeleteButton container has an <input type=submit> then it will be "clicked" on confirmation. 
 * Otherwise it is up to a client of this to observe the "LCDeleteButton:confirmed" event and then take action.
 *
 * Custom events are fired off that can be observed for hooking into the flow and adding custom behavior.
 *
 * These events are:
 *  - LCDeleteButton:confirmed
 *  - LCDeleteButton:cancelled
 * 
 * An event will return data values that can be used by the event handler:
 *  - deleteButtonId => The ID of the Button
 *
 * These values can be retrieved via the "event.memo.*" property paths, such as "event.memo.deleteButtonId".
 *
 * An example listener is:
 *      document.observe("LCDeleteButton:confirmed", function(event) {
 *          console.log("DeleteButtonId: " + event.memo.deleteButtonId);
 *      }
 */
function LCDeleteButton(deleteButton) {
    /* define an internal variable for 'this' that can't get over-written by other call-paths such as event handlers */
    var self = this;
    
    /*  get elements */
    var confirmDeleteContainer = deleteButton.ancestors().find(function(item) {return item.tagName == 'SPAN' && item.hasClassName('lcDeleteButton'); })
    var confirmDeleteDiv = confirmDeleteContainer.select('div.lcDeleteButton').first();
    var deleteCancelButton = confirmDeleteDiv.select('span.lcDeleteButtonCancel').first();
    var deleteConfirmButton = confirmDeleteDiv.select('span.lcDeleteButtonConfirm').first();
    
    var actionFormSubmitButton = confirmDeleteContainer.select('input.lcDeleteButtonFormSubmit').first();
    
    /*  function to display the confirmation window  */
    this.showConfirmation = function() {
        var originalBoxWidth = confirmDeleteDiv.style.width;
        var originalBoxHeight = confirmDeleteDiv.style.height;
    
        var showOnRight = true;
        // figure out if we have space to show on right, if not we show on left
        var buttonLeftPositionInViewport = deleteButton.viewportOffset().left;
        var viewportWidth = document.viewport.getWidth();
        var buttonWidth = deleteButton.getWidth();
        var popupWidth = $(confirmDeleteDiv).getWidth(); 
        if((buttonLeftPositionInViewport+buttonWidth+popupWidth+10) > viewportWidth) {
            showOnRight=false;
        }
    
        if(showOnRight) {
            // width of button for placement of popup on right side
            var buttonWidth = deleteButton.getWidth()+3;
            var buttonHeight = deleteButton.getHeight();
            var offsetFromButtonTop = (buttonHeight/2)-35;
            // get the position of the Delete button
            confirmDeleteDiv.clonePosition(deleteButton, {offsetTop: offsetFromButtonTop, offsetLeft: buttonWidth});
        } else {
            // show on left side of button
            var leftOffset = ($(confirmDeleteDiv).getWidth()*-1)+5;
            // change the beak to the right side of popup
            var beakDiv = $(confirmDeleteDiv).select('div.tool-tip-beak').first();
            beakDiv.addClassName('tool-tip-beak-right');
            // determine vertical placement
            var buttonHeight = deleteButton.getHeight();
            var offsetFromButtonTop = (buttonHeight/2)-35;
            // get the position of the Delete button
            confirmDeleteDiv.clonePosition(deleteButton, {offsetTop: offsetFromButtonTop, offsetLeft: leftOffset});
        }
        
        
        // need to reset the width since the clone resized to the button
        confirmDeleteDiv.setStyle({width: originalBoxWidth, height: originalBoxHeight});
        
        // set the event observer for if the delete confirmation is selected
        deleteConfirmButton.observe('click', function(event) {
            LCAjaxLogger.log(LCAjaxLogger.DELETE_BUTTON, "Going to execute DELETE after confirmation");
            self.performDelete();
            // cancel all listeners on the confirmation box
            deleteConfirmButton.stopObserving('click');
            deleteCancelButton.stopObserving('click');
            // update the UI
            Effect.toggle(confirmDeleteDiv, 'slide', { duration: 0.2 }); 
            
            // fire the confirmed event
            self.fireEvent("confirmed");
            
            return false;
        });
        
        // set the event observer for if the cancel is selected
        deleteCancelButton.observe('click', function(event) {
            LCAjaxLogger.log(LCAjaxLogger.DELETE_BUTTON, "Cancel DELETE");
            // cancel all listeners on the confirmation box
            deleteConfirmButton.stopObserving('click');
            deleteCancelButton.stopObserving('click');
            // update the UI
            Effect.toggle(confirmDeleteDiv, 'slide', { duration: 0.2 }); 
            
            // fire the cancel event
            self.fireEvent("cancelled");
            
            return false;
        });
        
        //$(confirmDeleteBox).show();
        Effect.toggle(confirmDeleteDiv, 'slide', { duration: 0.2 }); return false;
    }
    
    /*  function to perform the delete action if this is a form action */
    this.performDelete = function() {
        LCAjaxLogger.log(LCAjaxLogger.DELETE_BUTTON, "Performing DELETE action ... ");
        if(actionFormSubmitButton) {
            // this is a form submission action so we'll click it now that we've been confirmed
            $(actionFormSubmitButton).click();
            LCAjaxLogger.trace("should have submitted");
        }
    }
    
    /* This is used to trigger custom events. */
    this.fireEvent = function(eventName) {
        LCAjaxLogger.log(LCAjaxLogger.DELETE_BUTTON, "Firing LCDeleteButton:" + eventName);
        document.fire("LCDeleteButton:" + eventName, { deleteButtonId: $(deleteButton).identify() });
    }
    
    /* show the confirmation box now that we're initialized */
    self.showConfirmation();
    
}
// ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// END LCDeleteButton
// ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////


