var LCImageWiggler = {
    //this submits and does NOT update
    startWiggle: function(image, step) {
        //wiggle function.
        var wiggleDegrees = ['1','0','-1','0'];
        
        if(!image.hasClassName('wiggling')) {
            image.addClassName('wiggling');
        }

        var degree = wiggleDegrees[step % wiggleDegrees.length];
        image.setStyle({
            '-webkit-transform': 'rotate('+degree+'deg)',
            '-moz-transform': 'rotate('+degree+'deg)',
            '-ms-transform': 'rotate('+degree+'deg)',
            '-o-transform': 'rotate('+degree+'deg)',
            '-sand-transform': 'rotate('+degree+'deg)',
            'transform': 'rotate('+degree+'deg)'
        });
                
                
        image.timeout = window.setTimeout(function(){
            LCImageWiggler.startWiggle(image, step+1);
        }, 60);
        
    },
    
    // this submits and does NOT update and then executes the given function
    stopWiggle: function(image) {
        image.setStyle({
                    '-webkit-transform': 'rotate(0deg)',
                    '-moz-transform': 'rotate(0deg)',
                    '-ms-transform': 'rotate(0deg)',
                    '-o-transform': 'rotate(0deg)',
                    '-sand-transform': 'rotate(0deg)',
                    'transform': 'rotate(0deg)'
        });

        if(image.hasClassName('wiggling')) {
            image.removeClassName('wiggling');
        }

        window.clearTimeout(image.timeout);
    }
}

