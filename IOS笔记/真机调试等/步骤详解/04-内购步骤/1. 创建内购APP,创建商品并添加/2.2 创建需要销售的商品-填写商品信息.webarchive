/* Dynamic Toggle Tool Tip (no index value needed) */
function showToolTip(node) {
  var link = $(node);
  var tips = $$('.div-element');
  if (link.down('div').style.display == 'none') {
    for (i = 0; i < tips.length; i++) {
      tips[i].style.display = "none";
    }
  }
  link.down('div').toggle();
}

