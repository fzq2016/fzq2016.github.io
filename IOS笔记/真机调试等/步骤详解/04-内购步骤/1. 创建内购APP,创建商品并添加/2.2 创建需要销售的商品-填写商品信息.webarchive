	        }
	        else if (statusType == 'onSubsStore') {
	             msg = "On Apple Music";
             needLink = false;
             color = "green";
	        }
	        else if (statusType == 'notOnSubsStore') {
	            msg = 'Not on Apple Music';
            color = "red";       
	        }
	        else if (statusType == 'readyForSubsStore') {
            msg = 'Ready for Apple Music';
            color = "orange";
            needLink = false;	        
	        }
	        else {
	           //should not get here
	           msg = 'Not on Store';
	           color = "red";
	           needLink = false;
	        }
	         
	        divLink = new Element("div");
	        divLink.setAttribute("class", "see-details");
	        var iconElt = new Element("img");
	        iconElt.setAttribute("src", "/itc/images/status-"+color+".png");
	        iconElt.setAttribute("class", "status-icon");
	        divLink.appendChild(iconElt);
	        if (needLink && this.hasNoProducts()) {
	            var linkElt = new Element("a");
