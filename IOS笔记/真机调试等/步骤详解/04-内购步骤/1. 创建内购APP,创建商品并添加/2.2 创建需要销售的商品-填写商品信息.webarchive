var loadLocalizationData = function () {
    
    if (!localizationData) {
      $j.ajax({
          url: '/WebObjects/iTunesConnect.woa/ra/l10n', 
          success: function(data) {
            localizationData = data.data;
          },
          async: false
      });   
    }
}

// The interpolate function allows us to replace arbitrary variables with their appropriate value
var interpolateFunc = function( localizationKey, replacements ) {
    if (!localizationData) loadLocalizationData();

    var text = localizationData[ localizationKey ] || localizationKey,
      r = replacements;
      
    if (typeof r === 'object' && Object.keys(r).length > 0) {
      for (var key in r) {
        if (r.hasOwnProperty(key)) {
            r['@@'+key+'@@'] = r[key];
            delete r[key];
          }
      }
      var rgx = new RegExp( Object.keys(r).join('|'), 'gi');
      return text.replace(rgx, function(match) { return r[match] });
    } else {
      return text;
    }
};


