// This is the core class that help us manipulate the UI for the content status that appear at the bottom of the page. Usually you will be ending up doing the following calls:
// var contentStatusBar = new LCContentStatusBar('content-status-container');
// ...
// var oneProblem = new LCContentStatusItem(...);
// contentStatusBar.showContentStatusFor(oneProblem,shouldNotifyServer);
// 
// When contentStatusBar.showContentStatusFor() is executed, the code will take care of showing the content status bar at the bottom of the page.
var LCContentStatusBar = Class.create( {
    initialize : function(mainContainerName) {
        this.mainContainer = mainContainerName;
        this.mainContainerElement = $(this.mainContainer);
        this.contentStatusElement = $("content-status");
        this.leftTitleElement = this.mainContainerElement.select("div.content-status-left-title").first();
        this.centerTitleElement = this.mainContainerElement.select("div.content-status-center-title").first();
        this.countryFlagElement = this.mainContainerElement.select("img.flag").first();
        this.problemAreaElement = $("problem-area");
        this.problemListElement = $("content-status-problem-list");
        var rowTemplate = $("content-status-row-template");
        this.rowTemplateElementDiv = rowTemplate.select("div.left").first();
        this.currentStatusItem = null;
        // Visibility status
        this.statusBarVisibility = false;
        this.headerBarVisibility = true;
        // setup the function
        var self = this;
        $('content-status-header').observe("click", function( e) { self.toggleHeaderArea(e) }); 
        $('close-button').observe("click", function(e) {
            self.toggleStatusBar(e)
            $$('.footerCopyright')[0].style.marginBottom = "0px";
            scrollBarDetect(8);
        } );
    },
    
    setCallbackUrl: function(url,notifyServer) {
        this.callbackUrl = url;
        this.shouldNotifyServer = notifyServer;
    },
    notifyServerNewStatus: function() {
        if (!this.shouldNotifyServer || this.currentStatusItem == null || !this.callbackUrl) { return; }
        // check the status of the content bar : if user closed it, then we should notify server that 
        // there is nothing to track anymore
        var dataToPost = { 'visible': this.statusBarVisibility, 
                            'drawerExpanded': this.headerBarVisibility,
                            'title': this.currentStatusItem.getTitle(),
                            'countryCode': this.currentStatusItem.getCountryCode(), 
                            'productId': this.currentStatusItem.getProductId(), 
                            'statusType': this.currentStatusItem.getStatusType(),
                            'pType': this.currentStatusItem.getPType(),
                            'sType': this.currentStatusItem.getSType(),
                            'statusCountry': this.currentStatusItem.getStatusCountry()};
        var xhr = new XMLHttpRequest();
        xhr.open('POST', this.callbackUrl, true);
        xhr.onreadystatechange = function() {
           if (this.readyState != 4) return;
    
           var httpStatus = 0;
           try {
               httpStatus = this.status || 0;  // FYI: status is zero for http 500s
           } catch (e) {}
    
           var rawData = this.responseText;
           var resData = null;
           try {
               resData = its.evalJson(rawData); 
           } catch (e) {
               LCAjaxLogger.log(LCAjaxLogger.CONTENT_STATUS, 'invalid server response, json eval failed: "' + rawData + '", status: ' + httpStatus);
           }
    
           var success = httpStatus >= 200 && httpStatus <= 299;
           if (!success) {
            // not sure what to do yet.
           }
        }
        // Post the data
        var jsonPayload = Object.toJSON(dataToPost);
        xhr.send(jsonPayload);             
    },
    toggleStatusBar : function(e) {
        e.stopPropagation();
        if (this.statusBarVisibility === false) {
            toggle_drawer_margin(true);
            this.headerBarVisibility = true;
            this.problemAreaElement.show();
            this.statusBarVisibility = true;
            new Effect.BlindDown(this.contentStatusElement, {duration: 0.5}); // Check that it's working
        } else {
            toggle_drawer_margin(false);
            this.statusBarVisibility = false;
            new Effect.BlindUp(this.contentStatusElement, {duration: 0.5});
            this.notifyServerNewStatus();
        }
    },

    toggleHeaderArea : function(e) {
        e.stopPropagation();
        if (this.headerBarVisibility === false) {
            toggle_drawer_margin(true);
            this.headerBarVisibility = true;
            new Effect.BlindDown(this.problemAreaElement, {duration: 0.5});
        } else {
            toggle_drawer_margin(true,true);
            this.headerBarVisibility = false;
            new Effect.BlindUp(this.problemAreaElement, {duration: 0.5});
        } 
        this.notifyServerNewStatus();
    },
    
    showContentStatusFor : function(statusItem,shouldNotifyServer,drawerExpanded) {
        this.currentStatusItem = statusItem;
        this.renderStatus(statusItem);
        this.renderProblems(statusItem.getProblems());
        if (this.statusBarVisibility === false) {
            toggle_drawer_margin(true);
            if (drawerExpanded) {
                this.headerBarVisibility = true;
                this.problemAreaElement.show();
            } else {
                this.headerBarVisibility = false;
                this.problemAreaElement.hide();
            }
            this.statusBarVisibility = true;
            new Effect.BlindDown(this.contentStatusElement, {duration: 0.5}); // Check that it's working
        }
        if (shouldNotifyServer) {
            this.notifyServerNewStatus();
        }
    },

    renderStatus : function(statusItem) {
        this.leftTitleElement.innerHTML = statusItem.getReason();
        this.centerTitleElement.innerHTML = statusItem.getTitle();
        var flagValue = statusItem.getFlag();
        this.countryFlagElement.src = flagValue.indexOf("/") == 0 ? flagValue : "/itc/images/" + flagValue;
        var nextSibling = this.countryFlagElement.nextSibling;
        if (nextSibling && nextSibling.textContent != " ") {
            this.countryFlagElement.parentNode.removeChild(nextSibling);
        }
        $('reason-territory').innerHTML = statusItem.getCountry()
        this.countryFlagElement.parentNode.insertBefore(this.countryFlagElement,null);
    },
    
    renderProblems : function(problems) {
        // We will be replacing the existing content with the new content
        if (this.problemListElement.hasChildNodes()) {
            while (this.problemListElement.children.length > 0) {
                this.problemListElement.removeChild(this.problemListElement.childNodes[0])
            }
        }
        // Now prepare all the child
        var count = problems.length;
        for(i=0;i<count;i++) {
            newRowElement = this.createNewRowElementWithProblem(problems[i]);
            this.problemListElement.appendChild(newRowElement);
        }
    },
    
    createNewRowElementWithProblem : function(problem) {
        // problem has the following structure: { alertIcon: 'url for icon', description: 'some html description', action: 'some html action'}
        var newRowElement = this.rowTemplateElementDiv.cloneNode(true); // Deep clone
        var alertIconElement = newRowElement.select('img.alert-icon').first();
        var descriptionElement = document.createTextNode(problem.getDescription());
        var actionElement = newRowElement.select('div').first().select('span').first();
        var templateDescriptionElement = newRowElement.select('div.content-status-template-description').first();
        var alertIconValue = problem.getAlertIcon();
        alertIconElement.src = alertIconValue.indexOf("/") == 0 ? alertIconValue : "/images/itc/" + alertIconValue;
        if (shouldShowContentSatusInternalName()) { alertIconElement.setAttribute("title", problem.getInternalName()); }
        templateDescriptionElement.parentNode.replaceChild(descriptionElement, templateDescriptionElement);
        actionElement.innerHTML = " " + problem.getAction();
        return newRowElement;
    }
});

