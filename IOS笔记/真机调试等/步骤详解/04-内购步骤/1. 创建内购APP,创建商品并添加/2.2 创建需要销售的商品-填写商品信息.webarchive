    },
    /**
     * OVERRIDE: We don't want to call the standard LCUploader success handling.
     */
    handleUploadSuccessXHR : function() {
        // do nothing, we want to override the parent so it won't call the iTC LCUploader action
    },
    /* OVERRIDE: check if file size is too large
     */
    isValidFileForUpload: function() {
        // Return true if the file is less than maxFileSizeBytes
        if (this.fileInput.files[0].size > this.maxFileSizeBytes) {
