// BEGIN LCAjaxContainer
// ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////


var LCAjaxContainer = {
    // return the first LCAjaxContainer found wrapping the given element 
    getWrappingContainerForElement: function(element) {
        return $(element).ancestors().find(function(item) {return item.tagName == 'SPAN' && item.hasClassName('lcAjaxContainer'); })
    },
    //this submits and does NOT update
    submit: function(container) {
        LCAjaxLogger.log(LCAjaxLogger.UPDATE_CONTAINER, "LCAjaxContainer.update updating container: " + container);
        var actionURL = $(container).getAttribute('action');
        LCAjaxLogger.log(LCAjaxLogger.UPDATE_CONTAINER, "LCAjaxContainer.update url: " + actionURL);
        
        new Ajax.Request(actionURL, {
            method: 'post',
             parameters: serializedFormValuesForContainer(container) + "&viaLCAjaxContainer=true",
             onFailure: function() {
                LCAjaxContainer.handleError(response, container, "Failed to submit container.");
             }
        });
    },
    // this submits and does NOT update and then executes the given function
    submitWithFollowup: function(container, executeAfter) {
        LCAjaxLogger.log(LCAjaxLogger.UPDATE_CONTAINER, "LCAjaxContainer.submitWithFollowup submitting container: " + container);
        var actionURL = $(container).getAttribute('action');
        LCAjaxLogger.log(LCAjaxLogger.UPDATE_CONTAINER, "LCAjaxContainer.submitWithFollowup url: " + actionURL);
        
        new Ajax.Request(actionURL, {
            method: 'post',
             parameters: serializedFormValuesForContainer(container) + "&viaLCAjaxContainer=true",
             onSuccess: function(response) {
                 if(response.responseText) {
                   executeAfter.call(this, true, response.responseText);
                 } else {
                   executeAfter.call(this, true);
                 }
             },
             onFailure: function(response) {
                LCAjaxContainer.handleError(response, container, "Failed to submit container.");
                if(response.responseText) {
                    executeAfter.call(this, false, response.responseText);
                } else {
                    executeAfter.call(this, false);
                } 
             }
        });
    },
    // this updates
    update: function(container) {
        LCAjaxLogger.log(LCAjaxLogger.UPDATE_CONTAINER, "LCAjaxContainer.update updating container: " + container);
        var actionURL = $(container).getAttribute('action');
        LCAjaxLogger.log(LCAjaxLogger.UPDATE_CONTAINER, "LCAjaxContainer.update url: " + actionURL);
        
        new Ajax.Request(actionURL, {
            method: 'get',
             onSuccess: function(response) {
                 if(response.responseText) {
                     $(container).innerHTML=response.responseText;
                     $(container).innerHTML.evalScripts();
                 }
             },
             onFailure: function() {
                LCAjaxContainer.handleError(response, container, "Failed to update container.");
             }
        });
    },
    // this updates and then executes the given function
    updateWithFollowup: function(container, executeAfter) {
        LCAjaxLogger.log(LCAjaxLogger.UPDATE_CONTAINER, "LCAjaxContainer.updateWithFollowup updating container: " + container);
        var actionURL = $(container).getAttribute('action');
        LCAjaxLogger.log(LCAjaxLogger.UPDATE_CONTAINER, "LCAjaxContainer.updateWithFollowup url: " + actionURL);
        
        new Ajax.Request(actionURL, {
            method: 'get',
             onSuccess: function(response) {
                 if(response.responseText) {
                     $(container).innerHTML=response.responseText;
                     $(container).innerHTML.evalScripts();
                 }
                 executeAfter.call(this, true);
             },
             onFailure: function(response) {
                LCAjaxContainer.handleError(response, container, "Failed to update container.");
                if(response.responseText) {
                    executeAfter.call(this, false, response.responseText);
                } else {
                    executeAfter.call(this, false);
                } 
             }
        });
    },
    /*
     * This submits and updates
     * 
     * Parameters is a Map of values to submit as parameters with the URL.
     * 
     * Example: {methodName:'submit', item:'123'} will be sent as methodName=submit&item=123 
     */
    submitAndUpdate: function(container, parameters) {
    	LCAjaxContainer.submitAndUpdateWithFollowup(container, null, parameters);
    },
    /*
     * This submits, updates and then executes the given function
     * 
     * Parameters is a Map of values to submit as parameters with the URL.
     * 
     * Example: {methodName:'submit', item:'123'} will be sent as methodName=submit&item=123 
     */
    submitAndUpdateWithFollowup: function(container, executeAfter, parameters) {
        LCAjaxLogger.log(LCAjaxLogger.UPDATE_CONTAINER, "LCAjaxContainer.submitAndUpdate submitting container: " + container);
        var actionURL = $(container).getAttribute('action');
        LCAjaxLogger.log(LCAjaxLogger.UPDATE_CONTAINER, "LCAjaxContainer.submitAndUpdate url: " + actionURL);
        
        // if we have parameters let's convert them to a string
        var extraParameters = '';
        if(parameters) {
        	extraParameters = '&' + $H(parameters).toQueryString();
        	LCAjaxLogger.log(LCAjaxLogger.UPDATE_CONTAINER, "LCAjaxContainer.submitAndUpdate Extra Parameters: " + extraParameters);
        }
        
        new Ajax.Request(actionURL, {
            method: 'post',
             parameters: serializedFormValuesForContainer(container) + "&viaLCAjaxContainer=true" + extraParameters,
             onSuccess: function(response) {
                 if(response.responseText) {
                     $(container).innerHTML=response.responseText;
                     $(container).innerHTML.evalScripts();
                 }
                 if(executeAfter) {
                	 executeAfter.call(this, true);
                 }
             },
             onFailure: function(response) {
                LCAjaxContainer.handleError(response, container, "Failed to submit and update container.");
                if(response.responseText && executeAfter) {
                    executeAfter.call(this, false, response.responseText);
                } else {
                    executeAfter.call(this, false);
                } 
             }
        });
    },
    /*
     * Determine what we should do with the error response.
     */
    handleError: function(response, lcAjaxContainer, message) {
    	if(response.responseText.isJSON()) {
            var jsonResponse = response.responseText.evalJSON();
            if(jsonResponse != null && jsonResponse.validationExceptions != null) {
            	return LCAjaxContainer.displayValidationExceptions(jsonResponse, lcAjaxContainer);
            }
        }
    	
    	// if the above didn't execute as 'displayValidationExceptions' and return we will displayError instead
    	LCAjaxContainer.displayError(lcAjaxContainer, message);
        
    },
    // display an error for the given LCAjaxContainer and with the given message.
    displayError: function(lcAjaxContainer, message) {
        var errorElementID = $(lcAjaxContainer).getAttribute('errorId');
        displayLCAjaxErrorMessage(errorElementID, message);
    },
    /*
     * If the error response had validation exceptions we will either:
     * 
     * a) Show them at the top of an LCAjaxLightbox
     * b) Display them as an error message for the container if we're not in a lightbox
     */
    displayValidationExceptions : function(jsonResponse, lcAjaxContainer) {

    	var currentLightbox = currentlyVisibleLightbox();
    	if(currentLightbox) {
    		var lightboxInstance = LCAjaxLightboxUtil.instanceOfLightboxForDivId(currentLightbox.id);
    	
            /*  we have validation exceptions */
            // Exceptions Array:        jsonResponse.validationExceptions
            // Number of Exceptions:    validationExceptions.length
            //    Message:              validationExceptions[i].message
            //    FieldId:              validationExceptions[i].fieldId  (this can be null)
            //    FieldValue:           validationExceptions[i].fieldValue  (this can be null)

            LCAjaxLogger.log(LCAjaxLogger.UPDATE_CONTAINER, "Received Validation Exceptions. Number: " + jsonResponse.validationExceptions.length);
            
            lightboxInstance._displayErrorMessages(jsonResponse);
    	} else {
    		// we're not in a lightbox
    		LCAjaxContainer.displayError(lcAjaxContainer, message);
    	}
    }
}

