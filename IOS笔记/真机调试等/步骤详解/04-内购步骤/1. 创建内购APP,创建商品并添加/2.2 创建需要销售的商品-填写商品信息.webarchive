/************************************************************************************************************************/
/************************************************ LCTextfieldListener.js ************************************************/
/************************************************************************************************************************/

/*
 * 
 * Arguments: 
 * 
 * 'textfield' => [REQUIRED] What textfield element to monitor. 
 * 'functionToExecuteOnEvent' => [REQUIRED] What function to execute when an event occurs. 
 * 'delay' => [OPTIONAL, default: 250] The time in milliseconds to delay action execution after event occurs and during which to accumulate events.
 * 'alwaysDelay' [OPTIONAL, default: true] If true an event will always delay before executing. If false, the delay will only occur if the event is within DELAY time from last event execution.
 * 
 * @param args
 *            Example => {'textfield':search_field, 'delay' : 250, 'functionToExecuteOnEvent' : function(textfield, query) { function definition here }}
 * @returns {LCTextfieldListener}
 */
function LCTextfieldListener(args) {
    /* define an internal variable for 'this' that can't get over-written by other call-paths such as event handlers */
    var self = this;

    var current_textfield_value;
    var textfield = args.textfield;
    var functionToExecuteOnEvent = args.functionToExecuteOnEvent;
    var queueDelay = args.delay;
    var alwaysDelay = args.alwaysDelay;

    if (textfield === undefined || functionToExecuteOnEvent === undefined) {
        throw "LCTextfieldListener 'args' must contains values for 'textfield' and 'functionToExecuteOnEvent'.";
    }

    if (queueDelay === undefined) {
        queueDelay = 250;
    }

    if (alwaysDelay === undefined) {
        alwaysDelay = true;
    }

    /**
     * Start observing a textfield and provide the function to call when an event occurs
     */
    this.startObserving = function() {
        /* we want to turn off the browser feature for autocomplete */
        textfield.setAttribute("autocomplete", "off");

        /* observe the field for 'keyup' and trigger 'queue_event' when typing occurs */
        Element.observe(textfield, 'keyup', function(e) {
            // typing into search field
            if (e.keyCode === Event.KEY_DOWN || e.keyCode === Event.Key_UP) {
                return false;
            }
            var textfieldValue = textfield.value;
            if (current_textfield_value !== textfieldValue) {
                current_textfield_value = textfieldValue;
                self.queueEvent(textfield, textfieldValue);
            }
        });
    }(); // execute this once defined so we start observing when the class is instantiated

    /**
     * Whenever an event is triggered this will queue the event.
     * <p>
     * >>> If alwaysDelay == false:
     * <p>
     * If an event has never been fired, or it has been longer than 'delay' since firing, then it will be executed immediately.
     * <p>
     * If an event has occurred within the 'delay' time then it will be queued to execute after the delay.
     * <p>
     * If any new events occur and are queued while another is already queued they will replace the one in the queue.
     * <p>
     * >>> If alwaysDelay == true:
     * <p>
     * If an event fires and nothing is queued, it will be queued up.
     * <p>
     * If an event fires and something is already queued, it will replace what is in the queue.
     * 
     */
    this.queueEvent = function(textfield, textfieldValue) {
        if (!alwaysDelay) {
            // alwaysDelay=false;
            if (self.last_event_action_time === undefined) {
                // never been fired so do so immediately
                lcs_logger.log(lcs_logger.TEXTFIELD_LISTENER, '>>> executing event for the first time   Event: ' + textfieldValue);
                self.performEventAction(textfield, textfieldValue);
            } else {
                // how long since the last completed event
                var time_diff = (new Date()).getTime() - self.last_event_action_time;
                // how long until we want to execute again => the diff from how long we want to wait and the time since last completed event
                var time_to_wait = queueDelay - time_diff;

                // if we are past the delayPeriod and nothing is scheduled, execute immediately
                if (time_diff > queueDelay && self.queue_timer === undefined) {
                    // let's execute immediately
                    lcs_logger.log(lcs_logger.TEXTFIELD_LISTENER, ">>> executing event immediately (time diff past timeout)   Event: " + textfieldValue);
                    self.performEventAction(textfield, textfieldValue);
                } else {
                    // we are within the delay so we want to schedule the event

                    // if we have a scheduled event, cancel it and we'll re-schedule a new one
                    if (self._isEventQueued()) {
                        // we want to replace the currently scheduled event with this new one
                        lcs_logger.log(lcs_logger.TEXTFIELD_LISTENER, "Cancelling previously scheduled event and will replace with new one.   Event: " + textfieldValue);
                        // cancel the existing scheduled event
                        self._cancelQueue();
                    }

                    lcs_logger.log(lcs_logger.TEXTFIELD_LISTENER, "scheduling event to occur in " + time_to_wait + "ms   Event: " + textfieldValue);
                    // create a timer that will wait 'time_to_wait' before executing
                    // if no other events replace this before that time is up then the event will fire with the current textfield/textfieldValue
                    self._queue(textfield, textfieldValue, time_to_wait);
                }
            }
        } else {
            // alwaysDelay=true

            if (self.last_event_action_time === undefined) {
                // in this path we want to delay first execution so we'll set this action_time to 'now' the first time through
                self.last_event_action_time = (new Date()).getTime();
            }
            
            // how long since the last completed event
            var time_diff = (new Date()).getTime() - self.last_event_action_time;
            if (time_diff > queueDelay) {
                time_diff = 0;
                // we act as if we just executed an event and queue up the events
                self.last_event_action_time = (new Date()).getTime();
            }
            // how long until we want to execute again => the diff from how long we want to wait and the time since last completed event
            var time_to_wait = queueDelay - time_diff;
            

            // if we have a scheduled event, cancel it and we'll re-schedule a new one
            if (self._isEventQueued()) {
                // we want to replace the currently scheduled event with this new one
                lcs_logger.log(lcs_logger.TEXTFIELD_LISTENER, "Cancelling previously scheduled event and will replace with new one.   Event: " + textfieldValue);
                // cancel the existing scheduled event
                self._cancelQueue();
            }

            lcs_logger.log(lcs_logger.TEXTFIELD_LISTENER, "scheduling event to occur in " + time_to_wait + "ms   Event: " + textfieldValue);
            // create a timer that will wait 'time_to_wait' before executing
            // if no other events replace this before that time is up then the event will fire with the current textfield/textfieldValue
            self._queue(textfield, textfieldValue, time_to_wait);
        }
    }


    this._queue = function(textfield, textfieldValue, time_to_wait) {
        self.queue_timer = window.setTimeout(function() {
            lcs_logger.log(lcs_logger.TEXTFIELD_LISTENER, ">>> executing event after timeout   Event: " + textfieldValue);
            self.performEventAction(textfield, textfieldValue);
            // null out the timer so other events can be scheduled
            self.queue_timer = undefined;
        }, time_to_wait);
    }

    this._cancelQueue = function() {
        clearTimeout(self.queue_timer);
    }

    this._isEventQueued = function() {
        return self.queue_timer !== undefined;
    }


    /**
     * This will execute the callback function provided by the client of this class
     */
    this.performEventAction = function(textfield, textfieldValue) {
        lcs_logger.log(lcs_logger.TEXTFIELD_LISTENER, "****** Executing Event: " + textfieldValue);
        self.last_event_action_time = (new Date()).getTime();
        functionToExecuteOnEvent.call(this, textfield, textfieldValue);
    }

}

