            $j('.ajaxListColumn_1').each(function () {
                $j('.ajaxListColumn_1').hide();
                $j('.ajaxListColumn_2').addClass('default-to-first-column');
            })
    	}
    }, 
    
    handleDurationChange : function() {
    	LCAjaxContainer.submitAndUpdate('recurringUpdateContainer');
    }, 
    
    handleMarketingIncentiveSelection : function(radioButton) {
    	var selectionTrue=false;
    	if($j(radioButton).attr('classname') == 'radioTrue' && $j(radioButton).attr('checked')) {
    		selectionTrue = true;
    	}
    	
    	if(selectionTrue) {
    		$j('div.paperboyOptinChoices').show();
    	} else {
    		$j('div.paperboyOptinChoices').hide();
    		$j('div.paperboyOptinChoices select').get(0).selectedIndex = 0;
    	}
    },
    
    handleFreeTrialSelection : function(radioButton) {
        var selectionTrue=false;
        if($j(radioButton).attr('classname') == 'radioTrue' && $j(radioButton).attr('checked')) {
            selectionTrue = true;
        }
        
        if(selectionTrue) {
            $j('div.freeTrialOptinChoices').show();
        } else {
            $j('div.freeTrialOptinChoices').hide();
            $j('div.freeTrialOptinChoices select').get(0).selectedIndex = 0;
        }
    },
    
    addToPricingSchedule : function() {
    	var lightboxInstance = LCAjaxLightboxUtil.instanceOfLightboxForDivId(currentlyVisibleLightbox().id);
    	if(lightboxInstance._isDisplayingMessage()) {
    		lightboxInstance._hideDisplayMessage(function() {
    			LCAjaxContainer.submitAndUpdate('pricingUpdateContainer', {action:'addToPricingSchedule'});
        	});
    	} else {
    		LCAjaxContainer.submitAndUpdate('pricingUpdateContainer', {action:'addToPricingSchedule'});
    	}
    	
    },
    /* enable/disable the addToPricingScheduleButton according to selections */
    manageAddToPricingScheduleButton : function() {
    	var enableButton = true;
    	if($j('#price_tier_popup').val() == 'WONoSelectionString') {
    		enableButton = false;
    	}
    	
    	$j('div.priceTierEffectiveDate select').each(function(index, element) {
    		if($j(element).val() == 'WONoSelectionString') {
        		enableButton = false;
        	}
    	});
    	
    	$j('div.priceTierEndDate select').each(function(index, element) {
    		if($j(element).val() == 'WONoSelectionString') {
        		enableButton = false;
        	}
    	});
    	
    	if(enableButton) {
    		$j('#addToPricingScheduleButtonEnabled').show();
    		$j('#addToPricingScheduleButtonDisabled').hide();
    	} else {
    		$j('#addToPricingScheduleButtonEnabled').hide();
    		$j('#addToPricingScheduleButtonDisabled').show();
    	}
    },
    /* submit a duration for review */
    submitDurationForReview : function(button) {
    	showLCPopup('submitDurationConfirmationMessageId');
    	var itemId = LCAjaxListUtil.getItemIdFromRow(button);
    	var hrefButton = $j('#submitDurationConfirmationMessageId a');
    	var href = hrefButton.attr('href');
    	var end = href.indexOf('?');
    	if(end > -1) {
    		href = href.substring(0, end);
    	}
    	hrefButton.attr('href', href + '?itemID=' + itemId);
    }
}


