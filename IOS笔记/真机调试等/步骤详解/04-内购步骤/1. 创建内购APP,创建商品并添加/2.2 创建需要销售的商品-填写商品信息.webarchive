    /**
    * Updates an item row, or adds the row to the list if that itemID is not currently represented.
    */
    this.updateRow = function(itemID, executeAfterUpdate) {
        var ownsSpinner = self.showSpinner();
        
        /* by default the URL does not have an itemID and will just get the most recent row, such as a newly added one */
        var requestURL = getAjaxActionURL(viewRowUrl); 
        if(itemID != null) {
            // this is looking up a specific row so let's add that itemID to the URL
            requestURL = requestURL+"?itemID=" + itemID;
        }
        
        /* the row on the UI ... if itemID is null, this will have 'null' in it and not be found when we look for it below, which is what we want */
        var rowElementID = self.rowElementIdForItemId(itemID);
        
        LCAjaxLogger.trace("row: " + rowElementID);
        
        
        LCAjaxLogger.log(LCAjaxLogger.LIST, "Requesting row at: " + requestURL);
        /* send it to the server */
        new Ajax.Request(requestURL, {
              onSuccess: function(response) {
                   // if it worked we have nothing to do
                    //LCAjaxLogger.log(LCAjaxLogger.LIST, "Received response => " + response.responseText);
                    if(!$(rowElementID)) {
                        // we don't have a row element, so let's get the table instead and add an entire new row
                        
                        LCAjaxLogger.trace("Can't find row for: " + itemID);
                        
                        var newRow = new Element('li');
    
                        /* set the ID to the correct value */
                        newRow.id=rowElementID;
                        /* set the class */
                        newRow.className = 'ajaxListLI';
                        
                        /* set to the response HTML */
                        newRow.innerHTML=innerHTML=response.responseText;
                        
                        LCAjaxLogger.log(LCAjaxLogger.LIST, "Adding new row with id: " + newRow.id);
                        
                        $(ajaxListId).insert({
                            bottom: newRow
                        });
                        
                    } else {
                        // we have the element so update the row
                        LCAjaxLogger.log(LCAjaxLogger.LIST, "Updating row with response: " + response.responseText);
                        $(rowElementID).innerHTML=response.responseText;
                    }
                    
                    // after a row is added or updated
                    self.makeListReorderable();
                    /* update the index numbers for display */
                    self.updateAjaxListSortOrderColumn();
                    /* trigger change event */
                    self.onAjaxListChange();
                    
                    /* we don't check for ownership here as we are definitely done doing all work at this point */
                    self.hideSpinner();
                    
                    if(executeAfterUpdate) {
                        executeAfterUpdate.call();
                    }
              },
              onFailure: function(){
                 self.displayAjaxListErrorMessage("Failed to retrieve row. Please try again."); 
              },
              onException: function(request, exception) { 
                LCAjaxLogger.log(LCAjaxLogger.LIST, "Exception while retrieving row => " + exception);
                self.displayAjaxListErrorMessage("Failed to retrieve row. Please try again."); 
              }
        });
    }
    
    this.getItemActionURL = function(itemID) {
        return getAjaxActionURL(ActionType.ITEM.url)+"?itemID=" + itemID;
    }
    
    this.getCustomActionURL = function(itemID, columnID) {
        return getAjaxActionURL(ActionType.CUSTOM.url)+"?itemID=" + itemID + "&columnID="+columnID;
    } 

    this.getActionURL = function(itemID, elementClicked, actionType) {
        var actionURL = null;
        var columnID = self.getColumnIdFromElement(elementClicked);
        
        if (actionType == ActionType.ITEM) {
            actionURL = self.getItemActionURL(itemID);
        }
        else if (actionType == ActionType.CUSTOM) {
            actionURL = self.getCustomActionURL(itemID, columnID);
        }
        
        return actionURL;
    }

    this.fireActionEvent = function(columnID) {
        LCAjaxLogger.log(LCAjaxLogger.LIST, "Firing LCAjaxList:action");
        $(listDiv).fire("LCAjaxList:action", { ajaxListId: ajaxListId, columnID: columnID });
    }
    
    this.performAction = function(itemID, elementClicked, actionType) {
        var columnID = self.getColumnIdFromElement(elementClicked);
        var refreshOnAction = self.isRefreshOnAction(elementClicked);
        var actionURL = self.getActionURL(itemID, elementClicked, actionType);
        
        var confirmationDivId = self.getConfirmationDivId(elementClicked);
        if ($j('#' + confirmationDivId).length > 0) {
            self.performActionWithConfirmation(elementClicked, itemID, 
                function() {
                    LCAjaxLogger.log(LCAjaxLogger.LIST, "Perform Action: itemID => " + itemID + " columnID => " + columnID + " refreshOnAction => " + (refreshOnAction ? "true" : "false") + " actionType => " + actionType);
                    self.executeActionURL(actionURL, refreshOnAction, columnID);
                });
        }
        else {
            LCAjaxLogger.log(LCAjaxLogger.LIST, "Perform Action: itemID => " + itemID + " columnID => " + columnID + " refreshOnAction => " + (refreshOnAction ? "true" : "false") + " actionType => " + actionType);
            self.executeActionURL(actionURL, refreshOnAction, columnID);
        }
    }
    
    /**
     * Returns the div id of the confirmation popup corresponding to the
     * given clicked element (usually the button that was clicked).   
     */
    this.getConfirmationDivId = function(elementClicked) {
        var columnID = self.getColumnIdFromElement(elementClicked);
        return ajaxListId + '_confirm_' + columnID;
    }
    
    /**
     * Returns true if the given action element that was clicked is designated to
     * refresh the list after the action has been executed.  Otherwise, returns false.
     */
     this.isRefreshOnAction = function(elementClicked) {
        return elementClicked.hasClassName('ajaxListRowDivRefreshOnAction');
     } 
    
    
    /**
     * Performs an action by either going to the page given by the url or refreshing
     * the lightbox after an ajax call to the requested url.
     */
    this.executeActionURL = function(requestURL, refreshOnAction, columnID) {
       if ( ! refreshOnAction ) {
            window.location = requestURL;
        }
        else {
            self.performActionWithRefresh(requestURL, columnID);
        }
    }
    
    /* 
     * Execute the action associated with the given request URL and refresh the list on success
     */
    this.performActionWithRefresh = function(requestURL, columnID) {
       new Ajax.Request(requestURL, {
           onSuccess: function(response) {
                 self.refreshData(function() {
                     self.onAjaxListChange();    // trigger change event
                     self.hideSpinner();
                     self.fireActionEvent(columnID);
                 });
           },
           onFailure: function(response){
              self.displayAjaxListErrorMessage("Failed to perform operation. Please try again.");
           },
           onException: function(request, exception) {
              LCAjaxLogger.log(LCAjaxLogger.LIST, "Exception while performing sort on server => " + exception);
              self.displayAjaxListErrorMessage("Failed to perform operation. Please try again.");
           }
       });
    }

    /* 
     * Sort the data using the given column (columnID) in the given direction (asc, desc)
     */
    this.sortColumn = function(direction, columnID) {
        // remove the sort up/down classnames from all sort buttons
        listDiv.select("div.ajaxListSort").each(function(button) {
            button.removeClassName("ajaxListSortUp"); 
            button.removeClassName("ajaxListSortDown"); 
        });
        
        // remove 'sorted' from all headers
        listDiv.select("div.ajaxListColumnSortable").each(function(button) {
        	button.removeClassName("ajaxListColumnSorted");
        });
        
        // set the className as sorted on this column
        var headerDiv = listDiv.select('div.ajaxListHeaderDiv').last();
        var columnHeader = headerDiv.select('div.ajaxListColumn_' + columnID).last();
        // set 'sorted' on the header
        columnHeader.addClassName("ajaxListColumnSorted");
        // set the direction on the action div
        if("desc" == direction) {
            columnHeader.select("div.ajaxListSort").last().addClassName("ajaxListSortDown");
        } else {
            columnHeader.select("div.ajaxListSort").last().addClassName("ajaxListSortUp");
        }
        
        if (useServerSideOnly || pageSize > -1) {
            self.sortColumnOnServer(direction, columnID);
        } else {
            // no pagination so sort locally
            self.sortColumnLocally(direction, columnID);
        }
    }
    
    /**
     * If we have only a single page we can sort here in the client
     */
    this.sortColumnLocally = function(direction, columnID) {
        LCAjaxLogger.log(LCAjaxLogger.LIST, "Sorting list locally: direction => " + direction + " columnID => " + columnID);
        
        var ulDiv = listDiv.select('ul.ajaxListUL').last();
        var rows = self.ajaxListRows();
        
        rows.sort(function(a, b){
            var aDiv = a.select("div.ajaxListColumn_" + columnID).last().innerHTML.trim().toLowerCase();
            var bDiv = b.select("div.ajaxListColumn_" + columnID).last().innerHTML.trim().toLowerCase();
            if (aDiv < bDiv) { // sort string ascending
                return -1;
            } else if (aDiv > bDiv) {
                return 1;
            } else {
                return 0; // default return value (no sorting)
            }
        });
        
        // if descending, reverse the order that was just sorted in ascending
        if("desc" == direction) {
            rows.reverse();
        }

        // remove the rows from the UI
        ulDiv.childElements().each(function(child) {
            child.remove();
        });
        
        // insert the now sorted rows back into the UI
        rows.each(function(row) {
            ulDiv.insert(row);
        });
        
        /*  fire event */
        self.fireEvent("sorted");
     }
    
    /**
     * If we are paginated we have to perform the sort on the server
     */
    this.sortColumnOnServer = function(direction, columnID) {
        LCAjaxLogger.log(LCAjaxLogger.LIST, "Sorting list on server: direction => " + direction + " columnID => " + columnID);
        var ownsSpinner = self.showSpinner();
        
        var requestURL = getAjaxActionURL(sortColumnActionUrl) + "?sortDirection=" + direction + "&columnID=" + columnID;
        LCAjaxLogger.log(LCAjaxLogger.LIST, "Request URL: " + requestURL);
        
        /* send it to the server */
        new Ajax.Request(requestURL, {
              onSuccess: function(response) {
                   // if it worked we have nothing to do
                    LCAjaxLogger.log(LCAjaxLogger.LIST, "Successfully performed sort on server.");
                    var pageNumbers = response.responseJSON;
                    self.displayPageNumbers(pageNumbers);
                    self.refreshData(function() {
                        /*  fire event */
                        self.fireEvent("sorted");
                        /* we don't check for ownership here as we are definitely done doing all work at this point */
                        self.hideSpinner();
                    });
                    
              },
              onFailure: function(response){
                 self.displayAjaxListErrorMessage("Failed to perform sort on server. Please try again."); 
              },
              onException: function(request, exception) {
                 LCAjaxLogger.log(LCAjaxLogger.LIST, "Exception while performing sort on server => " + exception);
                 self.displayAjaxListErrorMessage("Failed to perform sort on server. Please try again.");
              }
        });
    }
    
    this.displayAjaxListErrorMessage = function(message) {
        self.hideSpinner();
        displayLCAjaxErrorMessage(ajaxListId + "_errorbox", message);
        
        // show the error
        //errorMessageDiv.innerHTML = message;
        //Effect.BlindDown(errorMessageBox, { duration: 0.5 });
    }
    
    /*  Return all rows */
    this.ajaxListRows = function() {
        return listDiv.select('li.ajaxListLI');
    }
    
    /* return the ID of the row element for a given itemID */
    this.rowElementIdForItemId = function(itemID) {
        return ajaxListId + "Row_"+itemID;
    }
    
    /*  Get the ItemID for a given Row by passing in any child element of that row */
    this.getItemIdFromRow = function(rowElement) {
       var itemId;
       rowElement.ancestors().each(function(element) {
            if(element.hasClassName && element.hasClassName('ajaxListRowDiv')) {
                itemId = element.getAttribute('itemId');
            }
       });
       return itemId;
    }
    
    /* Get the ColumnID for a given Element */
    this.getColumnIdFromElement = function(columnElement) {
       var columnId;
       var elementToCheck = columnElement;
       if(columnElement.hasClassName && (columnElement.hasClassName('ajaxListColumn') || columnElement.hasClassName('ajaxListColumnHeader'))) {
    	   // the element IS the columnHeader
       } else {
	       columnElement.ancestors().each(function(element) {
	            if(element.hasClassName && (element.hasClassName('ajaxListColumn') || element.hasClassName('ajaxListColumnHeader'))) {
	            	elementToCheck = element;
	            }
	       });
       }
       
       // iterate numbers until we find it
       for(var i=0; i < 30; i++) {
           if(elementToCheck.hasClassName('ajaxListColumn_'+i)) {
               columnId = i;
           }
       }
       return columnId;
    }
    
    /**
    * Manage list change events
    */
    this.onAjaxListChange = function() {
        self.manageEmptyListRow();
        self.preventLastRowDeletion();
        self.manageAddButtonAndMaxRowCount();
        self.fireEvent("rowChanged");
    }
    
    this.manageEmptyListRow = function() {
        if(self.ajaxListRows().size() == 0) {
        	var searchFieldValue;
        	if(self.searchField) {
        		searchFieldValue = self.searchField.value;
        	}

        	// if we are searching then show the no results message
        	if(searchFieldValue != undefined && searchFieldValue != '' && searchFieldValue != defaultSearchString) {
        		listDiv.select('div.emptyListMessage').each(function(item) {item.hide()});
        		listDiv.select('div.noResultsMessage').each(function(item) {item.show()});
        	} else {
        		// otherwise show the empty list message
        		listDiv.select('div.noResultsMessage').each(function(item) {item.hide()});
        		listDiv.select('div.emptyListMessage').each(function(item) {item.show()});
        	}
        } else {
        	// we have data so hide both of these
           listDiv.select('div.noResultsMessage').each(function(item) {item.hide()});
           listDiv.select('div.emptyListMessage').each(function(item) {item.hide()});
        }
    }
    
    // asks server if add button is enabled
    this.updateAddButtonStateFromAction = function() {    
        if ( ! isAddButtonActionEnabled ) {
           // using the action is disabled, so default is to have button enabled
	           enableAddButtonForLCAjaxList(ajaxListId);
	           return;
        }
        
        var requestURL = getAjaxActionURL(isAddButtonEnabledUrl);
        LCAjaxLogger.log(LCAjaxLogger.LIST, "Request URL: " + requestURL);
        
        /* send it to the server */
        new Ajax.Request(requestURL, {
              onSuccess: function(response) {
                   // if it worked we have nothing to do
                    LCAjaxLogger.log(LCAjaxLogger.LIST, "Successfully retrieved add button status from server.");
                    if (response.responseJSON.isAddButtonEnabled) {
                        enableAddButtonForLCAjaxList(ajaxListId);
                    }
                    else {
                        disableAddButtonForLCAjaxList(ajaxListId);
                    }
              },
              onFailure: function(response){
                 self.displayAjaxListErrorMessage("Failed to retrieve add button status. Please try again."); 
              },
              onException: function(request, exception) {
                 LCAjaxLogger.log(LCAjaxLogger.LIST, "Exception while retrieving add button status from server => " + exception);
                 self.displayAjaxListErrorMessage("Failed to retrieve add button status. Please try again.");
              }
        });
    }
    
    this.manageAddButtonAndMaxRowCount = function() {
        var isAddButtonEnabled = true;
        
        if (self.ajaxListRows().size() >= maxNumberRows) {
            // disable the add button if we're at or above the max number of rows
            disableAddButtonForLCAjaxList(ajaxListId);
        } else {
            self.updateAddButtonStateFromAction();
        }
    }
    
    this.showSpinner = function() {
        var spinner = listDiv.select('img.ajaxListSpinner').first();
        if(spinner && ! listSpinnerDisabled) {
            if(!spinner.visible()) {
                spinner.show();
                return true;
            }
        }
        return false;
    }
    
    this.hideSpinner = function() {
        var spinner = listDiv.select('img.ajaxListSpinner').first();
        if(spinner) {
            spinner.hide();
        }
    }
    
    this.showRowSpinner = function(itemID) {
        var rowElementID = self.rowElementIdForItemId(itemID);
        if(rowElementID && $(rowElementID) && ! rowSpinnerDisabled) {
	        var spinner = $(rowElementID).select('div.ajaxListRowSpinner').first();
	        if(spinner) {
	            if(!spinner.visible()) {
	                spinner.show();
	                return true;
	            }
	        }
        }
        return false;
    }
    
    this.hideRowSpinner = function(itemID) {
        var rowElementID = self.rowElementIdForItemId(itemID);
        if(rowElementID && $(rowElementID)) {
	        var spinner = $(rowElementID).select('div.ajaxListRowSpinner').first();
	        if(spinner) {
	            spinner.hide();
	        }
        }
    }
    
    this.preventLastRowDeletion = function() {
        var listOfRows = self.ajaxListRows();
    
        if(preventLastRowDeletion) {
            if(listOfRows.size() > 1) {
                // we always want it enabled
                listDiv.select('img.ajaxListItemRowDeleteButtonEnabled').each(function(item) {item.show()});
                listDiv.select('img.ajaxListItemRowDeleteButtonDisabled').each(function(item) {item.hide()});
            } else if(listOfRows.size() == 1) {
                // we want delete disabled
                listDiv.select('img.ajaxListItemRowDeleteButtonEnabled').each(function(item) {item.hide()});
                listDiv.select('img.ajaxListItemRowDeleteButtonDisabled').each(function(item) {item.show()});
            }
        }
    }
    
    /*
     * Refresh the data in the list via ajax. 
     */
    this.refreshData = function(executeAfterRefresh) {
        LCAjaxLogger.log(LCAjaxLogger.LIST, "Refresh list data...");
        var ownsSpinner = self.showSpinner();
        
        // update the container
        var listDataUpdateContainerSelector = '#' + ajaxListId + 'ListRefreshContainerId';
        var listHeaderDivSelector = '#' + ajaxListId + 'Title';
        var searchHeaderDivSelector = '#' + ajaxListId + 'HeaderContainerId';

        LCAjaxLogger.log(LCAjaxLogger.LIST, "Refreshing state of list.");
 
        var request = $j.ajax({
            type: "POST",
            url: listItemsUrl + "?refresh=1",
            dataType: "html"
        });
        
        request.done(function(responseBody) {
            // replace list title
            var headerText = $j(listHeaderDivSelector, responseBody);
            $j(listHeaderDivSelector).replaceWith(headerText);
            
            // replace search title
            if ($j(searchHeaderDivSelector).length > 0) {
	                var searchHeaderText = $j(searchHeaderDivSelector, responseBody);
	                $j(searchHeaderDivSelector).replaceWith(searchHeaderText);
	            }
            
            // replace list elements 
            var responseContainer = $j(listDataUpdateContainerSelector, responseBody);
            $j(listDataUpdateContainerSelector).replaceWith(responseContainer);
            
            // need to reinitialize everything since the list has been recreated in the DOM
            self.initializeUI();
            self.makeListReorderable();  
            if(executeAfterRefresh) {
                executeAfterRefresh.call();
            }
            if(ownsSpinner) {
                self.hideSpinner();
            }
        });

        request.fail(function(jqXHR, status) {
            self.displayAjaxListErrorMessage("Failed to update list elements."); 
        });                
    };
    
    /*
     * Tell the server what page we want and then refresh data
     */
    this.changePage = function(pageNum) {
        LCAjaxLogger.log(LCAjaxLogger.LIST, ">>> Change Page: " + pageNum);
        var ownsSpinner = self.showSpinner();
        
        var requestURL = getAjaxActionURL(pageNumberActionUrl) + "?pageNumber=" + pageNum;
        LCAjaxLogger.log(LCAjaxLogger.LIST, "Request URL: " + requestURL);
        
        /* send it to the server */
        new Ajax.Request(requestURL, {
              onSuccess: function(response) {
                   // if it worked we have nothing to do
                    LCAjaxLogger.log(LCAjaxLogger.LIST, "Successfully set new pageNumber on server.");
                    var pageNumbers = response.responseJSON;
                    self.displayPageNumbers(pageNumbers);
                    self.refreshData(function() {
                        if(ownsSpinner) {
                            self.hideSpinner();
                        }
                        self.fireEvent("pageChanged");
                    });
              },
              onFailure: function(response){
                 self.displayAjaxListErrorMessage("Failed to change pageNumber. Please try again."); 
              },
              onException: function(request, exception) { 
                 LCAjaxLogger.log(LCAjaxLogger.LIST, "Exception while changing pageNumber => " + exception);
                 self.displayAjaxListErrorMessage("Failed to change pageNumber. Please try again.");
              }
        });
    };
    
    /**
     * Update the header UI with the page numbers/links using the given JSON.
     */
    this.displayPageNumbers = function(json) {
        // eval the JSON if we're receiving a non-Object
        if(!(json instanceof Object)) {
            json = json.evalJSON();
        }
        
        var html = LCTemplate.getHtml('global/ajaxlist_pages', json);
        var paginationContainer = listDiv.select(".ajaxListPagination").last();
        if(paginationContainer) {
        	paginationContainer.innerHTML = html;
        }
    }
    
    /* 
     * When loaded the list needs to have these things done to initialize the UI.
     */
    this.initializeUI = function() {
        self.manageEmptyListRow();
        self.preventLastRowDeletion();
        self.manageAddButtonAndMaxRowCount();
        self.updateAjaxListSortOrderColumn();
    };
    
    /* This is used to trigger custom events. */
    this.fireEvent = function(eventName) {
        LCAjaxLogger.log(LCAjaxLogger.LIST, "Firing LCAjaxList:" + eventName);
        $(listDiv).fire("LCAjaxList:"+eventName, { ajaxListId: ajaxListId });
    }
}

/**
 * Class for showing the search textfield with the (x), grey text when empty and magnifying glass
 * @param parent
 * @param wrapper
 * @param default_value
 * @returns
 */
var LCAjaxSearchField = function(parent, wrapper, default_value) {
    var container = $$(wrapper)[0];
    var search_box = $$(wrapper + " input")[0];
    search_box.className += ' blurred';
    container.className += ' empty';
    search_box.value = default_value;
    var on_blur = function(e) {
        search_box.value = default_value;
        search_box.className += ' blurred';
        container.className += ' empty';
        parent.search('');
    };
    search_box.observe("focus", function(e) {
        if (search_box.value != default_value) return;
        search_box.value = "";
        search_box.className = search_box.className.replace(/blurred/g, '');
    });
    search_box.observe("keydown", function(e) {
        container.className = container.className.replace(/empty/g, '');
    });
    search_box.observe("blur", function(e) {
        if (search_box.value.match(/[^s+]/) != undefined) return;
        on_blur(e);
    });
    $$(wrapper + " .reset")[0].observe('mousedown', on_blur);
};


