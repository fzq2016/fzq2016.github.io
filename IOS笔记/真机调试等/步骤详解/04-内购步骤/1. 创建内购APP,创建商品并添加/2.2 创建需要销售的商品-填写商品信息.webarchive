/* since CSS doesn't give any simple way of doing this ... this function will calculate it */
function centerInViewport(element, maxFromTop) {
    var topValue =  (getViewportHeight()/2)-(element.getHeight()/2);
    if(maxFromTop && topValue > maxFromTop) {
        topValue = maxFromTop;
    }
    var leftValue = (getViewportWidth()/2)-(element.getWidth()/2);
    element.setStyle({
        top: topValue + "px",
        left: leftValue + "px"
    });
}

function centerHorizontallyInViewport(element) {
    var leftValue = (getViewportWidth()/2)-(element.getWidth()/2);
    element.setStyle({
        left: leftValue + "px"
    });
}

/* since CSS doesn't give any simple way of doing this ... this function will calculate it */
function centerInContainer(element, container, maxFromTop) {
    var topValue =  (container.getHeight()/2)-(element.getHeight()/2);
    if(maxFromTop && topValue > maxFromTop) {
        topValue = maxFromTop;
    }
    var leftValue = (container.getWidth()/2)-(element.getWidth()/2);
    element.setStyle({
        top: topValue + "px",
        left: leftValue + "px"
    });
}

function centerHorizontallyInContainer(element, container) {
    var leftValue = (container.getWidth()/2)-(element.getWidth()/2);
    
    element.setStyle({
        left: leftValue + "px"
    });
}

