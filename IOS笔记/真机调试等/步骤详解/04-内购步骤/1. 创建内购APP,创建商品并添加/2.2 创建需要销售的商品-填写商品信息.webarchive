// causes a prefix to pre-populate in the text field on focus and preprend or disappear on blur  
function attachTextFieldValuePrefixHandler(element, prefix) {
	var el = $j(element);
	
	el.focus (function() {
		if ($j(this).val() === ''){
        $j(this).val(prefix);
      }
    });

    el.blur (function() {
      if ($j(this).val() === prefix) {
        $j(this).val('');
      } 
      else {
        var inputValue = $j(this).val();
        var grpValue = inputValue.substring(0, prefix.length);
        if (grpValue != prefix) {
          $j(this).val(prefix + inputValue)
        }
      }
    });
}


/** Music/Music Video Date Calendar **/

var today = new Date();
var nextYear = today.getFullYear()+1;
nextYear = nextYear.toString();
var maxDatePickerDate = "12/31/"+nextYear;

var todayMonth = today.getMonth()+1;
var todayDay = today.getDate();
if (todayMonth < 10) { todayMonth = "0"+todayMonth; }
if (todayDay < 10) { todayDay = "0"+todayDay; }
var todayFormatted = todayMonth + "/" + todayDay + "/" + new Date().getFullYear();

$j(function() {
  $j( ".datepicker-nonebtn" ).datepicker({
    minDate: new Date(),
    dayNamesMin: ["S", "M", "T", "W", "T", "F", "S"],
    dateFormat: "mm/dd/yy",
    showButtonPanel: true,
    maxDate: maxDatePickerDate,
    beforeShow: function( input ) {
    setTimeout(function() {
      var buttonPane = $j( input ).datepicker( "widget" ).find( ".ui-datepicker-buttonpane" );
      var now = $j('<button class="ui-datepicker-nonebtn">None</button>');
      var theInput = input;
      now.click( function() { $j(theInput).val(""); $j(theInput).datepicker('hide'); });
      $j(buttonPane).html(now);
      }, 1 );
    },
    onClose: function() {
      $j(this).val();
    }
  });
  $j( ".datepicker-nonebtn" ).keyup(function(){
    var finalvalue = validateDate($j(this).val());
    $j(this).val(finalvalue);
  });
  function validateDate (datestring) {
    //var maxDate = "12/31/"+nextYear;
    var todayTimestamp = Date.parse(today);
    var timestamp=Date.parse(datestring);
    var maxdatestamp = Date.parse(maxDatePickerDate);
    if (isNaN(timestamp)==true) {
      return "";
    } else if(timestamp > maxdatestamp ) {
      return maxDatePickerDate;
    } else if (timestamp < todayTimestamp) {
      return todayFormatted;
    } else {
      return datestring;
    }
  }
});

function convertToHexString(string) {
  var hexString = null;

  if (string.length == 7 && string[0] == '#') {
    hexString = string;
  }
  else if (string.length == 6) {
    hexString = '#' + string;
  }
  else if (string.length == 3) {
    hexString = '#' + string + string;
  }

  return hexString;
}

