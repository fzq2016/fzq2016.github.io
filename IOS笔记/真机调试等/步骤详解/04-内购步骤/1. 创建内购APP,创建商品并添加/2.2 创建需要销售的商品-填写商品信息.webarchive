/* File upload */
function uploadImageDU(imageId, duBaseUrl, duImageApiType, appleId, correlationKey, itcToken, contentProviderId, imageTokenInfoId, imageButtonId, imageWellId) {
    // set payload
    var formData = new FormData();
    var file = document.getElementById(imageId).files[0];

    var fileType = (file.type.length == 0 && (file.name.endsWith(".lsr") || file.name.endsWith(".lcr"))) ? "image/lsr" : file.type;
    formData.append(file.name, file);

    jQuery.ajax({

                    url: duBaseUrl+"upload/"+duImageApiType,
                    type: 'POST',
                    contentType: fileType,
                    dataType: 'json',
                    data: file,
                    processData: false,
                    headers: {
                        'X-Apple-Upload-Referrer': window.location.href,
                        'X-Apple-Upload-AppleId': appleId,
                        'X-Apple-Jingle-Correlation-Key': correlationKey,
                        'X-Apple-Upload-itctoken': itcToken,
                        'X-Apple-Upload-ContentProviderId': contentProviderId,
                        'X-Original-Filename': file.name
                    },
                    success: function (data) {
                        data.imageFileName = file.name;
                        document.getElementById(imageTokenInfoId).value = JSON.stringify(data);
                        document.getElementById(imageButtonId).click();
                        //toggleProgressIndicator(imageWellId);
                    },
                    error: function (data) {
                        if (data.status != 0)
                            data.responseText = JSON.parse(data.responseText);
                        document.getElementById(imageTokenInfoId).value = JSON.stringify(data);
                        document.getElementById(imageButtonId).click();
                        toggleProgressIndicator(imageWellId);
                    }

                });
}

/* File upload using generic DU*/
function uploadImageGenericDU(imageId, duBaseUrl, duRuleSetName, appleId, correlationKey, itcToken, contentProviderId, imageTokenInfoId, imageButtonId) {
    // catch no file error
    if (document.getElementById(imageId).files.length == 0) {
        document.getElementById(imageTokenInfoId).value = "{\"status\":1000}";
        document.getElementById(imageButtonId).click();
        return;
    }

    // set payload
    var formData = new FormData();
    var file = document.getElementById(imageId).files[0];

    var fileType = (file.type.length == 0 && (file.name.endsWith(".lsr") || file.name.endsWith(".lcr"))) ? "image/lsr" : file.type;
    formData.append(file.name, file);

    jQuery.ajax({
                    url: duBaseUrl+"upload/image",
                    type: 'POST',
                    contentType: fileType,
                    dataType: 'json',
                    data: file,
                    processData: false,
                    headers: {
                        'X-Apple-Upload-Referrer': window.location.href,
                        'X-Apple-Upload-AppleId': appleId,
                        'X-Apple-Jingle-Correlation-Key': correlationKey,
                        'X-Apple-Upload-itctoken': itcToken,
                        'X-Apple-Upload-ContentProviderId': contentProviderId,
                        'X-Original-Filename': file.name,
                        'X-Apple-Upload-Validation-RuleSets': duRuleSetName
                    },
                    success: function (data) {
                        data.imageFileName = file.name;
                        document.getElementById(imageTokenInfoId).value = JSON.stringify(data);
                        document.getElementById(imageButtonId).click();
                    },
                    error: function (data) {
                        if (data.status != 0)
                            data.responseText = JSON.parse(data.responseText);
                        document.getElementById(imageTokenInfoId).value = JSON.stringify(data);
                        document.getElementById(imageButtonId).click();
                    }

                });
}

function processFileSelection(fileField) {
    function updateSupportingElements(fileField, fileName) {
        // Fill in field
        document.getElementById(fileField.id + "-itemName").innerHTML = fileName;
        document.getElementById(fileField.id + "-itemName").style.display = "block";
        //Display button
        document.getElementById(fileField.id + "Div").style.display = "block";
    }

    var fileName = fileNameFromPath(fileField.value);
    var fileFieldEle = $j(fileField);
    if (fileField.id == "binary") {
        var zipExt = ".zip", ipaExt = ".ipa";
        var format = "ZIP";
        if (fileName.endsWith(zipExt) || fileName.endsWith(ipaExt)) {
           updateSupportingElements(fileField, fileName);
        } else {
          fileFieldEle.replaceWith(fileFieldEle = fileFieldEle.val('').clone(true));
          alert('Binaries must be .zip or .ipa files.');
        }
    } else if (fileField.id == "pdf") {
        var zipExt = ".zip", pdfExt = ".pdf";
        if (fileName.endsWith(zipExt) || fileName.endsWith(pdfExt)) {
            updateSupportingElements(fileField, fileName);
        }else {
            fileFieldEle.replaceWith(fileFieldEle = fileFieldEle.val('').clone(true));
            alert('Uploads must be either .pdf or .zip files.');        
        }
            
    } else if (fileField.id != "additionalScreenshot") {
        updateSupportingElements(fileField, fileName);
    } else {
		newTitle = document.createElement("h2");
		newTitle.innerHTML = fileName;
		
		itemNameContentArea = document.getElementById("additionalScreenshot-itemName");
		itemNames = itemNameContentArea.getElementsByTagName("h2");
		
		if (itemNames.length > 0) {
			for (i=itemNames.length-1; i > -1; i--) {
				if (itemNames[i].getAttribute("class") == "serverGenerated") {
					itemNameContentArea.removeChild(itemNames[i]);
				}
			}
		}
		
		itemNames = itemNameContentArea.getElementsByTagName("h2");

		if (itemNames.length > 0) {
			document.getElementById("additionalScreenshot-itemName").insertBefore(newTitle,itemNames[0]);
		}
		else {
			document.getElementById("additionalScreenshot-itemName").appendChild(newTitle);
		}
        document.getElementById("additionalScreenshot-itemName").style.display = "block";
		
		//Display button
		document.getElementById("additionalScreenshotDiv").style.display = "block";
		
		// Hide file field div
		fileField.parentNode.style.display = "none";
	}
}

function processMultipleScreenshotsFileSelection(fileField, idPrefix) {

    var fileName = fileNameFromPath(fileField.value);

    newTitle = document.createElement("h2");
    newTitle.innerHTML = fileName;
    
    itemNameContentArea = document.getElementById(idPrefix+"-additionalScreenshot-itemName");
    itemNames = itemNameContentArea.getElementsByTagName("h2");
    
    if (itemNames.length > 0) {
        for (i=itemNames.length-1; i > -1; i--) {
            if (itemNames[i].getAttribute("class") == "serverGenerated") {
                itemNameContentArea.removeChild(itemNames[i]);
            }
        }
    }
    
    itemNames = itemNameContentArea.getElementsByTagName("h2");

    if (itemNames.length > 0) {
        document.getElementById(idPrefix+"-additionalScreenshot-itemName").insertBefore(newTitle,itemNames[0]);
    }
    else {
        document.getElementById(idPrefix+"-additionalScreenshot-itemName").appendChild(newTitle);
    }
    document.getElementById(idPrefix+"-additionalScreenshot-itemName").style.display = "block";
    
    //Display button
    document.getElementById(idPrefix+"-additionalScreenshotDiv").style.display = "block";
    
    // Hide file field div
    fileField.parentNode.style.display = "none";
}

function fileNameFromPath(path) {
    var name;
    
    // do this for all because of the c:\fakepath\ that shows on modern browsers.
    name = path.substring(path.lastIndexOf('\\') + 1, path.length);

    // grab the slashes from older-non-windows browsers
    if (!navigator.userAgent.match(/Windows/)) {
        name = name.substring(name.lastIndexOf('/') + 1, name.length);
    }

    name = name.replace(/%20/g, ' ');
    return name;
}

