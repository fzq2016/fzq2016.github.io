    /**
    * Shows the confirmation popup for the column that was clicked and calls the callback 
    * function if user clicks the confirmation button.  
    */
    this.performActionWithConfirmation = function(clickedElement, itemID, confirmCallback) {
        var confirmationDivId = self.getConfirmationDivId(clickedElement);
        
        var confirmationDiv = listDiv.select('#' + confirmationDivId).first();
        var cancelButton = confirmationDiv.select('.ajaxListCancelButton').first();
        var confirmButton = confirmationDiv.select('.ajaxListConfirmButton').first();

        confirmButton.stopObserving('click');
        cancelButton.stopObserving('click');
        
        var originalBoxWidth = confirmationDiv.style.width;
        var originalBoxHeight = confirmationDiv.style.height;
    
        // set confirmation box next to clicked button element
        confirmationDiv.clonePosition(clickedElement, {offsetTop: -25, offsetLeft: -290});
        // need to reset the width since the clone resized to the button
        confirmationDiv.setStyle({width: originalBoxWidth, height: originalBoxHeight});
        
        // set the event observer for if the delete confirmation is selected
        confirmButton.observe('click', function(event) {
            LCAjaxLogger.trace("Action confirmation: Confirmed.");
            confirmCallback();
            
            // cancel all listeners on the confirmation box
            confirmButton.stopObserving('click');
            cancelButton.stopObserving('click');

            popOverOpen = false;

            // update the UI
            Effect.toggle(confirmationDiv, 'slide', { duration: 0.2 }); return false;
        });
        
        // set the event observer for if the cancel is selected
        cancelButton.observe('click', function(event) {
            LCAjaxLogger.trace("Action confirmation: Canceled.");
        
            // cancel all listeners on the confirmation box
            confirmButton.stopObserving('click');
            cancelButton.stopObserving('click');

            popOverOpen = false;

            // update the UI
            Effect.toggle(confirmationDiv, 'slide', { duration: 0.2 }); return false;
        });
        
        if (previousItemID == itemID || (previousItemID != itemID && !popOverOpen)) { Effect.toggle(confirmationDiv, 'slide', { duration: 0.2 }); }
        
        popOverOpen = true;
        previousItemID = itemID;
    }

    /**
