/* *
 * Javascript object representing an LCAjaxList
 *
 * Arguments:
 * 
 * ajaxListId => the divID of the list
 * reorderingEnabled => true/false if reordering of rows via drag-and-drop should be enabled
 * maxNumberRows => the maximum number of rows that can be added
 * preventLastRowDeletion => true/false to prevent last row from being deleted
 * searchEnabled => true/false to display search field
 * updateUrl => WO ActionURL
 * deleteUrl => WO ActionURL
 * viewRowUrl => WO ActionURL
 * itemActionUrl => WO ActionURL
 * radioSelectionUrl => WO ActionURL
 * checkboxSelectionUrl => WO ActionURL
 * customActionUrl => WO ActionURL
 * pageNumberActionUrl => WO ActionURL
 * sortColumnActionUrl => WO ActionURL
 * searchActionUrl => WO ActionURL
 * isAddButtonEnabledUrl => WO ActionURL
 * useServerSideOnly => true/false if all sorting and searching should use server regardless of pagination
 * searchFieldBackgroundText => text to display in search field
 * pageSize => -1 for no pagination, a number >0 to set the pageSize
 * enableSearchAsYouType => true/false of whether search should require hitting the enter button or be performed while typing [Default: true, only applicable when it's a server-side search]
 * searchQueueTimeout => how long in milliseconds to wait between search events that hit the server (default: 250ms)
 * showSpinnerOnLightboxLoad => boolean whether the processing spinner should show on a row when it's clicked to load a lightbox (default: false)
 * 
 * Example Arguments:
 * 
 *  var arguments = {
 *      'ajaxListId' : 'ajaxList',
 *      'reorderingEnabled' : reorderEnabled,
 *      'maxNumberRows' : 2147483647,
 *      'pageSize' : -1,
 *      'preventLastRowDeletion' : preventLastRowDeletion,
 *      'searchEnabled' : searchEnabled,
 *      'updateUrl' : '/WebObjects/iTunesConnect.woa/wo/0.5.21',
 *      'deleteUrl' : '/WebObjects/iTunesConnect.woa/wo/0.5.23',
 *      'viewRowUrl' : '/WebObjects/iTunesConnect.woa/wo/0.5.25',
 *      'itemActionUrl' : '/WebObjects/iTunesConnect.woa/wo/0.5.27',
 *      'listItemsUrl' : '/WebObjects/iTunesConnect.woa/wo/0.5.28',
 *      'addButtonActionEnabled' : false
 *      'isAddButtonEnabledUrl' : '/WebObjects/iTunesConnect.woa/wo/0.5.36',
 *      'radioSelectionUrl' : '/WebObjects/iTunesConnect.woa/wo/0.5.29',
 *      'checkboxSelectionUrl' : '/WebObjects/iTunesConnect.woa/wo/0.5.31',
 *      'customActionUrl' : '/WebObjects/iTunesConnect.woa/wo/0.5.33',
 *      'pageNumberActionUrl' : '/WebObjects/iTunesConnect.woa/wo/0.5.34',
 *      'sortColumnActionUrl' : '/WebObjects/iTunesConnect.woa/wo/0.5.35',
 *      'searchFieldBackgroundText' : 'Keyword Filter',
 *      'useServerSideOnly' : false,
 *      'searchQueueTimeout' : 1000',
 *      'enableAsyncLoading' : enableAsyncLoading
 *  };
 *  
 *
 * Events:
 *  - LCAjaxList:rowChanged
 *  - LCAjaxList:rowDeleted
 *  - LCAjaxList:reordered (drag and drop)
 *  - LCAjaxList:sorted (column sort)
 *  - LCAjaxList:loaded
 *  - LCAjaxList:pageChanged
 *
 * An event will return data values that can be used by the event handler:
 *  - ajaxListId => The ID of the Div for the lcAjaxList.
 *
 * These values can be retrieved via the "event.memo.*" property paths, such as "event.memo.ajaxListId".
 *
 * An example listener is:
 *      document.observe("LCAjaxList:rowChanged", function(event) {
 *          console.log("ListDivId: " + event.memo.ajaxListId);
 *      });
 */
 
function LCAjaxListColumnConfirmationPopup(args) {
    var self = this;
    
    var actionUrl;
     
}

function LCAjaxList(args){
    /* define an internal variable for 'this' that can't get over-written by other call-paths such as event handlers */
    var self = this;
    
    var ajaxListId=args.ajaxListId;
    var reorderingEnabled=args.reorderingEnabled;
    var maxNumberRows=args.maxNumberRows;
    var preventLastRowDeletion=args.preventLastRowDeletion;
    var searchEnabled=args.searchEnabled;
    var updateUrl=args.updateUrl;
    var deleteUrl=args.deleteUrl;
    var viewRowUrl=args.viewRowUrl;
    // var itemActionUrl=args.itemActionUrl;
    var listItemsUrl=args.listItemsUrl;
    var radioSelectionUrl=args.radioSelectionUrl;
    var checkboxSelectionUrl=args.checkboxSelectionUrl;
    // var customActionUrl=args.customActionUrl;
    var pageNumberActionUrl=args.pageNumberActionUrl;
    var sortColumnActionUrl=args.sortColumnActionUrl;
    var isAddButtonActionEnabled=args.addButtonActionEnabled;
    var isAddButtonEnabledUrl=args.isAddButtonEnabledUrl;
    var searchActionUrl=args.searchActionUrl;
    var searchFieldBackgroundText=args.searchFieldBackgroundText;
    var pageSize=args.pageSize;
    var useServerSideOnly=args.useServerSideOnly;
    var enableSearchAsYouType=args.enableSearchAsYouType;
    var SEARCH_QUEUE_TIMEOUT=args.searchQueueTimeout;
    var showSpinnerOnLightboxLoad = args.showSpinnerOnLightboxLoad;
    var listSpinnerDisabled = args.disableListSpinner;
    var rowSpinnerDisabled = args.rowSpinnerDisabled;
    var enableAsyncLoading = args.enableAsyncLoading;

    var ActionType = {
        ITEM:       { name: "item",     url: args.itemActionUrl }, 
        CUSTOM:     { name: "custom",   url: args.customActionUrl } 
    };
    
    /* default text to show in the search field */
    var defaultSearchString = "Search";
    if(searchFieldBackgroundText) {
        defaultSearchString = searchFieldBackgroundText;
    }
    
    if(useServerSideOnly == undefined) {
        useServerSideOnly = false;
    }
    
    if(enableSearchAsYouType == undefined) {
        enableSearchAsYouType = true;
    }
    
    if(SEARCH_QUEUE_TIMEOUT == undefined) {
        SEARCH_QUEUE_TIMEOUT = 250;
    }
    
    if(showSpinnerOnLightboxLoad == undefined) {
        showSpinnerOnLightboxLoad = false;
    }

    if (listSpinnerDisabled == undefined) {
    	listSpinnerDisabled = false;
    }
    
    if (rowSpinnerDisabled == undefined) {
    	rowSpinnerDisabled = false;
    }

//    if (editLightboxSaveCheckDisabled == undefined) {
//    	editLightboxSaveCheckDisabled = false;
//    }
    
    var popOverOpen = false;
    var previousItemID = null;
    
    /*  The DIV Element we will use for everything inside this object */
    var listDiv = getAjaxListDivForListID(ajaxListId);
    
    /* The "constructor" for this object */
    this.initialize = function() {
        LCAjaxLogger.log(LCAjaxLogger.LIST, "Initializing LCAjaxList ...");

        /*  initialize the default state of the UI based on the data it is populated with and configuration variables */
        self.initializeUI();
        
        /* setup a listener for click events inside the listDiv */
        listDiv.observe("click", function(event) {
            var elementClicked = event.element();
            var itemId = self.getItemIdFromRow(elementClicked);
            
            LCAjaxLogger.log(LCAjaxLogger.LIST, "[Click Event] ItemId => " + itemId + "  Clicked Element Type => " + elementClicked.tagName + "    Class Names => " + elementClicked.className);
            
            // if it's an image and does NOT have a customActionItem then we will instead act on the parent div
            if(elementClicked.hasClassName('ajaxListItemRowImageDisplay') && !elementClicked.hasClassName('ajaxListRowDivColumnCustomActionItem')) {
                elementClicked = elementClicked.ancestors().find(function(item) {return item.tagName == 'DIV' && item.hasClassName('ajaxListColumn'); })
                LCAjaxLogger.log(LCAjaxLogger.LIST, "Replaced 'ElementClicked' for image with: " + elementClicked);
            }
            
            // if the element does not have any classname starting with 'ajaxList' then we'll look at parents to see if we find 'ajaxListColumn'
            var isAjaxListElement = false;
            $w(elementClicked.className).each(function(className) {
                if (className.startsWith('ajaxList')) {
                    isAjaxListElement = true;
                }
            });
            if (!isAjaxListElement) {
                LCAjaxLogger.log(LCAjaxLogger.LIST, "Clicking on a non-ajaxList element. Looking for parent ajaxListColumn...");
                var columnElement = elementClicked.ancestors().find(function(item) {
                    return item.tagName == 'DIV' && item.hasClassName('ajaxListColumn');
                });
                if (columnElement) {
                    elementClicked = columnElement;
                    LCAjaxLogger.log(LCAjaxLogger.LIST, "Found parent ajaxListColumn. Replaced 'ElementClicked' with: " + elementClicked);
                } else {
                    LCAjaxLogger.log(LCAjaxLogger.LIST, "Not inside an ajaxListColumn. Doing nothing.");
                }
            }
            
            /* handle a row edit event */
            if(elementClicked.hasClassName('ajaxListRowEditButton')) {
                self.displayItemPanel(itemId);
            }
            
            if (elementClicked.hasClassName('ajaxListItemRowDeleteButtonEnabledNoConfirmation')) {
                self.performDelete(itemId);
            } else
            /* handle a row delete event */
            if(elementClicked.hasClassName('ajaxListItemRowDeleteButtonEnabled')) {
                self.deleteItem(elementClicked, itemId);
            }
            
            /* handle a row click on an actionable column - Item Panel */
            if(elementClicked.hasClassName('ajaxListRowDivColumnActionLightbox')) {
                self.displayItemPanel(itemId);
            }
            
            /* handle a row click on an actionable column - View */
            if(elementClicked.hasClassName('ajaxListRowDivColumnActionItem')) {
                self.performAction(itemId, elementClicked, ActionType.ITEM);
            }
            
            /* handle a row click on an actionable column - custom action */
            if(elementClicked.hasClassName('ajaxListRowDivColumnCustomActionItem')) {
                self.performAction(itemId, elementClicked, ActionType.CUSTOM)
            }
            
            /*  handle radio buttons */
            if(elementClicked.hasClassName('ajaxListItemRowRadioButton')) {
                self.radioButtonSelected(itemId, self.getColumnIdFromElement(elementClicked));
            }
            
            /*  handle checkboxes */
            if(elementClicked.hasClassName('ajaxListItemRowCheckbox')) {
                self.checkboxSelected(elementClicked, itemId, self.getColumnIdFromElement(elementClicked));
            }
            
            /* handle column sort action */
            if(elementClicked.hasClassName('ajaxListColumnSortable') || elementClicked.hasClassName('ajaxListSort')) {
            	var direction = 'asc';
            	if(elementClicked.hasClassName('ajaxListSort')) {
            		// they clicked the icon itself, to look at it for the direction
            		if(elementClicked.hasClassName('ajaxListSortUp')) {
            			// this column is already sorted ascending, so we'll do the reverse
            			direction = 'desc';
            		}
            	} else {
            		// they clicked the header, so we need to find the icon to determine direction
            		var sortIcon = elementClicked.select('div.ajaxListSort').first();
            		if(sortIcon.hasClassName('ajaxListSortUp')) {
            			// this column is already sorted ascending, so we'll do the reverse
            			direction = 'desc';
            		}
            	}
            	self.sortColumn(direction, self.getColumnIdFromElement(elementClicked));
            }
            
            // ajaxListPageLink
            if(elementClicked.hasClassName('ajaxListPageLink')) {
                var pageNum = elementClicked.innerHTML;
                var href = elementClicked.href;
                // strip javascript:// 
                var pageNum = href.substring(13);
                self.changePage(pageNum);
            }
            
            
        });
        
        /* setup a listener for LCAjaxList:dropdownSelected (select box 'change') events inside the listDiv */
        listDiv.observe("LCAjaxList:dropdownSelected", function(event) {
            var elementChanged = event.element();
            var itemId = self.getItemIdFromRow(elementChanged);
        
            /*  handle 'ajaxListItemRowDropdown' dropdowns */
            if(elementChanged.hasClassName('ajaxListItemRowDropdown')) {
                self.dropdownValueChanged(itemId, elementChanged, $j(elementChanged).val());
            }
        });
        
        listDiv.observe("LCAjaxList:reload", function(event) {
            self.refreshData();
        });

        
        /* setup a listener for handling an add button event on image buttons (remove this block once all image buttons have been replaced with span buttons) */
        $$("img.ajaxListAddButtonEnabled").each(function(button) {
            var listId = button.getAttribute("listId");
            if(listId == ajaxListId) {
                /*  we found the add button for this list, so let's observe it */
                button.observe("click", function(event) {
                    //var elementClicked = event.element();
                    self.addRow();
                });            
            }
        });
        
        /* setup a listener for handling an add button event on span buttons */
        $$("span.ajaxListAddButton").each(function(button) {
            var listId = button.getAttribute("listId");
            if(listId == ajaxListId) {
                /*  we found the add button for this list, so let's observe it */
                button.observe("click", function(event) {
                	var elementClicked = event.element();
                	if ( ! $j(elementClicked).hasClass('disabled') ) {
                		self.addRow();	
                	}
                });            
            }
        });
        
        self.enableSearchAsYouType=true;

        /* setup a listener for handling search events */
        if (searchEnabled) {
            var findSearchFields = listDiv.select("input.ajaxListFilterTextField");
            if (findSearchFields != null) {
                var searchField = findSearchFields.first();
                /* remember the field so others can access it */
                self.searchField = searchField;
                
                var s = new LCAjaxSearchField(self, '#' + ajaxListId + 'Container .search-wrapper', defaultSearchString);

                // now setup listener
                if (self.isSearchOnServer()) {
                    if (self.enableSearchAsYouType) {
                        
                        var searchFieldListener = new LCTextfieldListener({
                            'textfield' : searchField,
                            'delay' : SEARCH_QUEUE_TIMEOUT,
                            'functionToExecuteOnEvent' : function(textfield, query) {
                                self.search(query);
                            }
                        });
                    } else {
                        // requires hitting the enter/return key or the 'Search' button
                        throw "We do not yet support search without search-as-you-type";
                    }
                } else {
                    // we are searching locally
                    searchField.observe("keyup", function(event) {
                        self.search(event.element().value);
                    });
                }
            }
        }
        
        /* setup a listener for handling a lightbox save event */
        document.observe("LCAjaxLightbox:save", function(event) {
           if(event.memo.lightboxDivId == ajaxListId + 'Lightbox') {
               self.updateRow(event.memo.id); 
           }
        });
        
        /* if reorderingEnabled = true, let's initialize the list */
        if(reorderingEnabled) {
           self.makeListReorderable();
        }
        
        self.fireEvent("loaded");
        
        if (enableAsyncLoading) {
            self.refreshData();
        }
        
        LCAjaxLogger.log(LCAjaxLogger.LIST, "Done Initializing LCAjaxList.");
    }


    /* Make the list reorderable via drag-and-drop  */
    this.makeListReorderable = function() {
        /*  double check if we want reordering enabled in case this is called from elsewhere */
        if(reorderingEnabled) {
            LCAjaxLogger.log(LCAjaxLogger.LIST, "Making list reorderable: " + ajaxListId);
            Sortable.create(ajaxListId, {
                constraint: 'vertical',
                handle: 'ajaxListRowDivDragHandle',
                onUpdate: self.reordered
            });
        }
    }
    
    /* called when reorder action occurs */ 
    this.reordered = function(list) {
    
                LCAjaxLogger.log(LCAjaxLogger.LIST, "Received reorder event from reorderable list...");
    
                self.showSpinner();
                
                var nodes = $A(list.childNodes);
                var ids = [];
                
                /* iterate the items in the reordered list and get their ids */        
                nodes.each(function(item) {
                    if(item.select('div.ajaxListRowDiv').length > 0) {
                        // get the itemId for each row and put it into the list in the new order to send to server
                        ids.push(item.select("div.ajaxListRowDiv").first().getAttribute("itemId"));
                    }
                });
                
                /* we now have an array of the ids in their reordered order to pass to the server */       
                LCAjaxLogger.log(LCAjaxLogger.LIST, "Reordered Node Array: " + ids);
                
                /* create the request URL ... the URL is dynamically generated by WebObjects, so we get it from a hidden HREF 'ajaxUpdateSortURL' */
