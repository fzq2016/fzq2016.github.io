            iconElt.setAttribute("src", "/itc/images/status-red.png");
            iconElt.setAttribute("class", "status-icon");
            divLink.appendChild(iconElt);
            divLink.appendChild(document.createTextNode("Status Error"));
        }
        msgStatusDiv.parentNode.replaceChild(divLink, msgStatusDiv);        
        msgStatusDiv = divLink;
        msgStatusDiv.show();
    },
    prepareStatus: function(statusType, divIdName, msgPrefix) {
        var status = this.statusFor(statusType);
        if (!status) {
            return; // nothing to do
        }
        // OK we have some status, so let's prepare the UI accordingly
        var divElement = $(divIdName + "-" + this.productId + "-" + this.standardizeSType );
        var msgDiv = $("message-" + divIdName + "-status-" + this.productId + "-" + this.standardizeSType );
        var total = Object.keys(status).length;
        var suffix = total > 1 ? "Stores" : "Store";
        // Create the node to insert in the DOM
        var nodePrefix = document.createTextNode(msgPrefix + " ");
        var nodeLink = new Element("a");
        nodeLink.setAttribute("href", "javascript:showStatusMenuInDiv(\""+this.productId+"\",\""+this.sType+"\",\"status-menu-"+divIdName+"-"+this.productId + "-" + this.standardizeSType +"\",\""+statusType+"\", this)");
        nodeLink.appendChild(document.createTextNode(""+ total + " " + suffix));
        msgDiv.appendChild(nodePrefix);
        msgDiv.appendChild(nodeLink);
        divElement.show();
    },
    hasNoProducts: function() {
        var status = this.statusFor('notOnStore');
        if (!status || status.length == 0) { return false; }
        // check the first item
        var item1 = status[0];
        // If countryCode + country (name) == '_none_' then it has no product
        return (item1.countryCode == '_none_' && item1.country == '_none_');
    },
    showStatusNoProduct: function() {
        var divIdName = 'not-on-store';
        var statusType = 'notOnStore';
        // find the DIV where we should dislay the status
        var divElement = $(divIdName + "-" + this.productId + "-" + this.standardizeSType );
        var msgDiv = $("message-"+divIdName+"-status-" + this.productId + "-" + this.standardizeSType );
        var nodeLink = new Element("a");
        nodeLink.setAttribute("href", "javascript:showNoProductStatusMenuInDiv(\""+this.productId+"\",\""+this.sType+"\",\"status-menu-"+divIdName+"-"+this.productId + "-" + this.standardizeSType +"\",\""+statusType+"\", this)");
        nodeLink.appendChild(document.createTextNode("Not on Store"));
        msgDiv.appendChild(nodeLink);
        divElement.show();
    },
    showContentItem: function() {
        // called when we got back the result from the XHR
        var spinner = $("lc-status-spinner-" + this.productId + "-" + this.standardizeSType );
        // Get the global DIV for this product
        var statusBox = $("lc-status-box-" + this.productId + "-" + this.standardizeSType );
        if (this.success === true) {
	        // We should now show the result based on the information we received
	        var onStore = this.statusFor('onStore');
	        if (onStore) {
	            // Need to update the corresponding div
	            var onStoreDiv = $("on-store-" + this.productId + "-" + this.standardizeSType );
	            var msgDiv = $("message-on-store-status-" + this.productId + "-" + this.standardizeSType );
	            var total = onStore.length;
	            var suffix = total > 1 ? "Stores" : "Store";
	            // Create the node for the DOM
	            var nodePrefix = document.createTextNode("On ");
	            var nodeLink = new Element("a");
	            nodeLink.setAttribute("href", "javascript:showStatusMenuInDiv(\""+this.productId+"\",\""+this.sType+"\",\"status-menu-on-store-"+this.productId + "-" + this.standardizeSType +"\",\"onStore\", this)");
	            nodeLink.appendChild(document.createTextNode(""+ total + " " + suffix));
	            msgDiv.appendChild(nodePrefix);
	            msgDiv.appendChild(nodeLink);
	            onStoreDiv.show();
	        }
	        // Ready for store status
	        this.prepareStatus('readyForStore', 'ready-store', 'Ready for');
	        if (this.hasNoProducts()) {
	            this.showStatusNoProduct();
	        } else {
	            this.prepareStatus('notOnStore', 'not-on-store', 'Not on');
	        }
        } else {
            // Something went wrong when fetching
	        var divIdName = 'not-on-store';
	        var statusType = 'notOnStore';
	        // find the DIV where we should dislay the status
	        var divElement = $(divIdName + "-" + this.productId + "-" + this.standardizeSType );
	        var msgDiv = $("message-"+divIdName+"-status-" + this.productId + "-" + this.standardizeSType );
	        var msgNode = document.createTextNode("Status Error");
	        msgDiv.appendChild(msgNode);
	        divElement.show();
        }
        spinner.hide(); // Hide spinner
        statusBox.show(); // Show the status box
    }
});

var LCContentStatusManager = Class.create( {
    initialize: function() {
        this.products = {};
    },
    getRegisteredAjaxContentFor: function(productId, sType) {
        var contentAjax = this.products[productId +"-"+sType];
        return contentAjax;
    },
    registerNewAjaxContent: function(contentAjax) {
        var sType = contentAjax.sType;
        var pid = contentAjax.productId;
        this.products[pid +"-"+sType] = contentAjax;
    },
    contentAjaxFor: function(productId, productName, sType, pType) {
        var contentAjax = this.getRegisteredAjaxContentFor(productId, sType);
        if (!contentAjax) {
            contentAjax = new LCContentStatusAjaxItem(productId, productName, sType, pType, null);
            this.registerNewAjaxContent(contentAjax);
        }
        return contentAjax;
    },
    registerContentStatusItemForDivAndURL: function(productId, productName, sType, pType, ajaxUrl) {
        var contentAjax = new LCContentStatusAjaxItem(productId, productName, sType, pType, ajaxUrl);
        this.registerNewAjaxContent(contentAjax);
    },
    registerContentStatusItemForCountryAndURL: function(productId, productName, countryCode, sType, pType, ajaxUrl) {
        var contentAjax = this.contentAjaxFor(productId, productName, sType, pType); // create if does not exists
        this.fetchContentStatusForCountryCode(contentAjax, countryCode, ajaxUrl);
        return contentAjax;
    },
    handleSuccess: function(contentAjax, jsonPayload) {        
        contentAjax.contentRaw = jsonPayload;
        contentAjax.success = true;
        // Show the UI for this component
        contentAjax.showContentItem();
    },
    registerContentStatusForTypeAndCountry: function(productId, productName, sType, pType, statusType, country, countryCode, countryCode3, reasons) {
        var contentAjax = this.contentAjaxFor(productId, productName, sType, pType);
        contentAjax.registerReasonForStatusTypeAndCountry(statusType, country, countryCode, countryCode3, reasons);
    },
    handleFailFetchFor: function(contentAjax) {
        LCAjaxLogger.log(LCAjaxLogger.CONTENT_STATUS, "Fail to log content for AjaxURL:" + contentAjax.ajaxUrl);
        contentAjax.success = false;
    },
    fetchContentFor: function(contentAjax) {
        contentAjax.startFetching();
        var xhr = new XMLHttpRequest();
        var self = this;
        xhr.onreadystatechange = function() {
            // 4 == DONE (success or failure)
            if (this.readyState != 4) {
                // still working or not started so do nothing
                return;
            }
            // We're done fetching 
            contentAjax.doneFetching();
            // at this point we are DONE so we'll process the response            
            var httpStatus = 0;
            try {
                httpStatus = this.status || 0; // FYI: status is zero for http 500s
            } catch (e) {
            }
            var success = httpStatus >= 200 && httpStatus <= 299;
            if (!success) {
                self.handleFailFetchFor(contentAjax);
            } else {
                self.handleSuccess(contentAjax, this.responseText.evalJSON())
            }
       }
       xhr.open('GET', contentAjax.ajaxUrl, true);
       xhr.send();
    },
    fetchAllContentStatusAjaxItems: function() {
        var self = this;
        $H(this.products).each(function(pair) {
            var contentAjax = pair.value;
            if (!contentAjax.hasContent() && !contentAjax.isFetching) {
                // Ok we should trigger the Ajax call for getting the status there
                self.fetchContentFor(contentAjax);
            }
        });        
    },
    cleanupSType: function(stringToClean) {
        if (stringToClean && stringToClean.indexOf("-world") > 0) {
            var newString = stringToClean.replace("-world", "");
            return newString;
        }
        return stringToClean;
    },
    uriForFetchingReason: function(reason, countryCode, contentAjax) {
        var cleanSType = this.cleanupSType(contentAjax.sType);
        var uri = "/2/messaging/contentStatus/" + reason.name + "/" + countryCode + "/" + cleanSType + "/" + contentAjax.pType + ".json";
        return uri;
    },
    fetchSpecificReasonForContentAjaxAndCountry: function(reason, countryCode, contentAjax, reasons, countryInfo, statusType, callback) {
        contentAjax.startFetching();
        var xhr = new XMLHttpRequest();
        var self = this;
        var uri = self.uriForFetchingReason(reason, countryCode, contentAjax);
        xhr.onreadystatechange = function() {
            // 4 == DONE (success or failure)
            if (this.readyState != 4) {
                // still working or not started so do nothing
                return;
            }
            // We're done fetching 
            contentAjax.doneFetching();
            // at this point we are DONE so we'll process the response            
            var httpStatus = 0;
            try {
                httpStatus = this.status || 0; // FYI: status is zero for http 500s
            } catch (e) {
            }
            var totalReasons = reasons.length;
            var totalWithValue = 0;
            var success = httpStatus >= 200 && httpStatus <= 299;
            if (!success) {
                //countryInfo.hasReasons = false;
                reason.value = {message: []};
            } else {
                var jsonResult = this.responseText.evalJSON();
                // Handle status mode
                if (jsonResult.status) {
                    if ("success" == jsonResult.status) {
                        var data = jsonResult.data;
                        if (data) {
                            reason.value = data;
                        } else {
                            reason.value = {message: []};
                        }
                    } else {
                        reason.value = {message: []};
                    }
                } else {
	                // Let's update our corresponding value
	                reason.value = jsonResult;
                }
            }
            reasons.each(function(oneReason) { // Count how many status we got back
                if (oneReason.value) { totalWithValue++; }
            });
            // Check if we got all our values for the correspond countries
            countryInfo.hasReasons = totalWithValue == totalReasons;
            if (totalWithValue == totalReasons) {
                LCAjaxLogger.log(LCAjaxLogger.CONTENT_STATUS, "Got all the reasons back: " + contentAjax.productId + " - country: " + countryCode);
                callback(contentAjax,statusType,countryCode);
            }
        };
        var url = getAPIURL() + uri;
        xhr.open('GET', url, true);
        xhr.send();
    },
    fetchReasonsForContentAjax: function(contentAjax, country, statusType, callback) {
        var statusInfo = contentAjax.statusFor(statusType);
        if (statusInfo) {
            var countryInfo = null;
            for(i=0;i<statusInfo.length;i++) {
                if (statusInfo[i].country == country) {
                    countryInfo = statusInfo[i];
                    break; 
                }
            }
            var reasons = countryInfo != null ? countryInfo.reasons : null;
            if (!reasons) { return; }
            if (countryInfo.hasReasons) {
                // We have already all the reasons, no need to check server
                callback(contentAjax,statusType,countryInfo.countryCode);
                return;
            }
            var countryCode = countryInfo.countryCode;
            var self = this;
            LCAjaxLogger.log(LCAjaxLogger.CONTENT_STATUS, "Starting code to retrieve reason for '"+statusType+"' for country: " + country);            
            // Find which reasons we need to fetch 
            reasons.each(function(oneReason) {
                if (!oneReason.value) {
                    self.fetchSpecificReasonForContentAjaxAndCountry(oneReason, countryCode, contentAjax, reasons, countryInfo, statusType, callback);
                }
            });
        }
    },
    handleFailFetchForCountry: function(contentAjax, countryCode) {
        LCAjaxLogger.log(LCAjaxLogger.CONTENT_STATUS, "Error fetching status for product: " + contentAjax.productId + " for country: " + countryCode);
        contentAjax.refreshCountryStatus(null, null, countryCode, false);
    },
    handleSuccessFetchForCountry: function(contentAjax, countryCode, payload) {
        // Analyze the status
        var status = payload.status;
        var detail = payload.detail;
        contentAjax.setStatusInformationFor(status, detail);
        contentAjax.refreshCountryStatus(status, detail.country, countryCode, true);
    },
    fetchContentStatusForCountryCode: function(contentAjax, countryCode, ajaxCountryUrl) {
        // we should fetch the status for that country
        contentAjax.startFetching();
        var xhr = new XMLHttpRequest();
        var self = this;
        xhr.onreadystatechange = function() {
            // 4 == DONE (success or failure)
            if (this.readyState != 4) {
                // still working or not started so do nothing
                return;
            }
            // We're done fetching 
            contentAjax.doneFetching();
            // at this point we are DONE so we'll process the response            
            var httpStatus = 0;
            try {
                httpStatus = this.status || 0; // FYI: status is zero for http 500s
            } catch (e) {
            }
            var success = httpStatus >= 200 && httpStatus <= 299;
            if (!success) {
                self.handleFailFetchForCountry(contentAjax, countryCode);
            } else {
                self.handleSuccessFetchForCountry(contentAjax, countryCode, this.responseText.evalJSON());
            }
       }
       xhr.open('GET', ajaxCountryUrl, true);
       xhr.send();
    },
    onStoreCountries: function(productId, sType) {
        var contentAjax = this.getRegisteredAjaxContentFor(productId, sType);
        // Let's get the information about what's on store
        var countries = contentAjax.statusFor('onStore');
        return countries;
    },
    notOnStoreCountries: function(productId, sType) {
        var contentAjax = this.getRegisteredAjaxContentFor(productId, sType);
        return contentAjax.statusFor('notOnStore');
    },
    readyStoreCountries: function(productId, sType) {
        var contentAjax = this.getRegisteredAjaxContentFor(productId, sType);
        return contentAjax.statusFor('readyForStore');
    }
});

