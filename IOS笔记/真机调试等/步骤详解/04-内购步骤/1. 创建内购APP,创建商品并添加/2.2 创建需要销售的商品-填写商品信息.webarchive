var LCMediaShared = {
    /* monitor the load of a media element (image or video) for success/failure */
    monitorMediaLoad: function(media, spinner, elementToShow, executeAfterMediaLoad) {
            // the element that we'll show when the load succeeds
            if(!elementToShow) {
                elementToShow = media;
            }
            // if it fails we want to do retries
            media.retries = 0;   
            media.onerror = function (event) {
                // try setting the source again in 1 second
                setTimeout(function() {
                    media.retries++;
                    LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Try loading the failed media again [" + media.retries + "]: " + media.src);
                    // try for a long time
                    if(media.retries < 30) {
                        // try again
                        media.src = media.src;
                    } else {
                        // show the broken link so the UI represents an error occurred
                        spinner.hide();
                        elementToShow.show();
                    }
                    // try again each time with a slightly longer wait
                },1000*((media.retries+1)*0.50));
            };
            
            // if it succeeds we'll show it
            // we only show on success so we don't have a broken media showing during the retries
            media.onload = function(event) {
                //setTimeout(function() { // uncomment this line and the one at the end of the function to simulate a slow network

                    Effect.Fade(spinner, { duration: 0.0, afterFinish: function() {
                        if(BrowserDetect.browser == "Explorer") {
                            // I have no idea why Effect.Appear isn't working in IE, but it's not, so we have to use media.show()
                            elementToShow.show();
                        } else {
                            Effect.Appear(elementToShow, { duration: 0.2 });
                        }
                        
                        if(executeAfterMediaLoad) {
                            // if we have a function to execute after media load, do it now
                            executeAfterMediaLoad.call();
                        }
                    } });
                //},1000);
            };
    },
    /* This will reduce the display size of a media element if it's bigger than the viewport */
    makeFitInViewport: function(media) {
            var bufferSize=60;
            var vHeight=getViewportHeight()-bufferSize;
            var vWidth=getViewportWidth()-bufferSize;
            var originalHeight = media.height;
            if(media.tagName == 'VIDEO') {
                // if we are VIDEO use that
                originalHeight = media.videoHeight;
            }
            var originalWidth = media.width;
            if(media.tagName == 'VIDEO') {
                // if we are VIDEO use that
                originalWidth = media.videoWidth;
            }
            
            // check the height
            if(originalHeight > vHeight) {
                // set the height to the size of the viewport minus a buffer
                var newHeight = vHeight;
                // calculate what the width should now be based on the ratio
                var ratio = newHeight/originalHeight;
                // set the sizes
                media.height=newHeight;
                media.width=originalWidth * ratio;
            }
            
            // recalculate
            originalHeight = media.height;
            originalWidth = media.width;
            
            // check the width
            if(originalWidth > vWidth) {
                // set the width to the size of the viewport minus a buffer
                var newWidth = vWidth;
                // calculate what the height should now be based on the ratio
                var ratio = newWidth/originalWidth;
                // set the sizes
                media.width=newWidth;
                media.height=originalHeight * ratio;
            }
     }
};
    
    
