/* restrict characters on text input */
// source originally from http://www.qodo.co.uk/assets/files/javascript-restrict-keyboard-character-input.html

var digitsOnly = /[1234567890]/;
var digitsOnlyPlusNegative = /[-1234567890]/;
var integerOnly = /[0-9\.]/;
var alphaOnly = /[A-Z]/;
var reverseDomainNotation = /[A-Za-z0-9.-]/;

// restrict input to the given regex (digitsOnly, integerOnly, alphaOnly, etc)
function lcRestrictCharacters(myfield, e, restrictionType) {
    
    //console.log("restricting to: " + restrictionType);
    
    if (!e) var e = window.event
    if (e.keyCode) code = e.keyCode;
    else if (e.which) code = e.which;
    var character = String.fromCharCode(code);
    
    //console.log("event character: " + character);
    
    if(e.metaKey || e.ctrlKey || e.altKey) {
        // a modifier is being used, so don't restrict this
        return true;
    }
    
    // ignore if they are press other keys
    // strange because code: 39 is the down key AND ' key...
    // and DEL also equals .
    if (!e.ctrlKey && code!=9 && code!=8 && code!=36 && code!=37 && code!=38 && (code!=39 || (code==39 && character=="'")) && code!=40) {
        if (character.match(restrictionType)) {
            return true;
        } else {
            return false;
        }
        
    }
}

// restrict input to only digits 
function lcRestrictToDigits(myfield, e, allowNegative) {
    
    if (!e) var e = window.event
    if (e.keyCode) code = e.keyCode;
    else if (e.which) code = e.which;
    var character = String.fromCharCode(code);
    
    //console.log("event character: " + character);
    
    if(e.metaKey || e.ctrlKey || e.altKey) {
        // a modifier is being used, so don't restrict this
        return true;
    }
    
    if(allowNegative) {
        return lcRestrictCharacters(myfield, e, digitsOnlyPlusNegative);
    } else {
        return lcRestrictCharacters(myfield, e, digitsOnly);
    }        
}

function lcRestrictToRegex(value, restrictionType) {
    var newValue="";
    for(var i=0; i<value.length; i++) {
        var character = String.fromCharCode(value.charCodeAt(i));
        if (character.match(restrictionType)) {
            newValue = newValue + character;
        }
    }
    return newValue;
}

function lcFilterToDigits(myfield, allowNegative) {
    if(allowNegative) {
        lcFilterToRegex(myfield, digitsOnlyPlusNegative);
    } else {
        lcFilterToRegex(myfield, digitsOnly);
    }
}
function lcFilterToRegex(myfield, restrictionType) {
    var newValue = lcRestrictToRegex(myfield.value, restrictionType);
    if(myfield.value != newValue) {
        // we do this in a check so we don't move the cursor in the field unless something changed
        myfield.value = newValue;
    }
    
}

// restrict the entry in a field to a maximum number
// typically this would be combined with lcRestrictToDigits like this:
//  onkeypress = "return lcRestrictToDigits(this, event);";
//  onkeyup = "lcFilterToDigits(this, true);lcRestrictToMaxNumber(this, 100);";
function lcRestrictToMaxNumber(myfield, maxValue) {
    lcRestrictToRange(myfield, 0, maxValue);
}

//restrict the entry in a field to a maximum number
//typically this would be combined with lcRestrictToDigits like this:
//onkeypress = "return lcRestrictToDigits(this, event);";
//onkeyup = "lcFilterToDigits(this, true);lcRestrictToMaxNumber(this, 100);";
function lcRestrictToRange(myfield, minValue, maxValue, allowNull) {
     /* check for empty first and make it 0 */
     if((myfield.value == null || myfield.value == '') && !allowNull) {
         myfield.value = 0;
         myfield.select();
         return;
     }
     
     var value = myfield.value;
     var negative = false;
     
     // remove whitespace if it got in there via a copy/paste
     value = value.replace(' ', '');
     
     if(value.startsWith('-')) {
         negative = true;
         value = value.substring(1);
     }
     // remove hyphens (minus signs) that are somewhere other than the beginning
     value = value.replace('-', '');
     
     // remove preceding 0s
     while(value.length > 1 && value.startsWith('0')) {
       value = value.substring(1);
     }
     
     // add back the negative now that we've cleaned it up
     if(negative) {
         value = "-" + value;
     }
     
     var intValue = parseInt(value);
     if(intValue == 0) { 
         if(myfield.value != value) {
             // we've stripped zeros or changed it in some other way so let's update the UI
             myfield.value = value;
         }
     } else {
         if(intValue > maxValue) {
             /* if they passed the max, set the value to the max */
             myfield.value = maxValue;
             //myfield.select();
         } else if(intValue < minValue) {
             /* if they passed the min, set the value to the min */
             myfield.value = minValue;
             //myfield.select();
         } else if(myfield.value != value) {
             // we've stripped zeros or changed it in some other way so let's update the UI
             myfield.value = value;
         }
     }
}

// restrict the length of text typed in
// it can be used like this:
//    onkeypress = "return lcRestrictLength(this, 15);";
//
// or like this:
//    onkeyup = "lcRestrictLength(this, 15)";
//
// generally the "onkeyup" is better as it handles copy/paste entries.
function lcRestrictLength(myfield, maxLength) {
        if(myfield.value != null && myfield.value.length >= maxLength) {
            if(myfield.value.length > maxLength) {
                myfield.value = myfield.value.substring(0, maxLength);
            }
            return false;
        }
        return true;
}

/* END restrict characters on text input */

function trimWhiteSpace(v) {
    return v.replace(/^\s+|\s+$/g, '') ;
}

// returns 0 if the passed in argument is undefined or not a number, otherwise returns the number passed in
var zeroIfBad = function(n) {
    return (!defined(n) || isNaN(n))?0:n;
};

// if a given argument is undefined
function defined(o) {
    return (typeof(o)!="undefined");
};

/* whether the browser page has a scrollbar present */
function pageHasScrollBar(element) {
   if (getDocumentHeight() > getViewportHeight()) {
    return true; 
   } else { 
    return false;
   } 
} 

function scrollPosition() {
    if(!pageHasScrollBar()) {
        return 0;
    }
    var scrollTop = document.body.scrollTop;

    if (scrollTop == 0) {
        if (window.pageYOffset)
            scrollTop = window.pageYOffset;
        else
            scrollTop = (document.body.parentElement) ? document.body.parentElement.scrollTop : 0;
    }
    return scrollTop;
}

//Get the height of the entire document
var getDocumentHeight = function() {
    var body = $(document.body);
    if (document.documentElement && (!document.compatMode || document.compatMode=="CSS1Compat")) {
        var topMargin = parseInt(body.getStyle('marginTop'),10) || 0;
        var bottomMargin = parseInt(body.getStyle('marginBottom'), 10) || 0;
      return Math.max(body.offsetHeight + topMargin + bottomMargin, document.documentElement.clientHeight, document.documentElement.scrollHeight, 0);
    }
    return Math.max(body.scrollHeight, body.clientHeight, 0);
 };
 //Get the height of the viewport
 var getViewportHeight = function() {
    if (!window.opera && document.documentElement && (!document.compatMode || document.compatMode=="CSS1Compat")) {
      return document.documentElement.clientHeight;
    }
    else if (document.compatMode && !window.opera && document.body) {
      return document.body.clientHeight;
    }
    return 0;
};

// Get the width of the viewport
var getViewportWidth = function() {
    if (document.documentElement && (!document.compatMode || document.compatMode=="CSS1Compat")) {
      return document.documentElement.clientWidth;
    }
    else if (document.compatMode && document.body) {
      return document.body.clientWidth;
    }
    return 0;
  };
  
function getScrollerWidth() {
    var scr = null;
    var inn = null;
    var wNoScroll = 0;
    var wScroll = 0;

    // Outer scrolling div
    scr = document.createElement('div');
    scr.style.position = 'absolute';
    scr.style.top = '-1000px';
    scr.style.left = '-1000px';
    scr.style.width = '100px';
    scr.style.height = '50px';
    // Start with no scrollbar
    scr.style.overflow = 'hidden';

    // Inner content div
    inn = document.createElement('div');
    inn.style.width = '100%';
    inn.style.height = '200px';

    // Put the inner div in the scrolling div
    scr.appendChild(inn);
    // Append the scrolling div to the doc
    document.body.appendChild(scr);

    // Width of the inner div sans scrollbar
    wNoScroll = inn.offsetWidth;
    // Add the scrollbar
    scr.style.overflow = 'auto';
    // Width of the inner div width scrollbar
    wScroll = inn.offsetWidth;

    // Remove the scrolling div from the doc
    document.body.removeChild(
        document.body.lastChild);

    // Pixel width of the scroller
    return (wNoScroll - wScroll);
}





