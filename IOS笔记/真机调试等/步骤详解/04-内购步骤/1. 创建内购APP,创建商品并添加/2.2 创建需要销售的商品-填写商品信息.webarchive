// ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// START LCUploader
// ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

/* 
 * Arguments:
 *      uploadContainer = (Required) the ID or Element of the DIV wrapping the UI (see LCUploadFileComponent.html)
 *      statusURL = (Required) AJAX URL for retrieving status reports on upload progress and completion (JSON).
 *      showUploadPercentage = (Optional) Boolean whether the upload percentage should be shown while uploading. [Default: false]
 *      showFilename = (Optional) Boolean whether the filename should be shown after upload succeeds. [Default: false]
 *      extraHeaders = (Optional) Dictionary of name/value pairs to be submitted on an upload request (AJAX only)
 *      uploadURL = (Optional) Url to upload to (AJAX only) [Default: iTunesConnect lcuploader URL]
 *      HTML5Only = (Optional) If a non-HTML5 browser can fallback to form upload or not. True == No Fallback. [Default: false]
 *
 * Custom events are fired off that can be observed for hooking into the flow and adding custom behavior.
 *
 * These events are:
 *  - LCUploader:uploadBegin
 *  - LCUploader:uploadCompleted
 *  - LCUploader:uploadCancelled
 *  - LCUploader:uploadFailed
 * 
 * An event will return three data values that can be used by the event handler:
 *  - uploadContainerId => The ID of the Div for the lightbox.
 *
 * These values can be retrieved via the "event.memo.*" property paths, such as "event.memo.uploadContainerId".
 *
 * An example listener is:
 *      document.observe("LCUploader:uploadCompleted", function(event) {
 *          console.log("UploadContainerId: " + event.memo.uploadContainerId);
 *      });
 *      
 * --- Workflow ---
 * 
 * If implementing a child class the following functions may be useful for overriding to customize behavior. 
 * 
 * They are executed in the following order:
 * 
 * 1) beginUpload
 *      - before anything else happens this method is invoked
 * 
 * 2) addHeadersToUploadViaXHR(xhr)  [AJAX upload only]
 *      - this is invoked while creating the request
 * 
 * If HTML5 Upload
 * 
 *      3a) handleUploadSuccessXHR
 *          - On success this method is invoked.
 *      3b) handleUploadErrorXHR
 *          - On error this method is invoked
 * 
 *      4) doneUpload(responseText)
 *          - this is invoked after the upload is all done
 *          - it passes the raw responseText
 *          - it is invoked whether success or failure
 * 
 * If MultipartForm Upload
 * 
 *      3) updateStatus
 *          - this is called when a successful upload completes
 *          - the following methods are invoked within the default 'updateStatus' function
 * 
 *          3a) handleUploadSuccess
 *              - this is called if the status reports 'success'
 *          3b) handleUploadErrorFORM
 *              - this is called if the status reports 'error'
 *  
 */
var LCUploader = Class.create(new function(){
    
    this.initialize = function(args) {
        /* variables retrieved from arguments */
        this.uploadContainer = args.uploadContainer;
        this.statusURL = args.statusURL;
        this.showUploadPercentage = args.showUploadPercentage;
        this.showFilename = args.showFilename;
        this.extraHeaders = args.extraHeaders;
        this.uploadURL = args.uploadURL;
        this.timeout = 30*1000;
        this.maxFileSizeBytes = (2*1024*1024*1024) - 1;
        
        if(args.HTML5Only) {
            this.HTML5Only = args.HTML5Only;
            if(!this.uploadURL) {
                throw "ERROR: If HTML5Only then an uploadURL must also be provided since it can not be retrieved from the form.";
            }
        } else {
            // we want to default to 'false'
            this.HTML5Only = false;
        }
        
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "*** Initializing LCUploader. [" + this.uploadContainer + "] ***");
        
        /* variables we retrieve from the DOM */
        this.uploadContainerElement = $(this.uploadContainer);
        this.percentageDiv = this.uploadContainerElement.select('div.lcUploadPercentage').first();
        this.statusDiv = this.uploadContainerElement.select('div.lcUploadStatus').first();
        this.errorMessageDiv = this.uploadContainerElement.select('div.lcUploadError').first();
        this.fileInput = this.uploadContainerElement.select('input[type="file"]').first();
        this.filenameHidden = this.uploadContainerElement.select('input[name="filename"]').first();
        this.buttonContainer = this.uploadContainerElement.select('div.lcUploadButtonContainer').first();
        this.spinner = this.uploadContainerElement.select("div.lcUploadSpinner").first();
        if (!this.HTML5Only) { 
            this.uploadForm = this.uploadContainerElement.select('form').first();
            this.iFrame = this.uploadContainerElement.select('iframe').first();
            this.iFrameOnloadFunctionName = this.iFrame.name + '_Completed';
        
            this.uploadSessionID = this.uploadForm.select('input[name="uploadSessionID"]').first().getValue();
            this.uploadKey = this.uploadForm.select('input[name="uploadKey"]').first().getValue();
            if(this.uploadForm == null) {
                throw "ERROR on LCUploader(" + this.uploadContainer + "): The upload form can not be found. Most likely the component was placed inside a form and the browser removed the nested upload form.";
            }
        }
        // if the uploadUrl (AJAX) wasn't passed in, we'll get it from the form
        if(!this.uploadURL) {
            this.uploadURL = this.uploadForm.getAttribute('action');
        }
        
        
        // observe the 'input' for change
        if(this.fileInput) {
            LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Observing uploadContainer: " + this.uploadContainer + "  input: " + this.fileInput.identify());
            this.fileInput.observe("change", function(event) {
                LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Uploading file for uploadContainer: " + this.uploadContainer + " input: " + this.fileInput.identify());
                this.submitUpload();
            }.bind(this));
        }
    
    }
    
    this.setTimeout = function(value) {
        this.timeout = value;
    }
    
    this.getTimeout = function() {
        return this.timeout;
    }
    
    this.startSpinner = function() {
        if (this.spinner) { this.spinner.show(); }
    }
    
    this.stopSpinner = function() {
        if (this.spinner) { this.spinner.hide(); }
    }
    
    this.displayErrorMessage = function(message) {
        this.stopSpinner();
        if (this.errorMessageDiv) { this.errorMessageDiv.innerHTML = message; this.errorMessageDiv.style.display = 'block'; }
    }
    
    this.displayPercentage = function(percentage) {
        if(percentage) {
            this.percentageDiv.innerHTML = percentage + "%";
        } else {
            this.percentageDiv.innerHTML = "";
        }
    }

    this.displayStatus = function(message) {
        if (this.statusDiv) { this.statusDiv.innerHTML = message; this.statusDiv.style.display = 'block'; }
    }
    
    this.reset = function() {
            LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Reset Uploader");
            
            this.displayStatus("");
            this.displayPercentage();
            this.enableUploadButton();
            var uploadedFilename = this.filenameHidden.getValue();
            if (!this.HTML5Only) {
                this.uploadForm.reset();
                this.iFrame.onload = null;
                this.iFrame.src="about:blank";
            } else {
                // we have no form to reset, just the fileInput when HTML5-only
                this.fileInput.setValue(null);
            }
            // retain the filenameHidden value through a reset
            this.filenameHidden.setValue(uploadedFilename);
    }
    
    /* retrieve the status from the server */
    this.updateStatus = function() {
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, ">>> Update Status");
        
        // check the iFrame src first to see if it has returned with "error"
        if(window.frames[this.iFrame.name].document.body.innerHTML == 'error') {
            this.displayErrorMessage("Upload Failed: Please try again.");
            this.reset();
            this.stopSpinner();
        } else {
            // otherwise we get the status from the server because the upload is either still in progress or has succeeded
            new Ajax.Request(this.statusURL, {
                method: 'post',
                 onSuccess: function(response) {
                     LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Retrieved Upload Status: " + response.responseText);
                     var statusJson = response.responseText.evalJSON();
                     LCAjaxLogger.trace("Parsed JSON => " + statusJson);
                     
                     if(statusJson.validationError != null) {
                        // a validation error occurred
                        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Validation Error Message Received: " + statusJson.validationError);
                        this.handleUploadFailureFORM(statusJson.validationError);
                     } else {
                       // successfull status response
                         if(statusJson.percentageComplete == "-1") {
                                LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Upload Error: No Upload in Progress.");
                                this.handleUploadFailureFORM("Upload Failed: Please try again.");
                         } else if(statusJson.percentageComplete != null) {
                             this.displayPercentage(statusJson.percentageComplete)
                             
                             if(statusJson.percentageComplete == '100') {
                                LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Stop monitoring");
                                this.reset();
                                
                                // update UI now that we're done loading
                                this.stopSpinner();
                                this.handleUploadSuccess(statusJson);
                             }
                         } else {
                              // we received an unknown response
                            LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Error retrieving response => " + response.responseText);
                            this.handleUploadFailureFORM("Unknown Error Occurred: Please try again.");
                         }
                     }
                 }.bind(this), 
                 onFailure: function(response) {
                    // an unexpected error occurred ... most likely an invalid file was uploaded, but it could be anything
                    LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Error retrieving response => " + response.responseText);
                    this.handleUploadFailureFORM("Error Processing Uploaded File: Please choose a different file and try again.");
                 }.bind(this)
            });
        }
    }
    
    /**
     * Whether the browser we're running in supports HTML5 upload
     */
    this.isHTML5UploadCapable = function() {
        var xhr = new XMLHttpRequest();
        if(xhr.upload) {
            return true;
        } else {
            return false;
        }
    }
    
    /**
     * Upload a file when the file is selected.
     * 
     * This will determine if we should upload via iFrame or AJAX depending on browser support.
     */
    this.submitUpload = function() {
        // announce we are beginning the upload
        this.beginUpload();

        // clear errors and status
		if (this.statusDiv) { this.statusDiv.style.display = 'none'; }
		if (this.errorMessageDiv)  { this.errorMessageDiv.style.display = 'none'; }
        
        // start the spinner
        this.startSpinner();
        
        if(this.isHTML5UploadCapable()) {
            // we have the 'upload' method so we'll use AJAX
            this._uploadViaAjax();
            // this._uploadViaIFrame();
        } else {
            if(this.HTML5Only) {
                // we have been asked to not allow uploading via form
                this.displayErrorMessage("This upload requires a browser with HTML5 upload support. Please try in a modern browser such as Safari or Firefox.");
            } else {
                // older browser without 'upload' support over AJAX so we'll use iFrame
                this._uploadViaIFrame();
            }
        }
    }

    /*
     * called immediatately when the upload begins
     */
    this.beginUpload = function() {
        // do nothing by default allow children to hook into this
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "DEFAULT beginUpload method invoked");
        this.fireEvent("uploadBegin");
    }
    
    /*
     * called with the responseText from the upload
     * 
     * NOTE: Only applicable to AJAX uploads
     */
    this.doneUpload = function(response) {
        // do nothing by default allow children to hook into this
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "DEFAULT doneUpload method invoked");
    }
    
    /**
     * This is invoked once the file is successfully uploaded. By default it just clears the spinner and fires an event that we have succeeded.
     * 
     * Override this method in subclasses for particular logic, such as displaying images.
     * 
     * NOTE: Only applicable to FORM uploads
     */
    this.handleUploadSuccess = function(statusJson) {
        if(this.showFilename) {
            this.displayStatus(this.filenameHidden.getValue());
        }
        /*  fire event */
        this.fireEvent("uploadCompleted");
    }
    
    /**
     * NOTE: Only applicable to FORM uploads
     */
    this.handleUploadFailureFORM = function(errorMessage) {
        this.displayErrorMessage(errorMessage);
        this.reset();
        this.stopSpinner();
        this.fireEvent("uploadFailed");
    }
    
    /*
     * Callback when the upload fail
     * 
     * NOTE: Only applicable to AJAX uploads
     */
    this.handleUploadErrorXHR = function(httpStatus) {
        this.displayErrorMessage("A network timeout error occurred. Please try again later.");
        this.reset();
        this.stopSpinner();
        this.fireEvent("uploadFailed");
    };
    
    /*
     * Callback when the upload fail
     * 
     * NOTE: Only applicable to AJAX uploads
     */
    this.handleUploadSuccessXHR = function(httpStatus) {
        // the upload is complete, so update the status
        this.updateStatus();
    };

    this._uploadViaIFrame = function() {
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Uploading file via iFrame form...");
        
        /*  store the filename from the 'file' input on the hidden field so we can use it for display and so that the form recognizes that something has changed such as in a lightbox for the save button */
        this.filenameHidden.setValue(this.fileInput.getValue());
        
        /* update the status and display the image after the upload has completed */
        // Internet Explorer doesn't work using "iFrame.onload=function ..." so we have to use an inline onload call in the HTML to a method we dynamically create here ... sigh ...
        // and to make things ever more fun ... using this then only works the first time in Safari and not subsequent, and I don't feel like making a workaround for a workaround
        // so we'll just have the 'stupid way' for IE and the other way for other browsers
        if(BrowserDetect.browser == "Explorer" && BrowserDetect.version < 9) {
            window[this.iFrameOnloadFunctionName] = function() {
                LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Upload Completed.");
                // once called, kill this function so it can't get called again until we upload something again
                window[this.iFrameOnloadFunctionName] = function() {};
                // the upload is complete, so update the status
                this.updateStatus();
            }.bind(this);
        } else {
            this.iFrame.onload=function() {
                LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Upload Completed.");
                // the upload is complete, so update the status
                this.updateStatus();
            }.bind(this);
        }
        
        try {
            this.uploadForm.submit();
            LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Form submitted ... ");
            
            /*  start monitoring when upload happens */
            // NOTE: We are not doing this right now because we currently have no UI to take advantage of percentage information
            // and only small images are being upload, so it's not worth the complexity and extra requests to the server
            // when we only want to know when it's finished
            //this.monitorUpload(); // this method has been deleted, reimplement a polling mechanism if we want to do percentage updates again, the server already supports it
        }catch(e) {
            LCAjaxLogger.trace("caught form submission error: ", e);
            this.handleUploadFailureFORM("Unknown Error Occurred.");
        }
    }
    

    this._uploadViaAjax = function() {
        if (!this.isValidFileForUpload()) {
            this.stopSpinner();
            this.doneUpload("{'statusCode': 400, 'failValidation': true, 'localizedMessage': 'fail validation'}");
            return;
        }
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Uploading file via AJAX...");
        /* use 'self' for inner-functions that can't use bind since they actually need 'this' to be dynamic instead of the instance 'this' */
        var self = this;
        
        var xhr = new XMLHttpRequest();
        // record this on the object instance so we can access it to abort it and other such things
        this.xhrRequest = xhr;
        // Check that progress is not stuck
        this.timerId = setInterval(function() {
            LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Monitoring upload heartbeat...");
            self.checkUploadHeartbeat();
        }, this.getTimeout());
        
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Upload file to URL: " + this.uploadURL);
        
        /* observe changes to the state to wait for it being DONE and then process the response */
        // Documentation on this state variable: http://www.w3.org/TR/XMLHttpRequest2/#states
        xhr.onreadystatechange = function() {
            
            LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Upload state change: " + this.readyState);
            
            // 4 == DONE (success or failure)
            if (this.readyState != 4) {
                // still working or not started so do nothing
                return;
            }

            // at this point we are DONE so we'll process the response
            
            var httpStatus = 0;
            try {
                httpStatus = this.status || 0; // FYI: status is zero for http 500s
            } catch (e) {
            }
            
            /* we've completed the upload so stop the heartbeat timer */
            if (self.timerId != null) {
                clearInterval(self.timerId);
                self.timerId = null;
            }

            var success = httpStatus >= 200 && httpStatus <= 299;
            if (!success) {
                LCAjaxLogger.log(LCAjaxLogger.UPLOADER, 'Invalid upload response, http status code: "' + httpStatus + '", msg: ' + this.responseText);
                self.handleUploadErrorXHR(httpStatus);
                
                // send the response incase a child component wants to do something with it
                self.doneUpload(this.responseText, httpStatus);
                return;
            } else {
                LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Upload response: " + this.responseText);

                if(self.showUploadPercentage) {
                    self.displayPercentage(100);
                }
                LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Upload of file completed.");
                
                self.handleUploadSuccessXHR();
                
                // send the response incase a child component wants to do something with it
                self.doneUpload(this.responseText, httpStatus);
                return;
            }
        };
        
        /* monitor progress */
        if(this.showUploadPercentage) {
            LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Monitoring upload progress...");
            this.percentageDiv.show();
            this.displayPercentage(0);
       }     
       xhr.upload.onprogress = function(event) {
            this.lastProgressDate = new Date().getTime();
            var perc = Math.round((event.loaded / event.total) * 100);
            LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Progress upload: " + event.loaded + " bytes sent. Percent completion: " + perc);
            
            if (perc == 100) {
                perc = 99; // wait till the 200 response before showing 100%, server processing takes time
                this.uploadDone = true;
                // turn off the heartbeat
                clearInterval(this.timerId);
                this.timerId = null;
            }
            // If the upload is suppose to show the progress, then trigger the callback
            if (this.showUploadPercentage) {
                this.displayPercentage(perc);
            }
        }.bind(this);

        var contentType;
        var fileName;
        var theFile;
        var inputFileObject = this.fileInput;
        // We could in theory add some sanity check on the mime type/file extension
        if (inputFileObject.files) {
            // Get the type from there
            theFile = inputFileObject.files[0];
            contentType = theFile.type;
            fileName = theFile.name ? theFile.name : theFile.fileName;
        } else {
            theFile = inputFileObject;
            contenType = "application/octet-stream";
            fileName = inputFileObject.value;
        }
        
         // Start the processing
        xhr.open('POST', this.uploadURL, true);
 
        /*  store the filename from the 'file' input on the hidden field so we can use it for display and so that the form recognizes that something has changed such as in a lightbox for the save button */
        this.filenameHidden.setValue(this.fileInput.getValue());
        
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Uploading File: " + fileName + "  ContentType: " + contentType);
        
        if (contentType == null || contentType.length == 0) {
            contentType = "application/octet-stream";
        }
        xhr.setRequestHeader('Content-Type', contentType);
        if (fileName) {
            xhr.setRequestHeader('x-original-filename', encodeURIComponent(fileName));
        }
        
        // add other headers
        this.addHeadersToUploadViaXHR(xhr);
        
         // Start the progress info
        this.lastProgressDate = new Date().getTime();
        try {
            if (xhr.sendAsBinary && theFile.getAsBinary) {
                LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Uploading File using sendBinary method...");
                xhr.sendAsBinary(theFile.getAsBinary());
            } else if (xhr.sendAsBinary && window.FileReader) {
                var reader = new FileReader();
                reader.onload = function(evt) {
                    xhr.sendAsBinary(evt.target.result);
                };
                reader.readAsBinaryString(theFile);
            } else {
                LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Uploading File using send method...");
                xhr.send(theFile);
            }
        } catch (e) {
            LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Exception while sending file:" + e);
            if(this.timerId) {
                clearInterval(this.timerId);
            }
            self.handleUploadErrorXHR();
        }
    }
    
    this.isValidFileForUpload = function() {
        if (this.fileInput.files[0].size > this.maxFileSizeBytes) {
            this.stopSpinner();
            this.displayErrorMessage('Files must be less than 2GB.');
            return false;
        }
        return true;
    };
    
    /** 
     * Opportunity to add headers to the XHR request when uploading.
     * 
     * This can be overridden for customized behavior.
     * 
     * NOTE: The following headers are always set before this method is called:
     *    - x-original-filename
     *    - Content-Type
     */
    this.addHeadersToUploadViaXHR = function(xhr) {
        xhr.setRequestHeader('x-uploadSessionID', encodeURIComponent(this.uploadSessionID));
        xhr.setRequestHeader('x-uploadKey', encodeURIComponent(this.uploadKey));
        
        // Check if we have some extra headers to push
        if (this.extraHeaders) {
            $H(this.extraHeaders).each(function(pair) {
                xhr.setRequestHeader(pair.key, pair.value);
            });
        }
    }
        
    /* check to see if we have aborted or gotten stuck in some way */
    this.checkUploadHeartbeat = function() {
        if (this.lastProgressDate == 0) { return; }
        
        var now = new Date().getTime();
        var diff = now - this.lastProgressDate;
        if (diff > this.getTimeout()) {
            
            // We have waited more than 10 seconds without any bytes being pushed
            clearInterval(this.timerId);
            // Mark the request as being aborted
            this.aborted = true;
            // And finally abort the XHR
            this.xhrRequest.abort();
            this.displayErrorMessage("Upload Aborted or Timed Out.");
            this.reset();
            this.stopSpinner();
        } else if (this.uploadDone == true) {
            // Stop the timer... the upload has completed
            clearInterval(this.timerId);
        }
    }
    
    this.cancelUpload = function() {
        if(this.xhrRequest) {
            LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Cancelling upload ...");
            this.xhrRequest.abort();
            this.reset();
            this.stopSpinner();
            this.fireEvent("uploadCancelled");
        } else {
            LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Unable to cancel a non-AJAX upload ... or upload not in progress.");
        }
    }
    
    /* This is used to trigger custom events. */
    this.fireEvent = function(eventName,statusJson) {
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "*** Firing LCUploader:" + eventName);
        this.uploadContainerElement.fire("LCUploader:"+eventName, { uploadContainerId: this.uploadContainerElement.identify(), lcUploader: this ,statusJson:(statusJson || '')});
    }
    
    this.displayAjaxErrorMessage = function(errorMessage) {
        displayLCAjaxErrorMessage(this.uploadContainerElement.identify(), errorMessage);
    }
    
    this.disableUploadButton = function() {
        LCUploaderUtil.disableUploadButton(this.uploadContainerElement);
    }
    
    this.enableUploadButton = function() {
        LCUploaderUtil.enableUploadButton(this.uploadContainerElement);
    }
});

/**
 * Static utility methods for LCUploader
 */
var LCUploaderUtil = {
        /**
         * Disable the upload button on the given LCUploader container
         */
        disableUploadButton : function(uploadContainerId) {
            var _uploadContainer = $(uploadContainerId);
            if (_uploadContainer.select('input[type="file"]').length > 0) {
              _uploadContainer.select('input[type="file"]').first().hide();
              _uploadContainer.select('div.lcUploadAddButton').first().addClassName('disabled');
            }
        },
        
        /**
         * Enable the upload button on the given LCUploader container
         */
        enableUploadButton : function(uploadContainerId) {
            var _uploadContainer = $(uploadContainerId);
            if (_uploadContainer.select('input[type="file"]').length > 0) {
              _uploadContainer.select('input[type="file"]').first().show();
              _uploadContainer.select('div.lcUploadAddButton').first().removeClassName('disabled');
            }
        }   
}

/* 
 * Version of LCUploader specific to handling images.
 * 
 * Arguments:
 *      uploadContainer = (Required) the ID or Element of the DIV wrapping the UI (see LCUploadFileComponent.html).
 *      statusURL = (Required) AJAX URL for retrieving status reports on upload progress and completion (JSON).
 *      sortURL = (Optional) AJAX URL for submitting a sort order change of multiple items.
 *      deleteURL = (Optional) AJAX URL for deleting an image.
 *      maxNumberImages = (Optional) The maximum number of images to support and display. [Default: 1]
 *
 * Custom events are fired off that can be observed for hooking into the flow and adding custom behavior.
 *
 * These events are:
 *  - LCUploader:uploadBegin
 *  - LCUploader:imageAdded
 *  - LCUploader:imageDeleted
 *  - LCUploader:uploadFailed
 * 
 * An event will return three data values that can be used by the event handler:
 *  - uploadContainerId => The ID of the Div for the lightbox.
 *
 * These values can be retrieved via the "event.memo.*" property paths, such as "event.memo.uploadContainerId".
 *
 * An example listener is:
 *      document.observe("LCUploader:imageAdded", function(event) {
 *          console.log("UploadContainerId: " + event.memo.uploadContainerId);
 *      });
 */
var LCUploaderImages = Class.create(LCUploader, new function(){
    
    this.initialize = function($super, args) {
        /* initialize super class */
        $super(args);
        
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Initializing LCUploaderImages. [" + this.uploadContainer + "]");
        
        /* image specific arguments */
        this.maxNumberImages = args.maxNumberImages;
        this.sortURL = args.sortURL;
        this.deleteURL = args.deleteURL;
        
        this.imageContainer = this.uploadContainerElement.select('div.lcUploaderImageContainer').first();
        this.jsonPictureProperty = 'pictureFile'+'_';
      }
    /**
     * OVERRIDE of parent. This is invoked once the file is successfully uploaded. 
     */
    this.handleUploadSuccess = function(statusJson) {
        // display images if the response has any
         this.displayImages(statusJson);
        /*  fire event -- after monitoring the upload and adding the image */
       // if(this.showFilename) {
       //     this.displayStatus(this.filenameHidden.getValue());
       // }
        this.fireEvent("imageAdded",statusJson);
    }
    
    /* If the uploader has an image or images to display, do it here */
    this.displayImages = function(statusJson) {
        var pictureCount=1;
        
        // if there is an image defined in the JSON but no image on screen, let's add it
        var picturePropertyName = this.jsonPictureProperty+pictureCount;
        while(statusJson[picturePropertyName] != null) {
            // add the image if it isn't already there
            if(this.imageContainer.select('[pictureId='+statusJson[picturePropertyName].pictureId+']').length == 0) {
                LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Display Existing Image: " + picturePropertyName + " => " + statusJson[picturePropertyName] + "  URL: " + statusJson[picturePropertyName].url + "  Orientation: " + statusJson[picturePropertyName].orientation + "  Type: " + statusJson[picturePropertyName].type);
                this.insertImage(statusJson[picturePropertyName]);
            } else {
                LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Update Existing Image: " + picturePropertyName + " => " + statusJson[picturePropertyName] + "  URL: " + statusJson[picturePropertyName].url + "  Orientation: " + statusJson[picturePropertyName].orientation + "  Type: " + statusJson[picturePropertyName].type);
                this.updateImage(statusJson[picturePropertyName]);
            }
            // go to next image
            pictureCount++;
            picturePropertyName = this.jsonPictureProperty+pictureCount;
        }
        this.makeSortable();
    } 
    
    /* 
     * Used for internal calls that have the JSON object.
     */
    this._addImage = function(jsonImageData) {
        this.addImage(jsonImageData.url, jsonImageData.pictureId, jsonImageData.orientation, jsonImageData.type);
    }
    
    /* 
     * Used for external calls from page using this object.
     */
    this.addImage = function(imageURL, pictureId, imageOrientation, imageType) {
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Add Image => PictureId: " + pictureId + "  Orientation: " + imageOrientation + "   URL: " + imageURL + "   Type: " + imageType);
        
            var imageWrapper = new Element('div');
            imageWrapper.id = this.imageContainer.identify()+'_'+pictureId;
            imageWrapper.addClassName('lcUploaderImage');
            imageWrapper.setAttribute('pictureId', pictureId);
            imageWrapper.addClassName(imageOrientation);
            imageWrapper.addClassName(imageType);

            // put another div inside to contain the background of the image
            var imageHolder = new Element('div');
            imageHolder.addClassName('lcUploaderImageHolder');
            imageWrapper.insert(imageHolder);
        
            // show a spinner where the image goes during the load
            var spinner = new Element('img');
            spinner.addClassName('lcUploaderImageWellSpinner');
            spinner.src = '/itc/images/spinner32-transparent.gif';
            imageHolder.insert(spinner);
            
            var image = new Element('img');
            // hide the image by default .. the load events will determine when to show it
            image.hide();
            // setup the rest of the properties
            image.addClassName('lcUploaderImage');
            LCMediaShared.monitorMediaLoad(image, spinner);
            image.src = imageURL;
            image.addClassName(imageOrientation);
            image.addClassName(imageType);
            this.setImageDimensions(image, imageType, imageOrientation);
            imageHolder.insert(image);
            
            // insert the delete div if we have a delete url
            if(this.deleteURL != null) {
                var divOverlay = new Element('div');
                divOverlay.addClassName('lcUploaderImageDelete');
                divOverlay.innerHTML = "Delete";
                divOverlay.setAttribute('pictureId', pictureId);
                divOverlay.observe('click', function(event) {
                    this.deleteImage(event.element().getAttribute('pictureId'));
                }.bind(this));
                imageWrapper.insert(divOverlay);
                
                //if the image is deletable lets wiggle it.
                //LCImageWiggler.startWiggle(image, 0);
            }
            
            this._hideImageWell();
            
            /*  add the image now */
            this.imageContainer.insert(imageWrapper);
            
            this.manageButtonVisibility();
    }
    
    this.setImageDimensions = function(image, imageType, imageOrientation) {
        // if we know the dimensions, this is cleaner than having the browser figuring it out, as browser aren't consistent
        if(imageOrientation == 'portrait') {
            image.height = 120;
            
            /*  per type differences */
            if(imageType == 'iPhoneScreenshot') {
               image.width = 80;
            } else if(imageType == 'iPadScreenshot') {
               image.width = 90;
            }
        } else if(imageOrientation == 'landscape') {
            image.width = 120;
            
            /*  per type differences */
            if(imageType == 'iPhoneScreenshot') {
               image.height = 80;
            } else if(imageType == 'iPadScreenshot') {
               image.height = 90;
            }
        } else {
            // it's a square
            image.width = 120;
            image.height = 120;
            
            /*  per type differences */
            if(imageType == 'LargeApplicationIcon') {
                // none
            } else if(imageType == 'AchievementIcon') {
                // none
            }
        }
    }
    
    // insert a new image
    this.insertImage = function(jsonImageData) {
        this._addImage(jsonImageData);
    }
    
    // update an existing image with a given pictureId
    this.updateImage = function(jsonImageData) {
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, ">>> Update Picture ID: " + jsonImageData.pictureId);
        
        // create the new image
        this._addImage(jsonImageData);
        
        // delete the existing image that we're "updating"
        var imageDiv = this.imageContainer.select('div[pictureId='+jsonImageData.pictureId+']').first();
        var image = imageDiv.select('img').first();
        if(image == null) {
            LCAjaxLogger.trace("ERROR: Could not find image to update. PictureID="+jsonImageData.pictureId);
        } else {
            imageDiv.remove();
        }
    }
    
    /**
    * Delete an item via AJAX and remove from the list on the UI.
    */
    this.deleteImage = function(pictureId) {
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, ">>> Delete Picture ID: " + pictureId);
        
        var requestURL = getAjaxActionURL(this.deleteURL) + "?pictureId=" + pictureId;
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Delete Image Request URL: " + requestURL);
        
        /* send it to the server */
        new Ajax.Request(requestURL, {
              onSuccess: function(response) {
                  
                   // if it worked we have nothing to do
                    LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Successfully deleted item on server.");
                    
                    var elements = this.imageContainer.select('div[pictureId='+pictureId+']');
                    elements.each(function(element) {
                        element.remove();
                    });
                    
                    /* if no images are left, show the image well */
                    if(this.numberOfImages() == 0) {
                        this._showImageWell();
                    }
                    
                    
                    this.manageButtonVisibility();
                    
                    /*  fire event */
                    this.fireEvent("imageDeleted");
                    
                    /* change the form to a value so it recognizes it changed and the lightbox 'change' listener sees that the form has been modified */
                    this.filenameHidden.setValue("deleted");
                    /* trigger that the form changed */
                    triggerEvent(this.uploadForm, 'change');
                    
              }.bind(this),
              onFailure: function(){
                  this.displayAjaxErrorMessage("Failed to delete image. Please try again."); 
              }.bind(this),
              onException: function(request, exception) { 
                 LCAjaxLogger.trace("Exception while deleting image => " + exception);
                 this.displayAjaxErrorMessage("Failed to delete image. Please try again.");
              }.bind(this)
        });
        
    }
    
    /*  if the well is showing, hide it */    
    this._hideImageWell = function() {
        var imageWell = this.imageContainer.select('img.lcUploaderImageWell').first();
        if(imageWell != null) imageWell.hide();
    }
    
    /*  if the well is hiding, show it */    
    this._showImageWell = function() {
        var imageWell = this.imageContainer.select('img.lcUploaderImageWell').first();
        if(imageWell != null) imageWell.show();
    }
    
    this.makeSortable = function() {
         if(this.imageContainer.select('img.lcUploaderImage').length > 1 && this.sortURL != null) {
            LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Making images sortable: " + this.imageContainer.identify());
            Sortable.create(this.imageContainer.identify(), {
                tag: 'div',
                overlap: 'horizontal',
                constraint: 'horizontal',
                onUpdate: this.sortUpdated.bind(this)
            });
        }
    }
    
    this.sortUpdated = function(list) {
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Sort updated... ");
        var nodes = $A(list.childNodes);
        var ids = [];
        
        /* iterate the items in the sorted list and get their ids */        
        nodes.each(function(item) {
            LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Sorted Picture: " + item.getAttribute('pictureId'));
            ids.push(item.getAttribute('pictureId'));
        });
        
        /* we now have an array of the ids in their sorted order to pass to the server */       
        LCAjaxLogger.trace("Sorted Image Array: " + ids);
        
        /* create the request URL ... the URL is dynamically generated by WebObjects, so we get it from a hidden HREF 'ajaxUpdateSortURL' */
        var requestURL = getAjaxActionURL(this.sortURL) + "?sortedIDs=" + ids;
        LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Sort Request URL: " + requestURL);
    
        /* send it to the server */
        new Ajax.Request(requestURL, {
              onSuccess: function(response) {
                   /* trigger that the form changed */
                   this.filenameHidden.setValue("sorted");
                   triggerEvent(this.uploadForm, 'change');
                   LCAjaxLogger.log(LCAjaxLogger.UPLOADER, "Successfully updated sort order on server.");
              }.bind(this),
              onFailure: function(){
                  this.displayAjaxErrorMessage("Failed to update sort order. Please try again.");
              }.bind(this),
              onException: function(request, exception) { 
                 LCAjaxLogger.trace("Exception while updating sort order => " + exception); 
                 this.displayAjaxErrorMessage("Failed to update sort order. Please try again.");   
              }.bind(this)
        });
                
    }
    
    
    // determine if the button should show or not
    this.manageButtonVisibility = function() {
        if(this.maxNumberImages > 1) {
            var pictureCount = this.numberOfImages();
            if(pictureCount >= this.maxNumberImages) {
                this.disableUploadButton();
                // hide the add button
                //this.buttonContainer.hide();
            } else {
                // make sure the add button is showing
                // this.buttonContainer.show();
                this.enableUploadButton();
            }
        }
    }
    
    /* how many images are displaying */
    this.numberOfImages = function() {
        return this.uploadContainerElement.select('div.lcUploaderImage').length;
    }

});



/* 
