/**
 * LCSearchQuickNav
 * 
 * Usage: new LCSearchQuickNav(<textfield>, <contentType>); 
 * 
 * - textfield: The field on the page to observe and display suggestions. 
 * - contentType: The contentType to query such as apps, film, song. See SLSearchContentType.java for the full list.
 * 
 * Example: new LCSearchQuickNav(input, 'book');
 * 
 * On a PageWrapper in the wod define the module as follows and it will get all necessary JS and CSS:
 * 
 */
var lcs_logger;
// logger globally accessible
 (function(window, undefined) {

    var SEARCH_BASE_URL = "/api/v1/search/simple/",
    TEMPLATE_URL = "/global/quick_nav",
    ABORT_TIMEOUT = 4000,
    QUEUE_TIMEOUT = 250,
    // load template for search result rows;
    suggestions_container,
    active_suggestions,
    search_hint,
    selected_suggestion_index = -1,
    active_search_field,
    last_request_update_time,
    last_good_request_time,
    queue_timer,
    SHOW_HINT = true,
    is_browser_ie = BrowserDetect.browser.match(/explorer/i) !== null,
    is_browser_firefox = BrowserDetect.browser.match(/firefox/i) !== null;

    /**
     * Main LCSearchQuickNav class that gets bound to the window as LCSearchQuickNav
     */
    window.LCSearchQuickNav = function(search_field, media_type) {
        var self = this,
        current_keywords;
        /* we want to turn off the browser feature for autocomplete */
        search_field.setAttribute("autocomplete", "off");

        /* observe the search field for 'keyup' and trigger 'queue_suggestions' when search term is typed */
        Element.observe(search_field, 'keyup',
        function(e) {
            // typing into search field
            if (e.keyCode === Event.KEY_DOWN || e.keyCode === Event.Key_UP) return false;
            var query = search_field.value;
            if (current_keywords !== query) {
                current_keywords = query;
                queue_suggestions(query, media_type, search_field);
            };
        });

        // if we've already initialized the suggestions_container before then don't create a new one and stop here
        // this happens if we have multiple textfields on the same page that we want to have suggestions on, we'll observe each textfield but use the same suggestion div
        if (suggestions_container !== undefined) return;

        // observe arrow keys to navigate suggestions
        Element.observe(document.body, is_browser_firefox ? 'keypress': 'keydown', arrow_navigation);

        // observe clicking anywhere on the document and hide suggestions
        Element.observe(document, 'click', hide_suggestions);

        // observe tab (switching search inputs) and esc keys to hide suggestions
        Element.observe(document, 'keyup',
        function(e) {
            if (e.keyCode === Event.KEY_TAB) {
                hide_suggestions();
            } else if (e.keyCode === Event.KEY_ESC) {
                hide_suggestions();
            };
        });

        (function() {
            // create hint
            search_hint = new Element('div');
            search_hint.setAttribute('id', 'search_suggestion_hint');
            document.body.appendChild(search_hint);
            search_hint.style['fontFamily'] = search_field.getStyle('font-family');
            search_hint.style['fontSize'] = search_field.getStyle('font-size');
            search_hint.style['lineHeight'] = search_field.getStyle('line-height');
            // create the suggestions container that's going to be used for every instance of this class
            suggestions_container = new Element('div');
            suggestions_container.setAttribute("id", "lc_search_quick_nav");
            document.body.appendChild(suggestions_container);
            Element.observe(suggestions_container, 'click', suggestion_selected);
            Element.observe(window, 'resize',
            function() {
                resize_results();
            });
        })();
    };

    // handler for arrow navigation of the suggestion list
    var arrow_navigation = function(e) {
        if (e === undefined) return;
        var ul = Element.select(suggestions_container, 'ul').first(),
        lis = Element.select(ul, 'li');
        if (e.keyCode === Event.KEY_DOWN) {
            if (selected_suggestion_index + 1 > lis.length - 1) return;
            if (selected_suggestion_index > -1) {
                lis[selected_suggestion_index].removeClassName('selected');
            };
            selected_suggestion_index++;
            lis[selected_suggestion_index].addClassName('selected');
        } else if (e.keyCode === Event.KEY_UP) {
            if (selected_suggestion_index < 0) return;
            lis[selected_suggestion_index].removeClassName('selected');
            selected_suggestion_index--;
            if (selected_suggestion_index > -1) lis[selected_suggestion_index].addClassName('selected');
        } else if (e.keyCode === Event.KEY_RETURN) {
            if (selected_suggestion_index < 0) return;
            // not selecting suggestion, hitting enter submits
            active_search_field.value = trim_funnies(lis[selected_suggestion_index].innerHTML);
        };
    };

    var hide_suggestions = function() {
        Element.hide(suggestions_container);
        Element.hide(search_hint);
    };
    var show_suggestions = function() {
        Element.show(suggestions_container);
        if (SHOW_HINT && !is_browser_ie) {
            Element.show(search_hint);
        };
    };

    /**
     * When a user types this will be invoked to queue up a request instead of sending it immediately every time.
     * 
     * This is done so that if a user is typing quickly it will only send a request every X milliseconds rather than on every keystroke.
     * 
     * See QUEUE_TIMEOUT for the duration.
     */
    var queue_suggestions = function(query, media_type, search_field) {
        if (active_suggestions !== undefined && active_suggestions[0] !== undefined) {
            var search_hint_txt = active_suggestions[0].match(new RegExp("^" + query, "i")) !== null ? active_suggestions[0].replace(new RegExp("^" + query, "i"), query) : '';
            search_hint.innerHTML = search_hint_txt.replace(new RegExp("^(" + query + ")", "i"), '<span class="search_hint_string">$1</span>');
        };
        if (last_request_update_time === undefined) {
            lcs_logger.log(lcs_logger.SUGGESTION, 'retrieving suggestions for the first time - ' + query);
            get_suggestions(query, media_type, search_field);
        } else {
            if (queue_timer !== undefined) {
                lcs_logger.log(lcs_logger.SUGGESTION, "retrieving suggestions delayed until timer comes around");
            } else {
                var time_diff = (new Date()).getTime() - last_request_update_time;
                if (time_diff > QUEUE_TIMEOUT) {
                    lcs_logger.log(lcs_logger.SUGGESTION, "retrieving suggestions immediately  - " + query);
                    get_suggestions(query, media_type, search_field);
                } else {
                    var time_to_wait = QUEUE_TIMEOUT - time_diff;
                    lcs_logger.log(lcs_logger.SUGGESTION, "scheduling retrieval to occur in " + time_to_wait + "ms");
                    queue_timer = window.setTimeout(function() {
                        lcs_logger.log(lcs_logger.SUGGESTION, "retrieving suggestions after timeout - " + query);
                        get_suggestions(query, media_type, search_field);
                        queue_timer = undefined;
                    },
                    time_to_wait);
                };
            };
        };
    };

    /**
     * AJAX request to server to get suggestions
     * 
     * There is specific logic in here to ensure correct ordering of responses and will ignore out-of-order responses.
     */
    var get_suggestions = function(query, media_type, search_field) {

        if (query == '') {
            // hide suggestions, don't send to server
            hide_suggestions();
            return;
        }

        var timer;
        last_request_update_time = (new Date()).getTime();
        var request = new Ajax.Request(SEARCH_BASE_URL + media_type + ".json", {
            'method': 'GET',
            'parameters': {
                'query': query.toLowerCase()
            },
            onSuccess: function(response) {
                if (response.responseText === undefined) {
                    lcs_logger.error(lcs_logger.SUGGESTION, "received response without responseText");
                    return false;
                };
                var request_time = response.request.requestTime,
                request_duration = (new Date()).getTime() - request_time;
                window.clearTimeout(timer);
                timer = undefined;
                // ensure that the orders are coming in the correct order otherwise ignore it
                if (last_good_request_time === undefined || request_time > last_good_request_time) {
                    lcs_logger.log(lcs_logger.SUGGESTION, "retrieved suggestions [query=" + query + ", duration=" + request_duration + "ms]. response => " + response.responseText);
                    populate_suggestions((response.responseText.evalJSON()), query, search_field);
                    last_good_request_time = request_time;
                } else {
                    // skip this response as it came out of order
                    lcs_logger.error(lcs_logger.SUGGESTION, "Response [query=" + query + ", duration=" + request_duration + "ms] came out of order, will be ignored");
                };
            },
            onCreate: function(new_request) {
                new_request.request.requestTime = (new Date()).getTime();
                timer = window.setTimeout(function() {
                    new_request.transport.abort();
                    lcs_logger.error(lcs_logger.SUGGESTION, "timeout while trying to retrieve search suggestions");
                },
                ABORT_TIMEOUT);
            },
            onComplete: function(response) {
                if (timer !== undefined) {
                    window.clearTimeout(timer);
                    timer = undefined;
                };
            },
            onFailure: function() {
                lcs_logger.error(lcs_logger.SUGGESTION, "failed to retrieve search suggestions");
            },
            onException: function(request, exception) {
                lcs_logger.error(lcs_logger.SUGGESTION, "exception while retrieving search suggestions => " + exception);
            }
        });
    };


    var populate_suggestions = function(suggestions, query, search_field) {
        active_suggestions = suggestions;
        active_search_field = search_field;
        selected_suggestion_index = -1;
        resize_results();
        if (suggestions.results.length > 0) {
            suggestions_container.innerHTML = LCTemplate.getHtml(TEMPLATE_URL, suggestions);
            
            // var search_hint_txt = suggestions[0].match(new RegExp("^" + query, "i")) !== null ? suggestions[0].replace(new RegExp("^" + query, "i"), query) : '';
            // search_hint.innerHTML = search_hint_txt.replace(new RegExp("^(" + query + ")","i"),'<span class="search_hint_string">$1</span>');
            show_suggestions();
        } else {
            // empty the list
            suggestions_container.innerHTML = "";
            hide_suggestions();
        }
    };
    var suggestion_selected = function(e) {
        var elem = Event.element(e);
        if (elem.className !== 'suggestion_element') return;
        active_search_field.value = trim_funnies(elem.innerHTML);
        hide_suggestions();
    };
    var trim_funnies = function(str) {
        return str.replace(/<\S[^><]*>/g, "").replace(/&amp;/g, "&").replace(/(^\s+|\s+$)/g, "");
    };
    var resize_results = function() {
        if (active_search_field === undefined) return;
        suggestions_container.clonePosition(active_search_field, {
            'offsetTop': active_search_field.getHeight(),
            'setWidth': true,
            'setHeight': false,
            'offsetLeft': -1
        });

        // ensure minimum width of 275px
        if (parseInt(suggestions_container.getStyle('width')) < 275) {
            suggestions_container.setStyle({
                'width': '275px'
            });
        }

        search_hint.clonePosition(active_search_field, {
            'offsetTop': is_browser_firefox ? 3: 2,
            'offsetLeft': is_browser_firefox ? 3: 2,
            'setWidth': true,
            'setHeight': false
        });
    };
    lcs_logger = new(Class.create(LCLogger, {
        'SUGGESTION': false
    }))();
})(window);

