    this.deleteItem = function(deleteButton, itemID) {

        var confirmDeleteDiv = listDiv.select('div#ajaxListConfirmDelete').first();
        var deleteCancelButton = confirmDeleteDiv.select('.ajaxListCancelButton').first();
        var deleteConfirmButton = confirmDeleteDiv.select('.ajaxListConfirmButton').first();

        deleteConfirmButton.stopObserving('click');
