function getAjaxActionURL(url) {
    var s = String.interpret(url);
    if(s.startsWith('http://')) {
        s = s.substr(s.indexOf('/', 7), s.length);
    } else if(s.startsWith('https://')) {
        s = s.substr(s.indexOf('/', 8), s.length);
    }
    
    return s;
}

// return the first LCAjaxContainer Error element ID found wrapping the given element 
function getBestFitLCAjaxErrorElementId(element) {
    // first look for an ancestor
    var ancestorErrorBox = $(element).ancestors().find(function(item) {return item.tagName == 'DIV' && item.hasClassName('lcAjaxErrorMessageBox'); });
    if(ancestorErrorBox) return ancestorErrorBox.identify();
    
    // now look for a child
    var childErrorBox = $(element).select('div.lcAjaxErrorMessageBox').first();
    if(childErrorBox) return childErrorBox.identify();
    
    // now look for any on the page
    var documentErrorBox = $(document.body).select('div.lcAjaxErrorMessageBox').first();
    if(documentErrorBox) return documentErrorBox.identify();
    
}

// Display the LCAjaxErrorMessageBox (errorElementID) with a given message.
function displayLCAjaxErrorMessage(errorElementID, message) {
    var messageID = errorElementID + "-message";
    
    if($(messageID) != null) {
        
        // center on page by default
        centerHorizontallyInViewport($(errorElementID));
        
        // see if we're inside a div, if so, center in that
        var ancestorDiv = $(errorElementID).ancestors().find(function(item) {return item.tagName == 'DIV';});
        if(ancestorDiv) centerHorizontallyInContainer($(errorElementID), ancestorDiv);
        
        // show the error
        $(messageID).innerHTML = message;
        Effect.BlindDown($(errorElementID), { duration: 0.5 });
    } else {
        LCAjaxLogger.trace("ERROR: " + message);
    }
}

